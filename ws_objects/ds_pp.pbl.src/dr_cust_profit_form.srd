table(column=(type=char(100) updatewhereclause=yes name=code dbname="v_profit_lost.code" dbalias=".code" )
 column=(type=char(500) updatewhereclause=yes name=name dbname="v_profit_lost.name" dbalias=".name" )
 column=(type=number updatewhereclause=yes name=revenue dbname="revenue" )
 column=(type=number updatewhereclause=yes name=cogs dbname="cogs" )
 column=(type=number updatewhereclause=yes name=profit dbname="profit" )
 column=(type=char(4) updatewhereclause=yes name=grp dbname="v_profit_lost.grp" dbalias=".grp" )
 retrieve="select 
v_profit_lost.code,
v_profit_lost.NAME,
sum(v_profit_lost.revenue) revenue,
sum(v_profit_lost.cogs) cogs,
sum(nvl(v_profit_lost.revenue,0)) - sum(nvl(v_profit_lost.cogs,0)) profit ,v_profit_lost.grp
from v_profit_lost
where v_profit_lost.trans_Date between :ad_fdate and :ad_tdate
and REGEXP_LIKE(v_profit_lost.code, :as_cust_code)
group by v_profit_lost.code,
            v_profit_lost.NAME ,v_profit_lost.grp            
order by v_profit_lost.grp" arguments=(("ad_fdate", date),("ad_tdate", date),("as_cust_code", string))  sort="grp A code A " )
