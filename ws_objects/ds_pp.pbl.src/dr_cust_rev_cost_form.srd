 retrieve="select 
	V_REVE_COST.code,
	V_REVE_COST.NAME,
	sum(V_REVE_COST.revenue) revenue,
	V_REVE_COST.ITEM_CODE,
	V_REVE_COST.ITEM_NAME,
	sum(V_REVE_COST.INPUT_QTY) INPUT_QTY,
	V_REVE_COST.TYPE
from V_REVE_COST 
where V_REVE_COST.trans_Date between :ad_fdate and :ad_tdate
and REGEXP_LIKE(V_REVE_COST.CODE, :as_cust_code) 
group by V_REVE_COST.code,
	V_REVE_COST.NAME,
	V_REVE_COST.ITEM_CODE,
	V_REVE_COST.ITEM_NAME,
	V_REVE_COST.TYPE" arguments=(("ad_fdate", date),("ad_tdate", date),("as_cust_code", string))  sort="code A name A type A item_name A " )
