 retrieve="select '' as gutter,
post_line.ID,
post_line.COMPANY_ID, 
post_line.BRANCH_ID,
post_line.OBJECT_REF_ID,
post_line.OBJECT_REF_TABLE,
post_line.CREATED_DATE,
post_line.CREATED_BY,
post_line.LAST_MDF_BY,
post_line.LAST_MDF_DATE,
post_line.DR_ACCOUNT_ID,
post_line.CR_ACCOUNT_ID, 
post_line.POST_TYPE,
post_line.TRANS_AMT,
post_line.TRANS_CUR_ID, 
post_line.BASE_AMT,
post_line.EX_RATE,
post_line.DR_OBJECT_ID,
post_line.CR_OBJECT_ID,  
post_line.doc_id,
post_line.dr_subaccount,
post_line.cr_subaccount, 
post_line.not_accept_yn,
post_line.trans_text,
post_line.post_type_text,
post_line.trans_DATE,
post_line.sob,
0 as BASE_AMT_settle,
0 as TRANS_AMT_settle,
OBJ1.CODE ,
OBJ1.NAME ,
OBJ1.OBJECT_REF_TABLE ,
OBJ2.CODE , 
OBJ2.NAME ,
OBJ2.OBJECT_REF_TABLE ,
OBJ3.CODE , 
OBJ3.NAME , 
OBJ4.CODE , 
OBJ4.NAME ,
UOM.CODE, 
VA.CODE ,
VA.NAME ,
VA1.CODE ,
VA1.NAME 
FROM POST_LINE
LEFT JOIN OBJECT OBJ1 ON OBJ1.ID=POST_LINE.DR_OBJECT_ID
LEFT JOIN OBJECT OBJ2 ON OBJ2.ID=POST_LINE.CR_OBJECT_ID
LEFT JOIN OBJECT OBJ3 ON OBJ3.ID=POST_LINE.DR_ACCOUNT_ID
        AND OBJ3.OBJECT_REF_TABLE='ACCOUNT'
LEFT JOIN OBJECT OBJ4 ON OBJ4.ID=POST_LINE.CR_ACCOUNT_ID
        AND OBJ4.OBJECT_REF_TABLE='ACCOUNT'
LEFT JOIN UOM ON UOM.ID=POST_LINE.TRANS_CUR_ID
LEFT JOIN VALUESET_VALUE VA ON VA.ID=POST_LINE.DR_SUBACCOUNT
LEFT JOIN VALUESET_VALUE VA1 ON VA1.ID=POST_LINE.CR_SUBACCOUNT" update="POST_LINE" updatewhere=1 updatekeyinplace=yes )
