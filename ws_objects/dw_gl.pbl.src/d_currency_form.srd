 retrieve="select
UOM.ID,
UOM.CODE,
UOM.NAME,
UOM.CLASS_ID,
UOM.BASE_YN,
UOM.company_id,
UOM.branch_id,
UOM.CREATED_BY,
UOM.CREATED_DATE,
UOM.LAST_MDF_BY,
UOM.LAST_MDF_DATE, 
UOM.MULTIPLE,
UOM.PRICE_ROUND_ID,
UOM.AMOUNT_ROUND_ID,
uom.realized_loss_account,
uom.realized_profit_account,
uom.unrealized_loss_account,
uom.unrealized_profit_account,
uom.exrate_diff_trans_id,
uom.exrate_method,
UOM.DIFF_AMOUNT,
TRANS_SETUP_HDR.TRANS_DESC,
RP.CODE as CODE$price_round,
RA.CODE as CODE$amount_round,        
ob1.code,
ob2.code,
ob3.code,
ob4.code 
from UOM 
			left join ROUNDING RP on RP.ID = UOM.PRICE_ROUND_ID
			left join ROUNDING RA on RA.ID = UOM.AMOUNT_ROUND_ID 
			join unit_class on UNIT_CLASS.ID = UOM.CLASS_ID and UNIT_CLASS.CURRENCY_YN = 'Y' 
			left join object ob1 on ob1.id = uom.realized_loss_account
			left join object ob2 on ob2.id = uom.realized_profit_account
			left join object ob3 on ob3.id = uom.unrealized_loss_account
			left join object ob4 on ob4.id = uom.unrealized_profit_account
			left join TRANS_SETUP_HDR on TRANS_SETUP_HDR.ID = uom.exrate_diff_trans_id" update="UOM" updatewhere=1 updatekeyinplace=yes )
