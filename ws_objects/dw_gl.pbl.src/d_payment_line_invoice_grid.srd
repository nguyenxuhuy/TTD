 retrieve="SELECT  '' AS gutter,
PAYMENT_LINE.ID                ,
PAYMENT_LINE.OBJECT_REF_ID     ,
PAYMENT_LINE.OBJECT_REF_TABLE ,
PAYMENT_LINE.COMPANY_ID        ,
PAYMENT_LINE.BRANCH_ID         ,   
PAYMENT_LINE.CREATED_BY        ,
PAYMENT_LINE.CREATED_DATE      ,
PAYMENT_LINE.LAST_MDF_BY       ,
PAYMENT_LINE.LAST_MDF_DATE     , 
PAYMENT_LINE.LINE_NO           ,
PAYMENT_LINE.OBJECT_ID         ,
PAYMENT_LINE.LINE_TEXT         ,
PAYMENT_LINE.AMOUNT            , 
PAYMENT_LINE.NOT_ACCEPT       ,
PAYMENT_LINE.INVOICE_CODE      , 
PAYMENT_LINE.INVOICE_SIGN     ,       
PAYMENT_LINE.INVOICE_NO       , 
PAYMENT_LINE.VENDOR_NAME      ,
PAYMENT_LINE.TAX_CODE        ,
PAYMENT_LINE.TAX_RATE,
PAYMENT_LINE.BASE_AMOUNT,
PAYMENT_LINE.SUBACCOUNT,  
PAYMENT_LINE.INVOICE_DATE,
PAYMENT_LINE.QTY,
PAYMENT_LINE.QTY_SKU,
PAYMENT_LINE.PRICE,
PAYMENT_LINE.TRANS_UOM, 
PAYMENT_LINE.DISC_PCT,
PAYMENT_LINE.DISC_AMT,
PAYMENT_LINE.VAT_AMT,
PAYMENT_LINE.VAT_PCT, 
PAYMENT_LINE.AMOUNT_EX_TAX,     
PAYMENT_LINE.BASE_AMOUNT_EX_TAX,
PAYMENT_LINE.PRICE_EX_TAX,
PAYMENT_LINE.WAREHOUSE_ID,
PAYMENT_LINE.BANK_ID, 
PAYMENT_LINE.SPEC_ID,
PAYMENT_LINE.LOC_ID, 
PAYMENT_LINE.START_ALLOC_DATE, 
PAYMENT_LINE.NBR_OF_ALLOC_PERIOD, 
PAYMENT_LINE.ALLOC_OBJECT, 
PAYMENT_LINE.ALLOC_SUBACCOUNT, 
OBJECT.CODE,
OBJECT.NAME,
OBJECT.OBJECT_REF_TABLE ,
VA.CODE ,
VA.NAME ,
UOM.CODE,
ITEM_SPEC.CODE, 
ITEM_SPEC.NAME,
OBJ.CODE ,
OBJ.NAME ,
BANK.BANK_CODE, 
BANK.BANK_NAME,
BANK.ACCOUNT_NUMBER,
obja.code alloc_code,
obja.name alloc_name
FROM PAYMENT_LINE
LEFT JOIN OBJECT ON OBJECT.ID=PAYMENT_LINE.OBJECT_ID
LEFT JOIN VALUESET_VALUE VA ON VA.ID=PAYMENT_LINE.SUBACCOUNT
LEFT JOIN UOM ON UOM.ID=PAYMENT_LINE.TRANS_UOM
LEFT JOIN ITEM_SPEC ON ITEM_SPEC.ID=PAYMENT_LINE.SPEC_ID
LEFT JOIN OBJECT OBJ ON OBJ.ID=PAYMENT_LINE.WAREHOUSE_ID 
        AND OBJ.OBJECT_REF_TABLE='WAREHOUSE' 
LEFT JOIN BANK ON BANK.ID=PAYMENT_LINE.BANK_ID 
LEFT JOIN OBJECT obja ON PAYMENT_LINE.ALLOC_OBJECT = obja.id 
    AND OBJA.OBJECT_REF_TABLE = 'ACCOUNT'" update="PAYMENT_LINE" updatewhere=1 updatekeyinplace=yes )
