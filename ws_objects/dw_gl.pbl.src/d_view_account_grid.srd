 retrieve="select '' as gutter,
  m.ID,
  m.COMPANY_ID,
  m.BRANCH_ID,
  m.CREATED_DATE,
  m.CREATED_BY,
  m.LAST_MDF_BY,
  m.LAST_MDF_DATE,
  m.DR_ACCOUNT_ID,
  m.CR_ACCOUNT_ID,
  m.TRANS_CURRENCY,
  m.TRANS_DATE,
  m.AMOUNT,
  m.BASE_AMOUNT,
  m.EXCHANGE_RATE,
  m.doc_ref_id,
  m.doc_ref_type,
  m.dr_subaccount, 
  m.cr_subaccount,
  obj3.code, obj3.name,
  obj4.code, obj4.name,
  uom.code,
  v1.code, v1.name,
  v2.code, v2.name
from ledger_trans m
left join object obj3 on m.DR_ACCOUNT_ID = obj3.id
left join object obj4 on m.CR_ACCOUNT_ID = obj4.id
left join uom on m.TRANS_CURRENCY = uom.id
left join valueset_value v1 on m.dr_subaccount = v1.id
left join valueset_value v2 on m.cr_subaccount = v2.id" )
