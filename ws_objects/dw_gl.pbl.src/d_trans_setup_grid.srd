m.dr_posting_group, 
m.cr_posting_group,
m.object_ref_id,
m.object_ref_table,
m.not_accept_col,
m.trans_text,
m.dw_trans_text,
m.column_trans_text,
m.tax_report_group,
m.DR_DW_CRITERIAL,
m.CR_DW_CRITERIAL,
VAL.code tax_report_group_code,
obj1.code, obj1.name,
le1.code,
obj2.code, obj2.name,
le2.code,
obj3.code, obj3.name,
le3.code,
obj4.code, obj4.name,
le4.code,
v1.code, v1.name,
v2.code, v2.name,
taudr.UPROW_YN,
taudr.INCLUDE_YN,
taucr.UPROW_YN,
taucr.INCLUDE_YN 
from trans_setup m
left join object obj1 on m.DR_OBJ_INSTANCE_ID = obj1.id
left join legal_entity le1 on le1.id = obj1.branch_id
left join tax_authority taudr on taudr.object_ref_id = m.DR_OBJ_INSTANCE_ID
left join object obj2 on m.CR_OBJ_INSTANCE_ID = obj2.id
left join legal_entity le2 on le2.id = obj2.branch_id
left join tax_authority taucr on taucr.object_ref_id = m.CR_OBJ_INSTANCE_ID
left join object obj3 on m.DR_ACCT_ID = obj3.id
left join legal_entity le3 on le3.id = obj3.branch_id
left join object obj4 on m.CR_ACCT_ID = obj4.id
left join legal_entity le4 on le4.id = obj4.branch_id
left join valueset_value v1 on m.dr_posting_group = v1.id
left join valueset_value v2 on m.cr_posting_group = v2.id
left join valueset_value v3 on m.dr_subacct_id = v3.id
left join valueset_value v4 on m.cr_subacct_id = v4.id
LEFT JOIN VALUESET_VALUE VAL ON VAL.ID = m.tax_report_group" update="trans_setup" updatewhere=1 updatekeyinplace=yes )
