 column=(type=char(500) updatewhereclause=yes name=object_cust_name dbname="object.cust_name" dbalias="o_cust.cust_name" )
 column=(type=char(100) updatewhereclause=yes name=object_cust_tel dbname="object.cust_tel" dbalias="o_cust.cust_tel" )
