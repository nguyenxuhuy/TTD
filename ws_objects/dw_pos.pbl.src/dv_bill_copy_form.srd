 column=(type=number updatewhereclause=yes name=so_line_qty dbname="so_line.qty" dbalias=".qty" )
 column=(type=number updatewhereclause=yes name=so_line_price dbname="so_line.price" dbalias=".price" )
 column=(type=number updatewhereclause=yes name=so_line_amount dbname="so_line.amount" dbalias=".amount" )
 column=(type=char(250) updatewhereclause=yes name=so_line_note dbname="so_line.note" dbalias=".note" )
