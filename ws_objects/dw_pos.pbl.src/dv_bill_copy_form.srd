 retrieve="select  1 copy,
so_line.line_no,
d.code,
d.document_date,
o_cust.name cust_name,
o_cust.code cust_tel,
o_staff.name sales_name,
so_line.LINE_TEXT,  
VUI.UOM_CODE,
so_line.QTY,  
so_line.PRICE, 
so_line.amount,
so_line.note,
case when b4_meal_yn ='Y' then 'Trước ăn:'
    when after_meal_yn = 'Y' then 'Sau ăn:'
end ||
case when MORNING_QTY > 0 then 'Sáng:' || MORNING_QTY ||' '||vui.to_unit_code || ';' else ''end ||
case when NOON_QTY > 0 then 'Trưa:' || NOON_QTY ||' '||vui.to_unit_code ||';' else '' end ||
case when AFTERNOON_QTY > 0 then 'Chiều:' || AFTERNOON_QTY ||' '||vui.to_unit_code ||';'  else '' end ||
case when EVENING_QTY > 0 then 'Chiều:' || EVENING_QTY ||' '||vui.to_unit_code ||';' else '' end dosage
from so_line 
join booked_slip bs on bs.id = so_line.object_ref_id
join document d on d.version_id= bs.id
join v_uom_item vui on vui.uom_id= so_line.TRANS_UOM and vui.item_id = so_line.item_id 
left join object o_staff on o_staff.id= so_line.sales_person and so_line.line_no = 1
left join object o_cust on o_cust.id= bs.dr_cr_object
left join measurement_line ml on ml.object_ref_id = so_line.id
where d.id = :an_id
