 retrieve="SELECT '' AS gutter,
       PR_LINE.AMOUNT,
       PR_LINE.BRANCH_ID,
       PR_LINE.COMPANY_ID,
       PR_LINE.CREATED_BY,
       PR_LINE.CREATED_DATE,
       PR_LINE.ID,  
       PR_LINE.ITEM_ID,
       PR_LINE.LAST_MDF_BY,
       PR_LINE.LAST_MDF_DATE, 
       PR_LINE.LINE_TEXT, 
       PR_LINE.NOTE,
       PR_LINE.OBJECT_REF_ID, 
       PR_LINE.OBJECT_REF_TABLE,   
       PR_LINE.PRICE,
       PR_LINE.QTY,
       PR_LINE.COUNT_QTY,  
       PR_LINE.SPEC_ID,
       PR_LINE.TRANS_UOM,
       PR_LINE.VENDOR_ID,
       PR_LINE.LINE_NO,
       O_ITEM.CODE,
       UOM.CODE,
       PR_LINE.REQUEST_RECEIVE_DATE,
       PR_LINE.CONFIRM_RECEIVE_DATE
  FROM PR_LINE
       LEFT JOIN OBJECT O_ITEM
          ON O_ITEM.ID = PR_LINE.ITEM_ID 
              AND O_ITEM.OBJECT_REF_TABLE = 'ITEM'
       LEFT JOIN UOM
          ON PR_LINE.TRANS_UOM = UOM.ID" update="PR_LINE" updatewhere=1 updatekeyinplace=yes )
