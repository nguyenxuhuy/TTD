table(column=(type=char(250) updatewhereclause=yes name=line_text dbname="so_line.line_text" dbalias="sl.line_text" )
 column=(type=number updatewhereclause=yes name=qty dbname="qty" )
 column=(type=char(49) updatewhereclause=yes name=uom_code dbname="v_uom_item.uom_code" dbalias="vui.uom_code" )
 column=(type=char(2000) updatewhereclause=yes name=note dbname="booked_slip.note" dbalias="bs.note" )
 column=(type=number updatewhereclause=yes name=dr_cr_object dbname="booked_slip.dr_cr_object" dbalias="bs.dr_cr_object" )
 column=(type=number updatewhereclause=yes name=item_id dbname="so_line.item_id" dbalias="sl.item_id" )
 retrieve="select 
          sl.line_text,
          sum(sl.qty) qty,
          vui.uom_code,
          bs.note,
		 bs.dr_cr_object,
		sl.item_id
from so_line sl
join booked_slip bs on bs.id= sl.object_ref_id
join v_uom_item vui on vui.uom_id = sl.trans_uom and vui.ITEM_ID = sl.item_id
where sl.company_id = :an_comp_Id
and sl.branch_id = :an_branch_id
and bs.doc_type = 'RETAIL' 
and bs.trans_Date between sysdate - 270 and sysdate
group by sl.line_text,vui.uom_code, bs.note, bs.dr_cr_object,sl.item_id" arguments=(("an_comp_Id", number),("an_branch_id", number)) )
