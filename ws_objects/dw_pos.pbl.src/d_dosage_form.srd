 column=(type=number updatewhereclause=yes name=company_id dbname="measurement_line.company_id" dbalias="ml.company_id" )
 column=(type=number updatewhereclause=yes name=branch_id dbname="measurement_line.branch_id" dbalias="ml.branch_id" )
 retrieve="select 'Liều dùng' dosage,
        ml.name ||' (tuổi :'|| ml.f_level_num||'~~'||ml.t_level_num||')' age, 
        case when b4_meal_yn ='Y' then 'Trước ăn'
                when after_meal_yn = 'Y' then 'Sau ăn'
         end usage, 
         case when MORNING_QTY > 0 then 'Sáng: ' || MORNING_QTY ||' '||vui.to_unit_code || '; ' else ''end ||
         case when NOON_QTY > 0 then 'Trưa: ' || NOON_QTY ||' '||vui.to_unit_code ||'; ' else '' end ||
         case when AFTERNOON_QTY > 0 then 'Chiều: ' || AFTERNOON_QTY ||' '||vui.to_unit_code ||'; '  else '' end ||
         case when EVENING_QTY > 0 then 'Chiều: ' || EVENING_QTY ||' '||vui.to_unit_code ||'; ' else '' end ||
        '- (Ngày ' || to_char(nvl(MORNING_QTY/MORNING_QTY,0) + nvl(NOON_QTY/NOON_QTY,0) + nvl(AFTERNOON_QTY/AFTERNOON_QTY,0) + nvl(EVENING_QTY/EVENING_QTY,0)) || ' lần)' qty,
         'Liệu trình ' process,
         ml.working_hours || ' ngày ~~ ' ||
         round(ml.working_hours * (nvl(ml.MORNING_QTY,0) + nvl(ml.NOON_QTY,0) + nvl(ml.AFTERNOON_QTY,0) +nvl(ml.EVENING_QTY,0))/vui.TO_COEFFICIENT,1) ||' '||
         VUI.UOM_CODE  || ' - Thời gian nghỉ: '||  ml.break_minute || ' ngày' process_qty,
         'Ngày: '||trim(to_char(round(ml.working_hours * (nvl(ml.MORNING_QTY,0) + nvl(ml.NOON_QTY,0) + nvl(ml.AFTERNOON_QTY,0) +nvl(ml.EVENING_QTY,0))/vui.TO_COEFFICIENT,1) * ip.retail_price / nvl(ml.working_hours,1) , '99G999L', 'NLS_NUMERIC_CHARACTERS = '',.'' NLS_CURRENCY = ''đ'' ')) ||
         ' Liệu trình: '|| trim(to_char(round(ml.working_hours * (nvl(ml.MORNING_QTY,0) + nvl(ml.NOON_QTY,0) + nvl(ml.AFTERNOON_QTY,0) +nvl(ml.EVENING_QTY,0))/vui.TO_COEFFICIENT,1) * nvl(ip.retail_price,0) , '99G999G999G999L', 'NLS_NUMERIC_CHARACTERS = '',.'' NLS_CURRENCY = ''đ'' ')) amount,
         ml.object_ref_id item_id,
         ml.company_id,
         ml.branch_id         
from measurement_line ml
join item  i on i.object_Ref_id = ml.object_ref_id
left join v_uom_item vui on vui.item_id = i.object_Ref_id and VUI.UOM_ID <> VUI.TO_UNIT_ID
left join item_price ip on ip.item_id = ml.object_ref_id and ip.trans_uom = vui.uom_id and trunc(sysdate) between f_date and coalesce(t_date, trunc(sysdate))"  sort="dosage A age A " )
