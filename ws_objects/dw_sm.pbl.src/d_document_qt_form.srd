 retrieve="SELECT 
DOCUMENT.ID,
DOCUMENT.CODE,
DOCUMENT.HANDLED_BY,
DOCUMENT.STATUS,
DOCUMENT.REQ_APPR_NOTE,
DOCUMENT.APPR_NOTE, 
DOCUMENT.DOCUMENT_DATE, 
DOCUMENT.DOC_TYPE, 
DOCUMENT.VERSION_ID, 
DOCUMENT.TRANS_HDR_ID,
DOCUMENT.MANAGE_GROUP,
DOCUMENT.COMPANY_ID, 
DOCUMENT.BRANCH_ID, 
DOCUMENT.OBJECT_REF_ID,
DOCUMENT.OBJECT_REF_TABLE,
DOCUMENT.CREATED_BY,
DOCUMENT.CREATED_DATE,  
DOCUMENT.LAST_MDF_BY ,
DOCUMENT.LAST_MDF_DATE, 
DOCUMENT.ROSTER_YN,
DOCUMENT.SOB_YN          ,
DOCUMENT.SOB_POPUP         ,
    O_STAFF.CODE,
    O_STAFF.NAME ,
    TRANS_SETUP_HDR.TRANS_DESC,
    O_VEND.short_name,
    O_VEND.NAME,
	V_APPR_PROFILE.APPR_ASSIGNED,
    V_APPR_PROFILE.APPR_DATE,
	V_APPR_PROFILE.APPR_NAME,
    ORDERS.ID, 
    ORDERS.OBJECT_REF_ID,
    V_NEXT_APPR.NEXT_APPR_ASSIGNED,
    V_NEXT_APPR.NEXT_APPR_NAME ,
    LEGAL_ENTITY.CODE,
    LEGAL_ENTITY.NAME,
	VVSTT.name status_name,
	vpa.amount,
PAYMENT_TERM.code payment_term_code,
VALUE1.code PAYMENT_METHOD_code,
VALUE2.code DELIVERY_MODE_code,
ORDERS.PAYMENT_TERM,
ORDERS.PAYMENT_METHOD,
ORDERS.DELIVERY_MODE,
ORDERS.REQUEST_SHIP_DATE, 
ORDERS.EXPIRED_DATE,
ORDERS.object_id,
ORDERS.bill_to_object,
ORDERS.BILL_TO_ADDTEXT,
ORDERS.SHIP_TO_ADDTEXT,
ORDERS.CONTACT_PERSON,
ORDERS.phone,
ORDERS.CURRENCY_ID,
ORDERS.EXCHANGE_RATE,
ORDERS.reason_lose,
ORDERS.include_scrap_yn,
ORDERS.scrap_pct, 
O_DIST.short_name ,
org.tax_number,
u.code,
vv1.code,
vv_ordtype.code
FROM DOCUMENT
JOIN ORDERS  ON DOCUMENT.ID=ORDERS.OBJECT_REF_ID  and DOCUMENT.doc_type = 'QT'
LEFT JOIN ROLE_USER ON DOCUMENT.HANDLED_BY=ROLE_USER.ID 
         AND ROLE_USER.TYPE='user' 
LEFT JOIN OBJECT O_STAFF ON ROLE_USER.OBJECT_STAFF=O_STAFF.ID 
        AND O_STAFF.OBJECT_REF_TABLE='STAFF'
LEFT JOIN VALUESET_VALUE vv1 ON vv1.id=O_STAFF.manage_group
LEFT JOIN TRANS_SETUP_HDR ON DOCUMENT.TRANS_HDR_ID=TRANS_SETUP_HDR.ID
LEFT JOIN OBJECT O_VEND ON ORDERS.BILL_TO_OBJECT=O_VEND.ID
LEFT JOIN OBJECT O_DIST ON ORDERS.OBJECT_ID=O_DIST.ID
LEFT JOIN V_APPR_PROFILE ON DOCUMENT.ID=V_APPR_PROFILE.OBJECT_REF_ID
LEFT JOIN V_NEXT_APPR ON V_NEXT_APPR.OBJECT_REF_ID = DOCUMENT.ID 
LEFT JOIN LEGAL_ENTITY ON LEGAL_ENTITY.ID=DOCUMENT.BRANCH_ID
LEFT JOIN VALUESET_VALUE VVSTT ON VVSTT.CODE=DOCUMENT.STATUS
left join v_qt_amount vpa on vpa.object_ref_id = document.version_id
LEFT JOIN PAYMENT_TERM ON ORDERS.PAYMENT_TERM=PAYMENT_TERM.ID
LEFT JOIN VALUESET_VALUE VALUE1 ON ORDERS.PAYMENT_METHOD=VALUE1.ID
LEFT JOIN VALUESET_VALUE VALUE2 ON ORDERS.DELIVERY_MODE=VALUE2.ID
left join customer c on c.object_ref_id = ORDERS.BILL_TO_OBJECT
left join organization org on org.object_ref_id = c.id
left join uom u on u.id= ORDERS.CURRENCY_ID
left join valueset_value vv_ordtype on vv_ordtype.id = DOCUMENT.manage_group" update="DOCUMENT" updatewhere=1 updatekeyinplace=yes )
