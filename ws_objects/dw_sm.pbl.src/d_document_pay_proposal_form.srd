 retrieve="SELECT
DOCUMENT.ID,
DOCUMENT.CODE,
DOCUMENT.HANDLED_BY,
DOCUMENT.STATUS, 
DOCUMENT.REQ_APPR_NOTE, 
DOCUMENT.APPR_NOTE,  
DOCUMENT.DOCUMENT_DATE, 
DOCUMENT.DOC_TYPE,
DOCUMENT.VERSION_ID,
DOCUMENT.TRANS_HDR_ID,
DOCUMENT.MANAGE_GROUP,
DOCUMENT.COMPANY_ID,
DOCUMENT.BRANCH_ID,
DOCUMENT.OBJECT_REF_ID,   
DOCUMENT.OBJECT_REF_TABLE,
DOCUMENT.CREATED_BY, 
DOCUMENT.CREATED_DATE, 
DOCUMENT.LAST_MDF_BY ,
DOCUMENT.LAST_MDF_DATE,
DOCUMENT.ROSTER_YN,
DOCUMENT.SOB,
DOCUMENT.SOB_YN           ,
DOCUMENT.SOB_POPUP         ,
V_DOC.OBJECT_CODE,
V_DOC.OBJECT_NAME,
V_DOC.TRANS_DESC,
V_DOC.GROUP_CODE,
V_DOC.GROUP_NAME,
V_DOC.SHIP_TO_OBJECT_CODE,
V_DOC.SHIP_TO_OBJECT_NAME,
V_DOC.SO_DATE,
V_DOC.LAST_APPR_NAME,
V_DOC.APPR_DATE,
V_DOC.EXTEND_ID,
V_DOC.EXTEND_OBJECT_REF_ID,
V_DOC.TRANS_DATE,
V_NEXT_APPR.NEXT_APPR_ASSIGNED,
V_NEXT_APPR.NEXT_APPR_NAME AS STAFF_NAME,
V_DOC.ATTACH_FILENAME,
V_DOC.BRANCH_CODE,
V_DOC.BRANCH_NAME
FROM DOCUMENT
JOIN  V_DOCUMENT_SO V_DOC ON DOCUMENT.ID = V_DOC.ID AND V_DOC.OBJECT_REF_TABLE = DOCUMENT.OBJECT_REF_TABLE
LEFT JOIN V_NEXT_APPR ON V_NEXT_APPR.OBJECT_REF_ID = DOCUMENT.ID AND V_NEXT_APPR.NEXT_APPR_ASSIGNED = :AN_USER_ID" update="DOCUMENT" updatewhere=1 updatekeyinplace=yes arguments=(("an_user_id", number)) )
