 retrieve="SELECT DISTINCT DOC1.ID,
            DOCUMENT.CODE,
            DOCUMENT.DOC_TYPE,
            DOCUMENT.STATUS,
            DOCUMENT.DOCUMENT_DATE,
            DOCUMENT.CODE,
            DOC1.DOCUMENT_DATE,
            V_QT_LINE.CUS_CODE,
            V_QT_LINE.CUS_NAME,
            V_QT_LINE.CUS_ADDRESS,
            V_QT_LINE.EXCHANGE_RATE,
            V_QT_LINE.ITEM_CODE,
            V_QT_LINE.ITEM_NAME, 
            V_QT_LINE.LINE_TEXT,
            V_QT_LINE.QTY,
            V_QT_LINE.PRICE_EX_TAX,
            V_QT_LINE.BASE_AMOUNT_EX_TAX,  
            V_QT_LINE.TRANS_UOM, 
            V_QT_LINE.LINE_NO,
            V_QT_LINE.CONFIRM_RECEIVE_DATE,
            V_QT_LINE.UOM_CODE , 
            V_QT_LINE.TEL, 
            V_QT_LINE.FAX, 
            V_QT_LINE.CONTACT_NAME, 
            V_QT_LINE.MANUFACTURERS,
            V_QT_LINE.TAX_CORRECTION,
           V_QT_LINE.BASE_PRICE,
           V_QT_LINE.EXPIRED_DATE,
           V_QT_LINE.COMPANY_NAME,
           V_QT_LINE.COMP_ADDRESS,
           V_QT_LINE.COMP_TEL,
            V_QT_LINE.STAFF_NAME,
            V_QT_LINE.ORDERS_NOTE,
           V_QT_LINE.COMP_FAX,
            V_QT_LINE.ITEM_SHORT_NAME,
		DOCUMENT.BRANCH_ID
FROM    DOCUMENT
    LEFT JOIN V_QT_LINE ON DOCUMENT.ID = V_QT_LINE.DOC_ID
    LEFT JOIN MATCHING MAT ON MAT.F_DOC_ID = DOCUMENT.ID
    LEFT JOIN DOCUMENT DOC1 ON DOC1.ID = MAT.T_DOC_ID
WHERE DOCUMENT.DOC_TYPE = 'QT'" )
