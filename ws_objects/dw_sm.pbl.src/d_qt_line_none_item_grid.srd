 retrieve="  SELECT '' as gutter,
               qt_line.ID                      ,
              qt_line.OBJECT_REF_ID           ,
              qt_line.OBJECT_REF_TABLE        ,
              qt_line.COMPANY_ID              ,
              qt_line.BRANCH_ID               ,
              qt_line.CREATED_BY          ,
              qt_line.CREATED_DATE            ,
              qt_line.LAST_MDF_BY             , 
              qt_line.LAST_MDF_DATE           , 
              qt_line.ITEM_ID              ,
              qt_line.SPEC_ID      ,  
              qt_line.QTY       , 
              qt_line.MARKUP_QTY            ,
              qt_line.PRICE                 ,
              qt_line.MARKUP_PRICE   ,
              qt_line.COMM_AMT     ,
              qt_line.COMM_PCT             ,
              qt_line.AMOUNT                  ,
              qt_line.MARKUP_AMT              ,
              qt_line.NOTE                ,
              qt_line.NOT_INVOICE          ,
              qt_line.FOC_YN                  ,
              qt_line.LINE_TEXT               ,
              qt_line.TRANS_UOM               ,
              qt_line.LINE_NO     ,
              qt_line.ACT_PRICE               ,
              qt_line.MARKUP_PCT              ,
              qt_line.ACT_QTY                 ,
              qt_line.ACT_AMOUNT              ,
              qt_line.SALES_PERSON            ,
              qt_line.MARKUP_RETURN           ,
              qt_line.QTY_SKU                 ,
              qt_line.ACT_QTY_SKU             ,
              qt_line.MARKUP_QTY_SKU          ,
              qt_line.AMOUNT_EX_TAX           , 
              qt_line.BASE_AMOUNT_EX_TAX      ,
              qt_line.ACT_AMOUNT_EX_TAX       ,
              qt_line.ACT_BASE_AMOUNT_EX_TAX  ,
              qt_line.OTHER_NOTE              ,
              qt_line.PRICE_EX_TAX            ,
              qt_line.ACT_PRICE_EX_TAX        ,
              qt_line.DISCOUNT                ,
              qt_line.DISCOUNT_PCT            , 
              qt_line.REQUEST_RECEIVE_DATE    ,
              qt_line.CONFIRM_RECEIVE_DATE    ,
            qt_line.base_price,
            qt_line.base_amount,
            inter.item_code,
            inter.item_name,
              uom.code,
              uom.name,
			inter.manufacturers,
			inter.origin,
			inter.factory_address,
		V_STAFF.staff_code,
          V_STAFF.staff_name,
          V_STAFF.dept_code,
          V_STAFF.dept_name,
          V_STAFF.phone,
          V_STAFF.staff_note
FROM qt_line join item_interface_bk inter on inter.id = qt_line.ITEM_ID
                    left join uom on uom.id = qt_line.TRANS_UOM
				LEFT JOIN V_STAFF
             					ON qt_line.sales_person = V_STAFF.object_ref_id

                
" update="qt_line" updatewhere=1 updatekeyinplace=yes )
