 retrieve="SELECT V_QT_LINE.DOC_ID,
			V_QT_LINE.VERSION_ID,
            V_QT_LINE.DOC_CODE,
            V_QT_LINE.DOC_TYPE,
            V_QT_LINE.STATUS_NAME,
            V_QT_LINE.DOCUMENT_DATE,
            V_QT_LINE.CUS_CODE,V_QT_LINE.cus_name,
            V_QT_LINE.CUS_ADDRESS,
            V_QT_LINE.EXCHANGE_RATE, 
            V_QT_LINE.ITEM_CODE,
            V_QT_LINE.ITEM_NAME,
            V_QT_LINE.LINE_TEXT,
            V_QT_LINE.QTY,
        V_QT_LINE.price, 
        V_QT_LINE.amount, 
        V_QT_LINE.amount_ex_tax,    
            V_QT_LINE.PRICE_EX_TAX, 
            V_QT_LINE.BASE_AMOUNT_EX_TAX, 
            V_QT_LINE.TRANS_UOM,
            V_QT_LINE.LINE_NO,  
            V_QT_LINE.CONFIRM_RECEIVE_DATE, 
            V_QT_LINE.UOM_CODE ,  
            V_QT_LINE.TEL,  
            V_QT_LINE.FAX,
            V_QT_LINE.CONTACT_NAME,
			V_QT_LINE.CONTACT_PHONE,
			V_QT_LINE.CONTACT_email,
            V_QT_LINE.MANUFACTURERS,
            V_QT_LINE.TAX_CORRECTION,
           V_QT_LINE.BASE_PRICE,
           V_QT_LINE.EXPIRED_DATE,
           V_QT_LINE.COMPANY_NAME,
           V_QT_LINE.COMP_ADDRESS,
           V_QT_LINE.COMP_TEL,
           V_QT_LINE.COMP_FAX,
            V_QT_LINE.staff_phone,
            V_QT_LINE.staff_note,
            V_QT_LINE.staff_name,
V_QT_LINE.branch_id,
V_QT_LINE.other_note,
V_QT_LINE.payment_term_name,
V_QT_LINE.DEPT_NAME,
V_QT_LINE.STAFF_EMAIL,
V_QT_LINE.payment_term_nday,
VQF.FOOTER_CODE STK ,VQF.FOOTER_NAME STK1,
VQF1.FOOTER_CODE CTK,VQF1.FOOTER_NAME CTK1,
V_QT_LINE.PAYMENT_METHOD_CODE,
V_QT_LINE.PAYMENT_METHOD_NAME,
V_QT_LINE.DELIVERY_MODE_CODE,
V_QT_LINE.DELIVERY_MODE_NAME,
V_QT_LINE.FOC_YN
FROM    V_QT_LINE
LEFT JOIN V_QT_FOOTER VQF ON VQF.BRANCH_REF_ID = V_QT_LINE.branch_id AND VQF.UP_CODE = 'FTQT01'
    LEFT JOIN V_QT_FOOTER VQF1 ON VQF1.BRANCH_REF_ID = V_QT_LINE.branch_id AND VQF1.UP_CODE = 'FTQT02'
    LEFT JOIN V_QT_FOOTER VQF2 ON VQF2.BRANCH_REF_ID = V_QT_LINE.branch_id AND VQF2.UP_CODE = 'FTQT03'
    LEFT JOIN V_QT_FOOTER VQF3 ON VQF3.BRANCH_REF_ID = V_QT_LINE.branch_id AND VQF3.UP_CODE = 'FTQT04'
WHERE V_QT_LINE.DOC_TYPE = 'QT' "  sort="line_no A " )
