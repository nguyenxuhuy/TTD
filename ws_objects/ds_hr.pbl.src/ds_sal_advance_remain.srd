table(column=(type=number updatewhereclause=yes name=id dbname="payment_line.id" dbalias=".id" )
 column=(type=number updatewhereclause=yes name=object_ref_id dbname="payment_line.object_ref_id" dbalias=".object_ref_id" )
 column=(type=char(50) updatewhereclause=yes name=object_ref_table dbname="payment_line.object_ref_table" dbalias=".object_ref_table" )
 column=(type=number updatewhereclause=yes name=company_id dbname="payment_line.company_id" dbalias=".company_id" )
 column=(type=number updatewhereclause=yes name=branch_id dbname="payment_line.branch_id" dbalias=".branch_id" )
 column=(type=number updatewhereclause=yes name=line_no dbname="payment_line.line_no" dbalias=".line_no" )
 column=(type=number updatewhereclause=yes name=object_id dbname="payment_line.object_id" dbalias=".object_id" )
 column=(type=char(500) updatewhereclause=yes name=line_text dbname="payment_line.line_text" dbalias=".line_text" )
 column=(type=number updatewhereclause=yes name=amount dbname="payment_line.amount" dbalias=".amount" )
 column=(type=number updatewhereclause=yes name=base_amount dbname="payment_line.base_amount" dbalias=".base_amount" )
 column=(type=number updatewhereclause=yes name=subaccount dbname="payment_line.subaccount" dbalias=".subaccount" )
 column=(type=number updatewhereclause=yes name=document_id dbname="document.id" dbalias="doc.id" )
 column=(type=datetime updatewhereclause=yes name=trans_date dbname="booked_slip.trans_date" dbalias="bks.trans_date" )
 retrieve="select 
  payment_line.ID      ,
  payment_line.OBJECT_REF_ID  ,
  payment_line.OBJECT_REF_TABLE  ,
  payment_line.COMPANY_ID ,
  payment_line.BRANCH_ID  ,
  payment_line.LINE_NO   ,
  payment_line.OBJECT_ID  ,
  payment_line.LINE_TEXT   ,
  payment_line.AMOUNT   ,
  payment_line.BASE_AMOUNT    , 
  payment_line.SUBACCOUNT      ,
  doc.id,bks.trans_date
 from payment_line
join document doc on doc.version_id = payment_line.object_ref_id and doc.doc_type ='SALARY_ADVANCE' and doc.status = 'booked'
join booked_slip bks on bks.id = doc.version_id
 where not exists ( select null from salary_line sl 
                                      join document on document.version_id = sl.object_ref_id 
                                                    and document.status = 'booked' 
                                                where instr(sl.ref_line_char, ';'||payment_line.id||';',1,1)>0)
       and bks.trans_date between :ad_f_date and :ad_t_date" arguments=(("ad_f_date", date),("ad_t_date", date)) )
