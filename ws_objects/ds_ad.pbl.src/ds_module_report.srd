 column=(type=decimal(0) updatewhereclause=yes name=menu_sort_number dbname="menu.sort_number" dbalias="m.sort_number" )
 retrieve="  SELECT L.TEXT,   
         M.OBJ_NAME,  
         M.TYPE,   
         M.CODE,
         (case M.UP_MENU_ID when (select m4.id from menu m4 where m4.menu_id =  :an_menu_id and m4.type = 11 )  then null else M.UP_MENU_ID  end ) up_menu_id ,
         M.MENU_ID,
         M.ID,
         M.SORT_NUMBER
    FROM MENU m
        join label l on M.CODE = L.CODE
        left join  identification_key ik on ik.object_ref_code = M.code
        left join  policy_hdr ph  on ik.ID = PH.IDENTIFICATION_ID
        left join business_rule br on BR.ID = PH.OBJECT_REF_ID
        left join  APPLICABLE_RULE_HDR arh on br.OBJECT_REF_ID = arh.ID
 where
         L.LANG = :as_language   
         and M.MENU_ID = :an_menu_id 
         and  M.UP_MENU_ID <> M.MENU_ID
         and ( :an_user_id = 0  
                OR
                (
                 nvl(ph.company_id,:an_company_id) = :an_company_id   
                 and nvl(ph.ACCESS_YN,'N') = 'Y' 
		 		and (
						REGEXP_LIKE(arh.APPLICABLE_REF_ID_STRING,:as_user_role) OR 
			          	(arh.APPLICABLE_REF_ID_STRING is null and arh.APPLICABLE_TYPE = 'all')
					  )
				)
              )
         connect  by prior m.ID =m.UP_MENU_ID
         start with m.ID =  (select m4.id from menu m4 where m4.menu_id =  :an_menu_id and m4.type = 11 )" arguments=(("an_company_id", number),("an_user_id", number),("as_user_role", string),("an_menu_id", number),("as_language", string))  sort="menu_up_menu_id A " )
