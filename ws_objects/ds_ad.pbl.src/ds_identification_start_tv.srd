table(column=(type=number updatewhereclause=yes name=tv_id dbname="identification_key.tv_id" dbalias=".tv_id" )
 column=(type=number updatewhereclause=yes name=tv_up_id dbname="identification_key.tv_up_id" dbalias=".tv_up_id" )
 column=(type=char(500) updatewhereclause=yes name=tv_label dbname="tv_label" )
 column=(type=number updatewhereclause=yes name=tv_pic_idx dbname="tv_pic_idx" )
 column=(type=number updatewhereclause=yes name=tv_sel_pic_idx dbname="tv_sel_pic_idx" )
 column=(type=char(150) updatewhereclause=yes name=tv_object dbname="identification_key.tv_object" dbalias=".tv_object" )
 column=(type=char(500) updatewhereclause=yes name=tv_sort dbname="tv_sort" )
 retrieve="select 
    IDENTIFICATION_KEY.ID as tv_id,
    IDENTIFICATION_KEY.UP_ID as tv_up_id,
COALESCE ( (select LABEL.TEXT from label where LABEL.code = IDENTIFICATION_KEY.OBJECT_REF_code 
                                                       and  LABEL.LANG = :as_lang 
                                                       and LABEL.subcode = substr(identification_key.code,1,instr(IDENTIFICATION_KEY.code,'.',1)-1) ),
     (select LABEL.TEXT from label where   LABEL.code = substr(IDENTIFICATION_KEY.OBJECT_REF_code, instr(IDENTIFICATION_KEY.OBJECT_REF_code,'.',1)+1) 
                                                       and  LABEL.LANG = :as_lang
                                                       and label.subcode = 'MENU'),
    (select LABEL.TEXT from label where LABEL.code = IDENTIFICATION_KEY.OBJECT_REF_code 
                                                       and  LABEL.LANG = :as_lang and rownum =1 ),
	substr(IDENTIFICATION_KEY.OBJECT_REF_code,instr(IDENTIFICATION_KEY.OBJECT_REF_code,'.')+1 ) ) as tv_label,
0 tv_pic_idx,   
1 tv_sel_pic_idx,
IDENTIFICATION_KEY.CODE as tv_object,
COALESCE ( (select LABEL.TEXT from label where LABEL.code = IDENTIFICATION_KEY.OBJECT_REF_code 
                                                       and  LABEL.LANG = :as_lang 
                                                       and LABEL.subcode = substr(identification_key.code,1,instr(IDENTIFICATION_KEY.code,'.',1)-1) ),
     (select LABEL.TEXT from label where   LABEL.code = substr(IDENTIFICATION_KEY.OBJECT_REF_code, instr(IDENTIFICATION_KEY.OBJECT_REF_code,'.',1)+1) 
                                                       and  LABEL.LANG = :as_lang
                                                       and label.subcode = 'MENU'),
     (select LABEL.TEXT from label where   LABEL.code = substr(IDENTIFICATION_KEY.OBJECT_REF_code, instr(IDENTIFICATION_KEY.OBJECT_REF_code,'.',2)+1) 
                                                       and  LABEL.LANG = :as_lang
                                                       and label.subcode = 't_m_actionbutton'),                                                      
    (select LABEL.TEXT from label where LABEL.code = IDENTIFICATION_KEY.OBJECT_REF_code 
                                                       and  LABEL.LANG = :as_lang and rownum =1 )                
                                                       ) as tv_sort
from identification_key 
where level <=3
connect  by prior IDENTIFICATION_KEY.ID = IDENTIFICATION_KEY.UP_ID 
start with nvl(IDENTIFICATION_KEY.UP_ID,0) = :an_up_id" arguments=(("as_lang", string),("an_up_id", number)) )
