table(column=(type=char(400) updatewhereclause=yes name=object_ref_code dbname="label.object_ref_code" dbalias="la.object_ref_code" )
 column=(type=char(402) updatewhereclause=yes name=code dbname="code" )
 column=(type=char(100) updatewhereclause=yes name=subcode dbname="label.subcode" dbalias="la.subcode" )
 retrieve="select 
    LA.CODE as object_ref_code,
    (case when instr(LA.CODE,'d_action',1) = 0 and instr(LA.CODE,'m_',1 ) =0 then  :as_objname ||'.'|| LA.CODE
            else :as_obj4name ||'.'|| LA.CODE  
     end ) as code,
	la.subcode
from label la 
where  LA.LANG = :as_lang
and (((la.code LIKE 'd\_%.%\_t' ESCAPE '\' or la.code like '%.b\_%' escape '\' or la.code like 'dp\_%.%\_t'  ESCAPE '\' or  la.code like 'dr\_%.%\_t'  ESCAPE '\') AND substr(LA.CODE,1,instr(LA.CODE,'.') -1) = :as_dwo )
        OR la.code in (:as_popupname)
        )
and not exists (select null from identification_key ik where ik.code =  :as_obj4name ||'.'|| LA.CODE)
and not exists (select null from identification_key ik where ik.code =  :as_obj4name ||'.'||:as_dwo ||'.'|| LA.CODE)
and (upper(LA.SUBCODE) = upper(:as_objname) )" arguments=(("as_lang", string),("as_objname", string),("as_obj4name", string),("as_dwo", string),("as_popupname", stringlist)) )
