 column=(type=char(1000) updatewhereclause=yes name=ttdhelp dbname="ttdhelp" )
 retrieve="select
    label.CODE,
    label.SUBCODE,
    nvl(LABEL_industry.text,label.TEXT) TEXT,
    nvl(LABEL_industry.ttdhelp,label.TTDHELP) TTDHELP
from label 
LEFT JOIN  LABEL_industry ON label.ID = LABEL_industry.LABEL_ID
where label.lang = :as_lang 
and (:an_indutry_id = 0 or :an_indutry_id is null or LABEL_industry.id = :an_indutry_id)
and LABEL.SUBCODE <> 'MESSAGE'
and LABEL.SUBCODE <> 'c_err_message'" arguments=(("as_lang", string),("an_indutry_id", number)) )
