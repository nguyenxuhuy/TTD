 retrieve="select 
menu.ID as tv_id,
menu.UP_MENU_ID as tv_up_id,
LABEL.TEXT as tv_label,
0 tv_pic_idx,   
1 tv_sel_pic_idx, 
menu.CODE as tv_object,
LABEL.TEXT as tv_sort
from menu 
join label on LABEL.code = menu.code and  LABEL.LANG = :as_lang
where menu.code not like '%@%' and level <= 3 and menu.company_id = :an_company_id
connect  by prior menu.ID = menu.UP_MENU_ID 
start with nvl(menu.UP_MENU_ID,0) = :an_up_id" arguments=(("as_lang", string),("an_up_id", number),("an_company_id", number)) )
