 retrieve="SELECT
bks.ID,
bks.BRANCH_ID, 
bks.COMPANY_ID,
bks.OBJECT_REF_ID, 
bks.OBJECT_REF_TABLE,
bks.CREATED_BY,
bks.CREATED_DATE,
bks.LAST_MDF_BY,
bks.LAST_MDF_DATE,
bks.DOC_TYPE,
bks.DESCRIPTION,
bks.VERSION_NO, 
bks.TRANS_DATE, 
bks.TRANS_ADDRESS,
bks.DELIVERY_PERSON,
bks.RECEIPT_PERSON,       
bks.DELIVERY_MODE,   
bks.INVOICE_OBJECT,  
bks.INVOICE_SIGN, 
bks.INVOICE_CODE, 
bks.BANK_STATEMENT_NO, 
bks.VENDOR_DELIVERY_NO,
bks.INVOICE_YN,
bks.QUANTITY_YN, 
bks.VALUE_YN,
bks.ALLOCATION_YN, 
bks.DR_CR_OBJECT,
bks.INVOICE_ADDRESS,
bks.exchange_rate,
bks.payment_term,
bks.payment_method,
bks.due_date,
bks.invoice_received_date,
bks.include_tax_yn,
bks.currency_id,
bks.REPLACE_YN          ,   
bks.BE_REPLACED_YN      ,
bks.SUBACCOUNT,
bks.tax_desc,
bks.NOTE,
bks.BUYER_PERSON,
bks.INVOICE_REQUEST,
bks.BANK_ID,
O_IV.CODE ,
O_IV.NAME ,
O_IV.OBJECT_REF_TABLE ,
O_DC.CODE ,
O_DC.NAME ,
O_DC.OBJECT_REF_TABLE ,
UOM.CODE,
PM.CODE, 
PM.NAME,
VA.CODE ,  
VA.NAME ,
OR_VEND.TAX_NUMBER , 
OR_CUS.TAX_NUMBER ,
BANK.BANK_CODE,
BANK.BANK_NAME,
BANK.ACCOUNT_NUMBER
FROM BOOKED_SLIP BKS
LEFT JOIN OBJECT O_IV ON O_IV.ID=BKS.INVOICE_OBJECT
LEFT JOIN OBJECT O_DC ON O_DC.ID=BKS.DR_CR_OBJECT
LEFT JOIN UOM ON UOM.ID=BKS.CURRENCY_ID
LEFT JOIN PAYMENT_TERM PM ON PM.ID=BKS.PAYMENT_TERM
LEFT JOIN VALUESET_VALUE VA ON VA.ID=BKS.PAYMENT_METHOD
LEFT JOIN VENDOR ON VENDOR.OBJECT_REF_ID=O_IV.ID
LEFT JOIN ORGANIZATION OR_VEND ON OR_VEND.OBJECT_REF_ID=VENDOR.ID
LEFT JOIN CUSTOMER ON CUSTOMER.OBJECT_REF_ID=O_IV.ID
LEFT JOIN ORGANIZATION OR_CUS ON OR_CUS.OBJECT_REF_ID=CUSTOMER.ID
LEFT JOIN BANK ON BANK.ID=BKS.BANK_ID " update="BOOKED_SLIP" updatewhere=1 updatekeyinplace=yes )
