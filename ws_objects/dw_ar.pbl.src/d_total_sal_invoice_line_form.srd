table(column=(type=number updatewhereclause=yes name=id dbname="document.id" dbalias="doc.id" )
 column=(type=number updatewhereclause=yes name=tax_correction dbname="tax_correction" )
 column=(type=number updatewhereclause=yes name=total_amount dbname="total_amount" )
 column=(type=number updatewhereclause=yes name=amount_b_tax dbname="amount_b_tax" )
 column=(type=number updatewhereclause=yes name=total_disc dbname="total_disc" )
 column=(type=number updatewhereclause=yes name=total_comm dbname="total_comm" )
 column=(type=number updatewhereclause=yes name=total_markup dbname="total_markup" )
 retrieve="select doc.id, 
       sum(tax.TAX_CORRECTION) as TAX_CORRECTION,
        sum(sal.amount_ex_tax)+ sum(tax.TAX_CORRECTION) as total_amount,
         sum(sal.amount_ex_tax)  as amount_b_tax,
        sum(sal.disc_amount) as total_disc,
        sum(sal.comm_amt) as total_comm,
       sum(sal.markup_amt) as total_markup 
from document doc
        left join BOOKED_SLIP bks on bks.id = doc.version_id
        left join sal_invoice_line sal on sal.object_ref_id = bks.id
        left join tax_line tax on sal.id = tax.object_ref_id
where doc.id = :an_doc_id   
group by doc.id  " arguments=(("an_doc_id", number)) )
