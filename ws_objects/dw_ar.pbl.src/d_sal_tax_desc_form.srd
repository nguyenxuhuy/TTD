 retrieve="SELECT
bks.ID,
bks.BRANCH_ID,
bks.COMPANY_ID,
bks.OBJECT_REF_ID, 
bks.OBJECT_REF_TABLE,
bks.CREATED_BY,
bks.CREATED_DATE, 
bks.LAST_MDF_BY,
bks.LAST_MDF_DATE,
bks.DOC_TYPE,
bks.DESCRIPTION,
bks.VERSION_NO, 
bks.TRANS_DATE,
bks.TRANS_ADDRESS,
bks.DELIVERY_PERSON,
bks.RECEIPT_PERSON,       
bks.DELIVERY_MODE,  
bks.INVOICE_OBJECT,  
bks.INVOICE_SIGN,
bks.INVOICE_CODE,  
bks.BANK_STATEMENT_NO, 
bks.VENDOR_DELIVERY_NO,
bks.INVOICE_YN,
bks.QUANTITY_YN, 
bks.VALUE_YN,
bks.ALLOCATION_YN, 
bks.DR_CR_OBJECT,
bks.INVOICE_ADDRESS,
bks.exchange_rate,
bks.payment_term,
bks.payment_method,
bks.due_date,
bks.invoice_received_date,
bks.include_tax_yn,
bks.currency_id,
bks.REPLACE_YN          ,   
bks.BE_REPLACED_YN         ,
bks.SUBACCOUNT,
bks.tax_desc,
bks.NOTE,
bks.BUYER_PERSON,
bks.INVOICE_REQUEST,
bks.BANK_ID,
OBJIV.CODE ,
OBJIV.NAME ,
OBJIV.OBJECT_REF_TABLE ,
OBJDC.CODE ,
OBJDC.NAME ,
OBJDC.OBJECT_REF_TABLE ,
UOM1.CODE,
PM1.CODE,
PM1.NAME, 
VA1.CODE , 
VA1.NAME , 
ORVD.TAX_NUMBER ,
ORCUS.TAX_NUMBER ,
BK1.BANK_CODE,
BK1.BANK_NAME,
BK1.ACCOUNT_NUMBER
FROM BOOKED_SLIP BKS
JOIN OBJECT OBJIV ON OBJIV.ID=BKS.INVOICE_OBJECT
JOIN OBJECT OBJDC ON OBJDC.ID=BKS.DR_CR_OBJECT
LEFT JOIN UOM UOM1 ON UOM1.ID=BKS.CURRENCY_ID
LEFT JOIN PAYMENT_TERM PM1 ON PM1.ID=BKS.PAYMENT_TERM
LEFT JOIN VALUESET_VALUE VA1 ON VA1.ID=BKS.PAYMENT_METHOD
LEFT JOIN VENDOR VDR ON VDR.OBJECT_REF_ID=OBJDC.ID
LEFT JOIN ORGANIZATION ORVD ON ORVD.OBJECT_REF_ID=VDR.ID
LEFT JOIN CUSTOMER CUS1 ON CUS1.OBJECT_REF_ID=OBJDC.ID
LEFT JOIN ORGANIZATION ORCUS ON ORCUS.OBJECT_REF_ID=CUS1.ID
LEFT JOIN BANK BK1 ON BK1.ID=BKS.BANK_ID 
	and BKS.doc_type IN  ('SAL_INVOICE',
                        'SAL_INVOICE_RETURN',
                        'SAL_INVOICE_REPLACE',
                        'RECEIPT',
                        'BANK_VOUCHER',
                        'RECEIPT_RETAIL')" update="BOOKED_SLIP" updatewhere=1 updatekeyinplace=yes )
