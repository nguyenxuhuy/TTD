 retrieve="SELECT
SAL_INVOICE_LINE.ID               ,
SAL_INVOICE_LINE.OBJECT_REF_ID     ,
SAL_INVOICE_LINE.OBJECT_REF_TABLE ,
SAL_INVOICE_LINE.COMPANY_ID        ,
SAL_INVOICE_LINE.BRANCH_ID         ,
SAL_INVOICE_LINE.CREATED_BY        ,
SAL_INVOICE_LINE.CREATED_DATE      ,         
SAL_INVOICE_LINE.LAST_MDF_BY       ,
SAL_INVOICE_LINE.LAST_MDF_DATE     ,
SAL_INVOICE_LINE.LINE_NO           ,   
SAL_INVOICE_LINE.OBJECT_ID         ,
SAL_INVOICE_LINE.LINE_TEXT         ,
SAL_INVOICE_LINE.SPEC_ID           ,
SAL_INVOICE_LINE.TRANS_UOM         , 
SAL_INVOICE_LINE.QTY               ,
SAL_INVOICE_LINE.MARKUP_QTY        , 
SAL_INVOICE_LINE.PRICE             ,
SAL_INVOICE_LINE.MARKUP_PCT      , 
SAL_INVOICE_LINE.COMM_AMT          ,
SAL_INVOICE_LINE.COMM_PCT          ,  
SAL_INVOICE_LINE.AMOUNT            ,  
SAL_INVOICE_LINE.MARKUP_AMT        ,
SAL_INVOICE_LINE.FOC_YN            ,
SAL_INVOICE_LINE.NOT_INVOICE        ,
SAL_INVOICE_LINE.NOTE              , 
SAL_INVOICE_LINE.DISC_AMOUNT       ,
SAL_INVOICE_LINE.DISC_PCT          ,
SAL_INVOICE_LINE.BASE_AMOUNT,
SAL_INVOICE_LINE.SALES_PERSON,
SAL_INVOICE_LINE.SUBACCOUNT,
SAL_INVOICE_LINE.act_qty,
SAL_INVOICE_LINE.act_price,
SAL_INVOICE_LINE.act_amount,
SAL_INVOICE_LINE.act_base_amount,
SAL_INVOICE_LINE.markup_return,
SAL_INVOICE_LINE.cogs,
SAL_INVOICE_LINE.QTY_SKU,
SAL_INVOICE_LINE.ACT_QTY_SKU,
SAL_INVOICE_LINE.MARKUP_QTY_SKU,
SAL_INVOICE_LINE.warehouse_id,
SAL_INVOICE_LINE.loc_id,
SAL_INVOICE_LINE.AMOUNT_EX_TAX,
SAL_INVOICE_LINE.BASE_AMOUNT_EX_TAX,
SAL_INVOICE_LINE.ACT_AMOUNT_EX_TAX      ,
SAL_INVOICE_LINE.ACT_BASE_AMOUNT_EX_TAX  ,
SAL_INVOICE_LINE.PRICE_EX_TAX      ,
SAL_INVOICE_LINE.ACT_PRICE_EX_TAX  ,
SAL_INVOICE_LINE.NOT_COMM_YN  ,
OBJ2.CODE,
OBJ2.NAME,
OBJ2.OBJECT_REF_TABLE ,
ITEM.CODE,
ITEM.NAME,
UOM2.CODE,
OBJ3.CODE ,
OBJ3.NAME ,
OBJ3.OBJECT_REF_TABLE ,
OBJ4.CODE ,
OBJ4.NAME ,
TL.TAX_PCT,
TL.TAX_AMT,
TL.TAX_CORRECTION, 
TL.TRANS_TAX_AMOUNT,
TTH.TAX_TYPE, 
VA2.CODE ,
VA2.NAME
FROM SAL_INVOICE_LINE
LEFT JOIN OBJECT OBJ2 ON OBJ2.ID=SAL_INVOICE_LINE.OBJECT_ID
LEFT JOIN ITEM_SPEC ITEM ON ITEM.ID=SAL_INVOICE_LINE.SPEC_ID
LEFT JOIN UOM UOM2 ON UOM2.ID=SAL_INVOICE_LINE.TRANS_UOM
LEFT JOIN OBJECT OBJ3 ON OBJ3.ID=SAL_INVOICE_LINE.SALES_PERSON
        AND OBJ3.OBJECT_REF_TABLE='STAFF'
LEFT JOIN OBJECT OBJ4 ON OBJ4.ID=SAL_INVOICE_LINE.WAREHOUSE_ID
        AND OBJ4.OBJECT_REF_TABLE='WAREHOUSE'        
LEFT JOIN TAX_LINE TL ON TL.OBJECT_REF_ID=SAL_INVOICE_LINE.ID
LEFT JOIN OBJECT O_TAX ON O_TAX.ID=TL.TAX_ID
LEFT JOIN TAX_AUTHORITY TTH ON TTH.OBJECT_REF_ID=O_TAX.ID
LEFT JOIN VALUESET_VALUE VA2 ON VA2.ID=SAL_INVOICE_LINE.SUBACCOUNT" update="SAL_INVOICE_LINE" updatewhere=1 updatekeyinplace=yes )
