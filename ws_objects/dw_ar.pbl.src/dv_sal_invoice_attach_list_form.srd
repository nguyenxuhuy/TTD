 retrieve="SELECT
		DOCUMENT.ID                ,
		DOCUMENT.CODE              ,
		DOCUMENT.REF_CODE              ,
		DOCUMENT.COMPANY_ID        , 
		DOCUMENT.BRANCH_ID         , 
		DOCUMENT.OBJECT_REF_ID     ,
		DOCUMENT.OBJECT_REF_TABLE  ,
		DOCUMENT.CREATED_BY       ,          
		DOCUMENT.CREATED_DATE     ,  
		DOCUMENT.LAST_MDF_BY      ,
		DOCUMENT.LAST_MDF_DATE    ,             
		DOCUMENT.HANDLED_BY       ,   
		DOCUMENT.STATUS            ,
		DOCUMENT.REQ_APPR_NOTE     ,    
		DOCUMENT.APPR_NOTE        ,  
		DOCUMENT.document_date,
		DOCUMENT.DOC_TYPE          ,
		DOCUMENT.TRANS_HDR_ID      ,      
		DOCUMENT.MANAGE_GROUP,     
		OBJ.NAME,    
		BKS.INVOICE_ADDRESS,  
		BKS.INCLUDE_TAX_YN,
		BKS.BUYER_PERSON, 
		nvl(payter.nday,0) as payter_nday,  
		SAL.LINE_TEXT, 
		ORG.TAX_NUMBER,
		SAL.QTY     , 
		SAL.PRICE,
		SAL.AMOUNT,
		UOM.CODE,
		txl.tax_pct, 
		txl.tax_correction,
		BANK.ACCOUNT_NUMBER,
		Legal_entity.name,
		org_yf.tax_number,
		ba_yf.ADDRESS,
		OBJ_dr_cr.name
  FROM DOCUMENT
  JOIN BOOKED_SLIP BKS ON BKS.ID=DOCUMENT.VERSION_ID AND DOCUMENT.DOC_TYPE = 'SAL_INVOICE'
  LEFT JOIN OBJECT OBJ ON BKS.invoice_object=OBJ.ID
  LEFT JOIN OBJECT OBJ_dr_cr ON BKS.dr_cr_object=OBJ_dr_cr.ID
  LEFT JOIN payment_term payter ON bks.payment_term=payter.id
  LEFT JOIN CUSTOMER CUS ON CUS.OBJECT_REF_ID=OBJ.ID
  LEFT JOIN VENDOR VEN ON VEN.OBJECT_REF_ID=OBJ.ID
  LEFT JOIN ORGANIZATION  ORG ON ORG.OBJECT_REF_ID=CUS.ID
											OR ORG.OBJECT_REF_ID=VEN.ID
  LEFT JOIN SAL_INVOICE_LINE SAL ON BKS.ID=SAL.OBJECT_REF_ID and NVL(SAL.NOT_INVOICE,'N') ='N'
  LEFT JOIN UOM ON SAL.TRANS_UOM=UOM.ID
  LEFT JOIN tax_line txl ON SAL.id=txl.object_ref_id
  LEFT JOIN BANK ON BANK.ID = BKS.BANK_ID
  LEFT JOIN Legal_entity ON Legal_entity.ID = DOCUMENT.branch_id
  LEFT JOIN organization org_yf ON org_yf.object_ref_id = DOCUMENT.branch_id
  LEFT JOIN business_address ba_yf ON ba_yf.object_ref_id = DOCUMENT.branch_id
order by
SAL.LINE_NO

" )
