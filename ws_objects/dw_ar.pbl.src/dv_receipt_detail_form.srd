 column=(type=char(200) updatewhereclause=yes name=branch_address dbname="business_address.address" dbalias="ADDR.address" )
 column=(type=char(30) updatewhereclause=yes name=tel dbname="business_address.tel" dbalias="ADDR.tel" )
 column=(type=char(100) updatewhereclause=yes name=receipt_person dbname="booked_slip.receipt_person" dbalias="BSP.receipt_person" )
 column=(type=char(100) updatewhereclause=yes name=delivery_person dbname="booked_slip.delivery_person" dbalias="BSP.delivery_person" )
 column=(type=char(500) updatewhereclause=yes name=line_text dbname="payment_line.line_text" dbalias=".line_text" )
 column=(type=number updatewhereclause=yes name=base_amount dbname="payment_line.base_amount" dbalias=".base_amount" )
 retrieve="SELECT
DOCMT.ID,
DOCMT.DOCUMENT_DATE,
DOCMT.CODE,  
LEG.CODE,
LEG.NAME,  
ADDR.ADDRESS,
ADDR.TEL,  
BSP.RECEIPT_PERSON, 
BSP.DELIVERY_PERSON,
PAYMENT_LINE.LINE_TEXT,
PAYMENT_LINE.BASE_AMOUNT
FROM DOCUMENT DOCMT
LEFT JOIN LEGAL_ENTITY LEG ON DOCMT.BRANCH_ID=LEG.ID
LEFT JOIN BUSINESS_ADDRESS ADDR ON ADDR.OBJECT_REF_ID=LEG.ID
LEFT JOIN BOOKED_SLIP BSP ON BSP.ID=DOCMT.VERSION_ID
LEFT JOIN PAYMENT_LINE ON PAYMENT_LINE.OBJECT_REF_ID=BSP.ID
WHERE DOCMT.DOC_TYPE IN ('BANK_VOUCHER','RECEIPT')
" )
