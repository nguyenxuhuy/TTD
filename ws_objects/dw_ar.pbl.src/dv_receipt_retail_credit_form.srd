table(column=(type=number updatewhereclause=yes name=id dbname="doc_id" )
 column=(type=char(30) updatewhereclause=yes name=doc_code dbname="doc_code" )
 column=(type=char(100) updatewhereclause=yes name=obj_code dbname="obj_code" )
 column=(type=number updatewhereclause=yes name=line_no dbname="line_no" )
 column=(type=number updatewhereclause=yes name=qty dbname="qty" )
 column=(type=number updatewhereclause=yes name=price dbname="price" )
 column=(type=number updatewhereclause=yes name=amt_aftr_disc dbname="amt_aftr_disc" )
 column=(type=char(500) updatewhereclause=yes name=line_text dbname="line_text" )
 column=(type=char(100) updatewhereclause=yes name=receipt_person dbname="receipt_person" )
 column=(type=number updatewhereclause=yes name=receive_amount dbname="receive_amount" )
 column=(type=char(100) updatewhereclause=yes name=sales_code dbname="sales_code" )
 column=(type=char(500) updatewhereclause=yes name=sales_name dbname="sales_name" )
 column=(type=datetime updatewhereclause=yes name=created_date dbname="created_date" )
 column=(type=char(500) updatewhereclause=yes name=cust_name dbname="cust_name" )
 column=(type=number updatewhereclause=yes name=open_trans_amount dbname="open_trans_amount" )
 column=(type=char(40) updatewhereclause=yes name=line_num dbname="line_num" )
 column=(type=number updatewhereclause=yes name=close_base_amount dbname="close_base_amount" )
 column=(type=char(29) updatewhereclause=yes name=status dbname="status" )
 retrieve="
    select 
    doc_ID,
    doc_code,
    obj_code,
    LINE_NO,
    QTY,
    PRICE, 
    amt_aftr_disc,
    line_text, 
    RECEIPT_PERSON, 
    RECEIVE_AMOUNT,
    sales_code, 
    sales_name,
    created_date,
    cust_name,
    OPEN_TRANS_AMOUNT,
    to_char(rownum) line_num,
    close_base_amount,
	status
    from (
        SELECT
        doc.ID doc_id,
        doc.CODE doc_code,
        OBJECT.CODE obj_code,
        PAYMENT_LINE.LINE_NO,
        PAYMENT_LINE.QTY,
        PAYMENT_LINE.PRICE, 
        PAYMENT_LINE.amt_aftr_disc, 
        PAYMENT_LINE.line_text,
        BOOKED_SLIP.RECEIPT_PERSON,
        BOOKED_SLIP.RECEIVE_AMOUNT,
        OBJ_RO1.CODE sales_code, 
        OBJ_RO1.NAME sales_name,
        doc.created_date,
        obj.name cust_name,
        nvl(ob.OPEN_TRANS_AMOUNT,0) + nvl(ob.DR_TRANS_AMOUNT,0) - nvl(ob.CR_TRANS_AMOUNT,0)  OPEN_TRANS_AMOUNT,
        to_char(rownum) line_num,
        vmb.close_base_amount,
        doc.status		 
        FROM PAYMENT_LINE
        JOIN BOOKED_SLIP ON BOOKED_SLIP.ID = PAYMENT_LINE.OBJECT_REF_ID  and instr(PAYMENT_LINE.line_text, 'KHUYỄN MÃI -')  = 0
        JOIN DOCUMENT doc ON doc.VERSION_ID = BOOKED_SLIP.ID AND doc.DOC_TYPE = 'RECEIPT_RETAIL' and doc.ID = :an_doc_id
        JOIN OBJECT ON OBJECT.ID = PAYMENT_LINE.OBJECT_ID
        left join object_balance ob on ob.object_id = BOOKED_SLIP.invoice_object 
                             and ob.trans_currency = BOOKED_SLIP.CURRENCY_ID
                             and ob.balance_date = BOOKED_SLIP.trans_Date
                             and ob.company_id = BOOKED_SLIP.company_id
                             and ob.branch_id = BOOKED_SLIP.branch_id
                             and ob.sob = doc.sob
        left join OBJECT obj ON obj.ID = BOOKED_SLIP.invoice_object 
        LEFT JOIN ROLE_USER ROL1 ON ROL1.ID = doc.HANDLED_BY AND ROL1.TYPE = 'user'
        LEFT JOIN OBJECT OBJ_RO1 ON OBJ_RO1.ID = ROL1.OBJECT_STAFF AND OBJ_RO1.OBJECT_REF_TABLE = 'STAFF'
        left join V_MEASUREMENT_BAL vmb on vmb.OBJECT_ID = BOOKED_SLIP.INVOICE_OBJECT
                                                                             and vmb.BALANCE_DATE = BOOKED_SLIP.TRANS_DATE 
                                                                             and vmb.company_id = BOOKED_SLIP.COMPANY_ID
                                                                             and vmb.branch_id = BOOKED_SLIP.branch_id
                                                                             and vmb.CODE = 'KHTT'        
        order by PAYMENT_LINE.LINE_NO
    ) t
    union all
    SELECT 
    doc.ID,
    doc.CODE,
    OBJECT.CODE,
    PAYMENT_LINE.LINE_NO,
    PAYMENT_LINE.QTY,
    PAYMENT_LINE.PRICE, 
    PAYMENT_LINE.amt_aftr_disc, 
    PAYMENT_LINE.line_text,
    BOOKED_SLIP.RECEIPT_PERSON,
    BOOKED_SLIP.RECEIVE_AMOUNT,
    OBJ_RO1.CODE, 
    OBJ_RO1.NAME,
    doc.created_date,
    obj.name cust_name,
    nvl(ob.OPEN_TRANS_AMOUNT,0) + nvl(ob.DR_TRANS_AMOUNT,0) - nvl(ob.CR_TRANS_AMOUNT,0)  OPEN_TRANS_AMOUNT,
    '*' line_num,
    vmb.close_base_amount,
    doc.status
    FROM PAYMENT_LINE
    JOIN BOOKED_SLIP ON BOOKED_SLIP.ID = PAYMENT_LINE.OBJECT_REF_ID  and instr(PAYMENT_LINE.line_text, 'KHUYỄN MÃI -')  > 0
    JOIN DOCUMENT doc ON doc.VERSION_ID = BOOKED_SLIP.ID AND doc.DOC_TYPE = 'RECEIPT_RETAIL' and doc.ID = :an_doc_id
    JOIN OBJECT ON OBJECT.ID = PAYMENT_LINE.OBJECT_ID
    left join object_balance ob on ob.object_id = BOOKED_SLIP.invoice_object 
                         and ob.trans_currency = BOOKED_SLIP.CURRENCY_ID
                         and ob.balance_date = BOOKED_SLIP.trans_Date
                         and ob.company_id = BOOKED_SLIP.company_id
                         and ob.branch_id = BOOKED_SLIP.branch_id
                         and ob.sob = doc.sob  
    left join OBJECT obj ON obj.ID = BOOKED_SLIP.invoice_object                               
    LEFT JOIN ROLE_USER ROL1 ON ROL1.ID = doc.HANDLED_BY AND ROL1.TYPE = 'user'
    LEFT JOIN OBJECT OBJ_RO1 ON OBJ_RO1.ID = ROL1.OBJECT_STAFF AND OBJ_RO1.OBJECT_REF_TABLE = 'STAFF'
	left join V_MEASUREMENT_BAL vmb on vmb.OBJECT_ID = BOOKED_SLIP.INVOICE_OBJECT
                                                                             and vmb.BALANCE_DATE = BOOKED_SLIP.TRANS_DATE 
                                                                             and vmb.company_id = BOOKED_SLIP.COMPANY_ID
                                                                             and vmb.branch_id = BOOKED_SLIP.branch_id
                                                                             and vmb.CODE = 'KHTT' " arguments=(("an_doc_id", number))  sort="line_no A " )
