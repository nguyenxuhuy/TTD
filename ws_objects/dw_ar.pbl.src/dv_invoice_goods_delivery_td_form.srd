 retrieve="select leg.name,
         bus.address,
         bus.tel,
         bus.fax,
         obj.code, 
         obj.short_name,
         sal.line_text,
         uom.code,
         sal.qty,
         sal.price_ex_tax,
         sal.amount_ex_tax, 
         sal.line_no,
        sal.disc_pct,
        sal.disc_amount, 
        sal.note, 
        sal.act_amount_ex_tax,
         obj2.code,
         obj2.name, 
         doc.code,
         doc.document_date,
         bks.trans_address,
         doc.id,
         bus1.tel, 
        tax.tax_pct,
        tax.TAX_CORRECTION,
        obj.object_ref_table,
        peo.phone,
        nvl(sal.foc_yn,'N') foc_yn,
        doc.doc_type
from sal_invoice_line sal left join object obj on obj.id = object_id
                                    left join uom on uom.id = sal.trans_uom
                                    left join object obj1 on obj1.id = sal.sales_person
                                    left join booked_slip bks on bks.id = sal.object_ref_id
                                    left join document doc on doc.version_id = bks.id
                                    left join object obj2 on obj2.id = bks.invoice_object
                                    left join legal_entity leg on leg.id = sal.company_id
                                    left join BUSINESS_ADDRESS bus on bus.object_ref_id = leg.id
                                    left join customer cus on cus.object_ref_id = bks.invoice_object
                                    left join BUSINESS_ADDRESS bus1 on bus1.object_ref_id = cus.id
                                    left join people peo on peo.object_ref_id = bus1.id and nvl(peo.main_yn,'N') = 'Y'
                                left join tax_line tax on tax.object_ref_id = sal.id"  sort="line_text A short_name A " )
