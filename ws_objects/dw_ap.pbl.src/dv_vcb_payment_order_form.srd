 column=(type=char(500) updatewhereclause=yes name=description dbname="booked_slip.description" dbalias=".description" )
 column=(type=datetime updatewhereclause=yes name=trans_date dbname="booked_slip.trans_date" dbalias=".trans_date" )
 column=(type=number updatewhereclause=yes name=base_amount dbname="payment_line.base_amount" dbalias=".base_amount" )
