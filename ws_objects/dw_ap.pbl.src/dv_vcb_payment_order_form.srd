 column=(type=char(20) updatewhereclause=yes name=com_account_number dbname="bank.com_account_number" dbalias="COM_BANK.com_account_number" )
 column=(type=char(500) updatewhereclause=yes name=object_bank_name dbname="bank.object_bank_name" dbalias=".object_bank_name" )
 column=(type=char(20) updatewhereclause=yes name=object_account_number dbname="bank.object_account_number" dbalias=".object_account_number" )
 column=(type=char(500) updatewhereclause=yes name=object_object_name dbname="object.object_name" dbalias="OBJ2.object_name" )
 retrieve="SELECT
DOCUMENT.ID,
DOCUMENT.CODE,
BOOKED_SLIP.DESCRIPTION,
BOOKED_SLIP.TRANS_DATE,
PAYMENT_LINE.BASE_AMOUNT,
LEGAL_ENTITY.NAME,
BUSINESS_ADDRESS.NAME,
BUSINESS_ADDRESS.ADDRESS,
OBJ1.NAME COM_BANK_NAME,
COM_BANK.ACCOUNT_NUMBER COM_ACCOUNT_NUMBER ,
BANK.BANK_NAME OBJECT_BANK_NAME,
BANK.ACCOUNT_NUMBER OBJECT_ACCOUNT_NUMBER,
OBJ2.NAME AS OBJECT_NAME
FROM DOCUMENT
JOIN BOOKED_SLIP ON BOOKED_SLIP.ID = DOCUMENT.VERSION_ID
JOIN PAYMENT_LINE ON PAYMENT_LINE.OBJECT_REF_ID = DOCUMENT.VERSION_ID
LEFT JOIN LEGAL_ENTITY ON LEGAL_ENTITY.ID = DOCUMENT.COMPANY_ID
LEFT JOIN BUSINESS_ADDRESS ON BUSINESS_ADDRESS.OBJECT_REF_ID = LEGAL_ENTITY.ID
LEFT JOIN OBJECT OBJ1 ON OBJ1.ID = BOOKED_SLIP.DR_CR_OBJECT
LEFT JOIN BANK COM_BANK ON COM_BANK.OBJECT_REF_ID = OBJ1.ID
LEFT JOIN BANK ON BANK.ID = PAYMENT_LINE.BANK_ID
LEFT JOIN OBJECT OBJ2 ON OBJ2.ID = PAYMENT_LINE.OBJECT_ID
WHERE NVL(BUSINESS_ADDRESS.ACTIVE_YN,'N') = 'Y'" )
