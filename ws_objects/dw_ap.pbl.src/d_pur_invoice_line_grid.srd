 retrieve="SELECT '' as gutter,
pil.id,
pil.branch_id,
pil.company_id,
pil.object_ref_id, 
pil.object_ref_table, 
pil.created_by,
pil.created_date,
pil.last_mdf_by, 
pil.last_mdf_date,
pil.line_no,
pil.object_id,
pil.line_text, 
pil.spec_id,
pil.qty, 
pil.trans_uom, 
pil.price,
pil.amount,
pil.disc_amount,
pil.disc_pct,
pil.base_amount, 
pil.foc_yn, 
pil.note, 
pil.auto_alloc_yn,
pil.start_alloc_date,
pil.alloc_amount,
pil.nbr_of_alloc_period,
pil.alloc_object,
pil.warehouse_id,
pil.BASE_AMOUNT_EX_TAX,
pil.AMOUNT_EX_TAX,
pil.PRICE_EX_TAX,
pil.subaccount,
pil.loc_id,
pil.QTY_SKU,
pil.vat,
pil.vat_amount,
objl.code,
objl.name,
objl.object_ref_table,
itspl.code,
itspl.name,
uml.code, 
obja.code, 
obja.name,
obja.object_ref_table,
whpur.code,
whpur.name,
o_sub.code,
o_sub.name,
o_sub.short_name,
ll.lot_no,
ll.manufacture_date,
ll.expired_date,
d.status, 
v_post.code,
d.trans_hdr_id
FROM PUR_INVOICE_LINE pil 
left join document d on d.version_id = pil.object_ref_id
LEFT JOIN OBJECT objl ON pil.object_id = objl.id
LEFT JOIN item_spec itspl ON pil.spec_id = itspl.id
LEFT JOIN uom uml ON pil.trans_uom = uml.id
LEFT JOIN OBJECT obja ON pil.ALLOC_OBJECT = obja.id 
    AND OBJA.OBJECT_REF_TABLE = 'ACCOUNT'
LEFT JOIN OBJECT whpur ON pil.warehouse_id = whpur.id 
    AND whpur.OBJECT_REF_TABLE = 'WAREHOUSE'
LEFT JOIN object o_sub ON pil.subaccount = o_sub.id
left join lot_line ll on ll.object_ref_id = pil.id
left join valueset_value v_post on v_post.id = objl.posting_group
" update="PUR_INVOICE_LINE" updatewhere=1 updatekeyinplace=yes  sort="line_no A " )
