 column=(type=number updatewhereclause=yes name=item_id dbname="so_line.item_id" dbalias="sl.item_id" )
 column=(type=number updatewhereclause=yes name=cust_id dbname="booked_slip.cust_id" dbalias="bs.cust_id" )
 column=(type=number updatewhereclause=yes name=document_id dbname="document.id" dbalias="d.id" )
 retrieve="select  '' gutter,
		ip.retail_price ,
          d.document_date,
          d.code,
          o_cust.name cust_name,
          o_cust.code cust_code,
          sl.line_text,
          sl.qty,
          vui.uom_code,
          sl.price, 
          sl.amount,
		  bs.note,
		  sl.note line_note,
		  dwh.doc_search, 
		sl.trans_uom,
		sl.item_id, 
		bs.dr_cr_object cust_id,
		d.id
from so_line sl
join booked_slip bs on bs.id= sl.object_ref_id
join document d on d.version_id = bs.id
join v_uom_item vui on vui.uom_id = sl.trans_uom and vui.ITEM_ID = sl.item_id
left join item_price ip on ip.item_id = sl.item_id and ip.trans_uom = sl.trans_uom
left join object o_cust on o_Cust.id= bs.dr_cr_object
left join dwh_search dwh on dwh.doc_id= d.id
where sl.company_id = :an_comp_Id
and sl.branch_id = :an_branch_id
and d.doc_type = 'RETAIL'
and d.code = :as_code" arguments=(("an_comp_Id", number),("an_branch_Id", number),("as_code", string))  sort="document_date D code D cust_name A line_text A " )
