 retrieve="select o.name, o.short_name, o.print_name, o.code,o.id, dwh.obj_search, o.company_id, o.branch_id, o.object_ref_Table, i.SPEC_DESC, ib.BARCODE,
i.ingredient,ip.retail_price, v.uom_code, vv.code, i.manufacturers
from object o
left join V_MAX_UOM_ITEM v on v.item_id=o.id and v.company_id = o.company_id
left join  item_price ip on v.uom_id = ip.trans_uom 
						and v.item_id = ip.item_id						
						and sysdate between ip.f_date and nvl(ip.t_date, sysdate)
						and ip.ITEM_APPLIED_TYPE = 'spec'
						and ip.OBJECT_APPLIED_TYPE = 'all' 
						and ip.OBJECT_TYPE = 'CUSTOMER'
						and ip.PRICE_TYPE = 'SALES'
left join item i on i.object_ref_id = o.id 
left join item_barcode ib on ib.object_ref_id = i.id
join dwh_search dwh on dwh.object_id = o.id
left join valueset_value vv on vv.id= o.manage_group 
where o.object_ref_table ='ITEM'
and o.active_yn = 'Y'"  sort="object_name A " )
