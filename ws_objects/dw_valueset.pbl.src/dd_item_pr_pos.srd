 column=(type=char(1000) updatewhereclause=yes name=ingredient dbname="item.ingredient" dbalias="i.ingredient" )
 column=(type=number updatewhereclause=yes name=retail_price dbname="item_price.retail_price" dbalias="ip.retail_price" )
 column=(type=char(49) updatewhereclause=yes name=uom_code dbname="v_max_uom_item.uom_code" dbalias="v.uom_code" )
 column=(type=char(300) updatewhereclause=yes name=group_code dbname="valueset_value.code" dbalias="vv.code" )
