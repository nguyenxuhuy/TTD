 retrieve="select o.name, o.short_name, o.print_name, o.code,o.id, dwh.obj_search, o.company_id, o.branch_id, o.object_ref_Table, i.SPEC_DESC, ib.BARCODE, ip.retail_price, l.code, vbp.bal_value, i.stock_uom
		, ipp.COMM_CUST_PCT, round(ipp.COMM_CUST_PCT*ip.retail_price/100,0) discount_amt, i.INGREDIENT,ipp.COMM_STAFF_PCT, vv.code
from object o
left join valueset_value vv on vv.id = o.manage_group
left join item i on i.object_ref_id = o.id
left join item_barcode ib on ib.object_ref_id = i.id
left join location l on l.id = o.location
left join v_cust_bal_point vbp on vbp.object_id =  o.id
left join  item_price ip on i.stock_uom = ip.trans_uom
						and i.object_ref_id = ip.item_id						
						and sysdate between ip.f_date and nvl(ip.t_date, sysdate)
						and ip.ITEM_APPLIED_TYPE = 'spec'
						and ip.OBJECT_APPLIED_TYPE = 'all'
						and ip.OBJECT_TYPE = 'CUSTOMER'
						and ip.PRICE_TYPE = 'SALES'
left join (select  t.item_id,t.ITEM_APPLIED_TYPE ,t.OBJECT_TYPE,t.PRICE_TYPE,t.COMM_CUST_PCT, t.COMM_STAFF_PCT 
			from  item_price t where t.ITEM_APPLIED_TYPE = 'spec' 
									and t.OBJECT_TYPE = 'VENDOR' 
									and t.PRICE_TYPE = 'PURCHASE'
									and t.DISCOUNT_PCT > 0 
									and t.DISCOUNT_PCT =  (select max(t1.DISCOUNT_PCT) from item_price t1 
                                                                                                    where t.item_id = t1.item_id 
                                                                                                    and t.OBJECT_TYPE = t1.OBJECT_TYPE
                                                                                                    and t.PRICE_TYPE = t1.PRICE_TYPE
                                                                                                    and t.ITEM_APPLIED_TYPE = t1.ITEM_APPLIED_TYPE)
		) ipp on i.object_ref_id = ipp.item_id				
						and ipp.ITEM_APPLIED_TYPE = 'spec' 
						and ipp.OBJECT_TYPE = 'VENDOR' 
						and ipp.PRICE_TYPE = 'PURCHASE'
join dwh_search dwh on dwh.object_id = o.id 
where o.object_ref_table in ('ITEM','CUSTOMER') 
and o.active_yn = 'Y'
and nvl(ip.stop_yn,'N') = 'N' "  sort="object_name A discount_amt D " )
