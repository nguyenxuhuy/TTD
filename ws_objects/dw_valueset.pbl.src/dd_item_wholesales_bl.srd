ip.wholesales_price, v.uom_code, v.uom_id, i.manufacturers, vv.code, sp.code, sp.id,ip.APPLIED_OBJECT_STAFF,ip.APPLIED_OBJECT_ID
from object o
join item i on i.object_ref_id = o.id and o.active_yn = 'Y'
left join item_spec sp on sp.object_ref_id = o.id and coalesce(sp.default_yn,'N') = 'Y'
left join V_MAX_UOM_ITEM v on v.item_id=o.id and v.company_id = o.company_id
left join  v_wholesales_price_bl ip on v.uom_id = ip.trans_uom 
						and v.item_id = ip.item_id		
						and sp.id = ip.spec_id 
left join dwh_search dwh on dwh.object_id = o.id
left join valueset_value vv on vv.id = o.posting_group"  sort="object_name A " )
