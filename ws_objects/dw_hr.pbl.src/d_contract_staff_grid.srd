 retrieve="select '' gutter,
      LABOR_CONTRACT.ID  ,
      LABOR_CONTRACT.CONTRACT_NO  ,
      LABOR_CONTRACT.VALID_DATE    ,
      LABOR_CONTRACT.SIGN_DATE     ,
      LABOR_CONTRACT.EXPIRE_DATE   ,
      LABOR_CONTRACT.CONTRACT_TYPE   ,
      LABOR_CONTRACT.END_DATE   ,
      LABOR_CONTRACT.END_REASON_ID  , 
      LABOR_CONTRACT.PROBATION_TIME  ,
      LABOR_CONTRACT.PROBATION_S_DATE , 
      LABOR_CONTRACT.PROBATION_E_DATE ,
      LABOR_CONTRACT.PROBATION_SAL   ,
      LABOR_CONTRACT.BASETIME   ,       
      LABOR_CONTRACT.BASE_VALUE   ,       
      LABOR_CONTRACT.CURRENCY_ID   ,
      LABOR_CONTRACT.OBJECT_REF_ID , 
      LABOR_CONTRACT.OBJECT_REF_TABLE, 
      LABOR_CONTRACT.COMPANY_ID  ,
      LABOR_CONTRACT.BRANCH_ID  ,      
      LABOR_CONTRACT.PAYMENT_TYPE ,
      LABOR_CONTRACT.CREATED_DATE  ,
      LABOR_CONTRACT.CREATED_BY    ,
      LABOR_CONTRACT.LAST_MDF_BY   ,
      LABOR_CONTRACT.LAST_MDF_DATE ,
      uom.code ,
      vv.code,
      vv.name
from   LABOR_CONTRACT
left join uom on uom.id = LABOR_CONTRACT.CURRENCY_ID
left join valueset_value vv on vv.id = LABOR_CONTRACT.END_REASON_ID
left join staff s on s.id= LABOR_CONTRACT.object_ref_id
left join role_user ru on ru.object_staff = s.object_ref_id" update="LABOR_CONTRACT" updatewhere=1 updatekeyinplace=yes )
