 retrieve="  SELECT '' as gutter,
        DOCUMENT.ID,   
         DOCUMENT.CODE,   
         DOCUMENT.HANDLED_BY,   
         DOCUMENT.STATUS,   
         DOCUMENT.DOCUMENT_DATE,   
         DOCUMENT.DOC_TYPE,  
         DOCUMENT.TRANS_HDR_ID,   
         DOCUMENT.MANAGE_GROUP,
         DOCUMENT.SOB,   
         DOCUMENT.ROSTER_YN,   
         DOCUMENT.FSOB_ID,  
         DOCUMENT.SOB_YN,           
         DOCUMENT.REQ_APPR_NOTE,     
         DOCUMENT.COUNT_F_DATE,  
         DOCUMENT.COUNT_T_DATE, 
         DOCUMENT.APPR_NOTE,   
         DOCUMENT.version_id,  
         DOCUMENT.CREATED_BY,   
         DOCUMENT.OBJECT_REF_TABLE,    
         DOCUMENT.OBJECT_REF_ID,   
         DOCUMENT.BRANCH_ID,   
         DOCUMENT.COMPANY_ID,         
         DOCUMENT.CREATED_DATE,   
         DOCUMENT.LAST_MDF_BY,   
         DOCUMENT.LAST_MDF_DATE,               
        TRANS_SETUP_HDR.TRANS_DESC,   
         OBJ2.code,
         OBJ2.name,
         attachment.LINE_NO,
        orders.id,
		DEP.CODE,DEP.NAME
    FROM DOCUMENT   
        JOIN orders on orders.object_ref_id = DOCUMENT.ID
        left join TRANS_SETUP_HDR on TRANS_SETUP_HDR.id = DOCUMENT.TRANS_HDR_ID
        LEFT JOIN ROLE_USER
             ON ROLE_USER.ID = DOCUMENT.HANDLED_BY
                AND ROLE_USER.TYPE = 'user'
        LEFT JOIN OBJECT OBJ2
             ON OBJ2.ID = ROLE_USER.OBJECT_STAFF 
                AND OBJ2.OBJECT_REF_TABLE = 'STAFF'
        LEFT JOIN attachment on attachment.dwobject = 'd_document_assign_shift_grid'
                                         and attachment.company_id = DOCUMENT.company_id
                                         and attachment.branch_id = DOCUMENT.branch_id
                                         and attachment.line_no = 1
		LEFT JOIN OBJECT DEP ON DEP.ID = DOCUMENT.MANAGE_GROUP
WHERE DOCUMENT.doc_type = 'ASSIGN_SHIFT'
" update="DOCUMENT" updatewhere=1 updatekeyinplace=yes )
