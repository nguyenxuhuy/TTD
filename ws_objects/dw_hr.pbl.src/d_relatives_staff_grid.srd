		PEOPLE.ID,
		PEOPLE.OBJECT_REF_ID,
		PEOPLE.OBJECT_REF_TABLE,
		PEOPLE.CREATED_BY,
		PEOPLE.CREATED_DATE,
		PEOPLE.LAST_MDF_BY,
		PEOPLE.LAST_MDF_DATE,
		PEOPLE.FIRST_NAME,
		PEOPLE.LAST_NAME ,
		PEOPLE.EMAIL ,
		PEOPLE.DATE_OF_BIRTH,
		PEOPLE.PHONE ,
		PEOPLE.GENDER ,
		PEOPLE.NOTE,
		PEOPLE.RELATIVE,
		PEOPLE.OCCUPATION ,
		PEOPLE.DEDUCTION_YN,
		PEOPLE.RELATIVE_YN
