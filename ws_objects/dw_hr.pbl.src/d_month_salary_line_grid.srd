 retrieve="select  '' gutter,     
  SALARY_LINE.ID ,
  SALARY_LINE.STAFF_ID    ,
  SALARY_LINE.COMPONENT_ID ,
  SALARY_LINE.WORKING_COUNT  ,
  SALARY_LINE.WORKING_HOURS  ,
  SALARY_LINE.SHIFT_COEFFICIENT,
  SALARY_LINE.INCOME_AMT   ,
  SALARY_LINE.AMT_4_CALC   ,
  SALARY_LINE.STAFF_SI_AMT  ,
  SALARY_LINE.COMP_SI_AMT   ,
  SALARY_LINE.STAFF_HI_AMT  , 
  SALARY_LINE.COMP_HI_AMT   , 
  SALARY_LINE.STAFF_UI_AMT  ,
  SALARY_LINE.COMP_UI_AMT   , 
  SALARY_LINE.PIT_AMT      ,  
  SALARY_LINE.PIT_EXEMPT_AMT  ,
  SALARY_LINE.SAL_AMT     , 
  SALARY_LINE.PAYMENT_AMT, 
  SALARY_LINE.MINUS_SAL_ADV, 
  SALARY_LINE.SAL_ADV_REF_DOC_ID, 
  SALARY_LINE.SAL_ADV_REF_LINE_ID,  
  SALARY_LINE.REF_DOC_CHAR, 
  SALARY_LINE.REF_LINE_CHAR,  
  SALARY_LINE.SAL_TYPE,
  SALARY_LINE.COMPANY_ID  ,
  SALARY_LINE.BRANCH_ID   ,
  SALARY_LINE.OBJECT_REF_ID ,
  SALARY_LINE.OBJECT_REF_TABLE ,
  SALARY_LINE.CREATED_BY   ,
  SALARY_LINE.CREATED_DATE   ,
  SALARY_LINE.LAST_MDF_BY   ,
  SALARY_LINE.LAST_MDF_DATE ,
  o_staff.code,
  o_staff.name,
  o_sal.code,
  o_sal.name    
FROM SALARY_LINE
left join object o_staff on o_staff.id = SALARY_LINE.STAFF_ID and o_staff.object_ref_table = 'STAFF'
left join object o_sal on o_sal.id = SALARY_LINE.COMPONENT_ID and o_sal.object_ref_table = 'SALARY'" update="SALARY_LINE" updatewhere=1 updatekeyinplace=yes )
