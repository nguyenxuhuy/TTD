 retrieve="SELECT   
            OBJ.CODE,
            OBJ.NAME,
            CASE TMP.S_DATE_AGE
                WHEN N'>' THEN TMP.S_DATE_AGE||' '||TMP.E_DATE_AGE
			   WHEN N'@' THEN TMP.E_DATE_AGE
                ELSE TMP.S_DATE_AGE||' - '||TMP.E_DATE_AGE
            END AS DATE_AGE,    
            TMP.DR_BASE_AMT,   
            TMP.CR_BASE_AMT,   
		   LEGAL_ENTITY.NAME,  
            BUSINESS_ADDRESS.ADDRESS,
	        TMP.DATE_AGE 
FROM OBJECT_AGING_BALANCE_TMP TMP
LEFT JOIN OBJECT OBJ ON OBJ.ID = TMP.OBJECT_ID
LEFT JOIN LEGAL_ENTITY ON LEGAL_ENTITY.ID = TMP.COMPANY_ID
LEFT JOIN BUSINESS_ADDRESS ON LEGAL_ENTITY.ID = BUSINESS_ADDRESS.OBJECT_REF_ID
WHERE TMP.USER_ID = :AN_USER_ID 
" arguments=(("AN_USER_ID", number))  sort="legal_entity_name A business_address_address A object_code A object_name A " )
