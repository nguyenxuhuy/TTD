 retrieve="    select document.id,
        document.code,
        bs.trans_Date,
        o_bom.code,
        o_bom.name,
        pl.plan_qty,
        pl.input_qty,
        fg_unit.code,
        o_mat.code,
        o_mat.name,
        pd.bom_qty,
        pd.act_qty,
        mat_unit.code,  
        o_cust.code,
        o_cust.name 
    from document 
     join booked_slip bs on bs.id = document.version_id
	left join object o_cust on o_cust.id = bs.dr_cr_object
     left join production_line pl on pl.object_ref_id = bs.id
     left join object o_bom on o_bom.id = pl.bom_id
     left join item on item.object_ref_id = pl.bom_id
     left join uom fg_unit on fg_unit.id = item.stock_uom
     left join production_detail pd on pd.object_ref_id = pl.id and pd.INPUT_OUTPUT = 'I'
     left join object o_mat on o_mat.id = pd. item_id
     left join uom mat_unit on mat_unit.id = pd.uom_id" )
