 retrieve="  SELECT 
         PRODUCTION_LINE.ID,   
         PRODUCTION_LINE.PLAN_S_DATE,   
         PRODUCTION_LINE.PLAN_E_DATE,   
         PRODUCTION_LINE.ACT_S_DATE,   
         PRODUCTION_LINE.ACT_E_DATE,    
         PRODUCTION_LINE.ROUTE_ID,   
         PRODUCTION_LINE.OPER_ID,  
         PRODUCTION_LINE.PRIOR_PROCESS ,   
         PRODUCTION_LINE.NEXT_PROCESS ,   
         PRODUCTION_LINE.BOM_ID,   
         PRODUCTION_LINE.PLAN_QTY,     
         PRODUCTION_LINE.INPUT_QTY,  
         PRODUCTION_LINE.FINISH_QTY,   
         PRODUCTION_LINE.WCENTER_ID,   
         PRODUCTION_LINE.LINE_TYPE,
         PRODUCTION_LINE.price,
         PRODUCTION_LINE.amount,
         PRODUCTION_LINE.base_amount,
         PRODUCTION_LINE.subaccount,
         PRODUCTION_LINE.OBJECT_REF_ID,   
         PRODUCTION_LINE.OBJECT_REF_TABLE,   
         PRODUCTION_LINE.COMPANY_ID,   
         PRODUCTION_LINE.BRANCH_ID,   
         PRODUCTION_LINE.CREATED_BY,   
         PRODUCTION_LINE.CREATED_DATE,   
         PRODUCTION_LINE.LAST_MDF_BY,   
         PRODUCTION_LINE.LAST_MDF_DATE,
         o_bom.code,
         o_bom.name,
         o_route.code,
         o_route.name,
         o_wcenter.code,
         o_wcenter.name
    FROM PRODUCTION_LINE   
left join object o_bom on o_bom.id = PRODUCTION_LINE.BOM_ID and o_bom.object_ref_Table = 'BOM'    
left join object o_route on o_route.id = PRODUCTION_LINE.ROUTE_ID and o_route.object_ref_Table = 'ROUTE'    
left join object o_wcenter on o_wcenter.id = PRODUCTION_LINE.WCENTER_ID and o_wcenter.object_ref_Table = 'WORK_CENTER'  
where PRODUCTION_LINE.line_type = 'BOM'

" update="PRODUCTION_LINE" updatewhere=1 updatekeyinplace=yes )
