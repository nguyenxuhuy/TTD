table(column=(type=char(0) updatewhereclause=yes name=gutter dbname="gutter" values="	Y/	N" )
 column=(type=number updatewhereclause=yes name=sort_seq dbname="sort_seq" )
 column=(type=number updatewhereclause=yes name=item_id dbname="production_detail.item_id" )
 column=(type=char(100) updatewhereclause=yes name=object_code dbname="object.code" )
 column=(type=char(500) updatewhereclause=yes name=object_name dbname="object.name" )
 column=(type=number updatewhereclause=yes name=bom_qty dbname="production_detail.bom_qty" )
 column=(type=number updatewhereclause=yes name=bom_value dbname="production_detail.bom_value" )
 column=(type=number updatewhereclause=yes name=finished_qty dbname="finished_qty" )
 column=(type=number updatewhereclause=yes name=finished_value dbname="finished_value" )
 column=(type=number updatewhereclause=yes name=last_qty dbname="last_qty" )
 column=(type=number updatewhereclause=yes name=last_value dbname="last_value" )
 column=(type=number updatewhereclause=yes name=next_qty dbname="next_qty" )
 column=(type=number updatewhereclause=yes name=next_value dbname="next_value" )
 column=(type=number updatewhereclause=yes name=finished_cash dbname="finished_cash" )
 column=(type=number updatewhereclause=yes name=next_cash dbname="next_cash" )
 retrieve="      SELECT '' as gutter, 
        1 sort_seq,
        pd_bom.item_id,
         o_item.code, 
         o_item.name, 
        pd_bom.BOM_QTY bom_qty, 
        pd_bom.cogs bom_value,
        null finished_qty,
        sum(m_sum.trans_value) finished_value,
        null last_qty, 
        sum(m_last.trans_value) last_value,
         null  next_qty,  
         sum(m_next.trans_value) next_value,     
         sum(m_sum.cash_value) finished_cash,
        sum(m_next.cash_value) next_cash 
    FROM PRODUCTION_DETAIL   pd 
    left join  PRODUCTION_DETAIL   pd_bom on pd_bom.item_id =  :an_bom_id  and pd_bom.object_ref_id = pd.object_ref_id
    left join object o_item on o_item.id = :an_bom_id    
    left join v_proj_match m_sum on m_sum.ITEM_ID = pd.ITEM_ID and  m_sum.bom_ref_id = pd.bom_ref_id and m_sum.f_ref_table = 'PRODUCTION_DETAIL' 
    left join v_proj_match m_last on m_last.PROD_LINE_ID = m_sum.PROD_LINE_ID
                                            and m_last.document_date  between :f1_date and :t1_date
                                            and m_last.PROJ_DETAIL_ID = m_sum.PROJ_DETAIL_ID
    left join v_proj_match m_next on m_next.PROD_LINE_ID =m_sum.PROD_LINE_ID
                                            and m_next.document_date  between :f2_date and :t2_date
                                            and m_next.PROJ_DETAIL_ID = m_sum.PROJ_DETAIL_ID
    where pd_bom.item_id = :an_bom_id
	and pd.object_ref_table = 'PRODUCTION_LINE'
    start with pd.bom_ref_id = :an_bom_id    
    connect by prior pd.item_id = pd.bom_ref_id        
GROUP BY o_item.code, 
         o_item.name, 
        pd_bom.item_id,
        pd_bom.BOM_QTY ,
        pd_bom.cogs                                              
union all         
       SELECT '' as gutter,
         2 sort_seq,
         pd.BOM_ref_ID,
         o_item.code, 
         o_item.name, 
        pd_bom.BOM_QTY bom_qty,
        pd_bom.cogs bom_value,
            null finished_qty,
            sum(m_sum.trans_value) finished_value,
            null last_qty,
            sum(m_last.trans_value) last_value,
            null  next_qty, 
           sum(m_next.trans_value) next_value,
         sum(m_sum.cash_value) finished_cash,
        sum(m_next.cash_value) next_cash      
    FROM PRODUCTION_DETAIL   pd
    left join  PRODUCTION_DETAIL   pd_bom on pd_bom.item_id =  pd.BOM_ref_ID and pd_bom.object_ref_id = pd.object_ref_id    
    left join object o_item on o_item.id = pd_bom.item_id 
    left join v_proj_match m_sum on m_sum.ITEM_ID = pd.ITEM_ID and m_sum.f_ref_table = 'PRODUCTION_DETAIL'
                                                                                                 and m_sum.bom_ref_id = pd.bom_ref_id
    left join v_proj_match m_last on m_last.PROD_LINE_ID = m_sum.PROD_LINE_ID
                                            and m_last.document_date  between :f1_date and :t1_date
                                            and m_last.PROJ_DETAIL_ID = m_sum.PROJ_DETAIL_ID
    left join v_proj_match m_next on m_next.PROD_LINE_ID =m_sum.PROD_LINE_ID
                                            and m_next.document_date  between :f2_date and :t2_date
                                            and m_next.PROJ_DETAIL_ID = m_sum.PROJ_DETAIL_ID
    WHERE pd.BOM_ref_ID <> :an_bom_id
         and pd.object_ref_table = 'PRODUCTION_LINE'
		and level = 2
    start with pd.BOM_ref_ID = :an_bom_id
    connect by prior pd.item_id = pd.BOM_ref_ID    
GROUP BY o_item.code, 
         o_item.name, 
        pd.BOM_ref_ID,
        pd_bom.BOM_QTY ,
        pd_bom.cogs             
 union all       
      SELECT '' as gutter,   
         3 sort_seq,
        pd.item_id,
         o_item.code, 
         o_item.name, 
        pd.BOM_QTY bom_qty,
        pd.cogs bom_value,
            sum(m_sum.qty) finished_qty,
            sum(m_sum.trans_value) finished_value,
         sum(m_last.qty) last_qty,
            sum(m_last.trans_value) last_value,
            sum(m_next.qty) next_qty, 
           sum(m_next.trans_value) next_value ,
         sum(m_sum.cash_value) finished_cash,
        sum(m_next.cash_value) next_cash     
    FROM PRODUCTION_DETAIL   pd 
    left join object o_item on o_item.id = pd.item_id
    left join v_proj_match m_sum on m_sum.ITEM_ID = pd.ITEM_ID and m_sum.f_ref_table = 'PRODUCTION_DETAIL'
                                                                                                 and m_sum.bom_ref_id = pd.bom_ref_id
    left join v_proj_match m_last on m_last.PROD_LINE_ID = m_sum.PROD_LINE_ID
                                            and m_last.document_date  between :f1_date and :t1_date
                                            and m_last.PROJ_DETAIL_ID = m_sum.PROJ_DETAIL_ID
    left join v_proj_match m_next on m_next.PROD_LINE_ID =m_sum.PROD_LINE_ID
                                            and m_next.document_date  between :f2_date and :t2_date
                                            and m_next.PROJ_DETAIL_ID = m_sum.PROJ_DETAIL_ID
    WHERE pd.BOM_ref_ID = :an_bom_id
                and pd.item_type <> 'BOM'
			and pd.object_ref_table = 'PRODUCTION_LINE'
GROUP BY o_item.code, 
         o_item.name, 
        pd.item_id,
        pd.BOM_QTY ,
        pd.cogs         " arguments=(("an_bom_id", number),("f1_date", date),("t1_date", date),("f2_date", date),("t2_date", date)) )
