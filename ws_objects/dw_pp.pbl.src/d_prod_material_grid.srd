 retrieve="  SELECT '' as gutter,
         PRODUCTION_DETAIL.ID,   
         PRODUCTION_DETAIL.ITEM_ID,   
         PRODUCTION_DETAIL.INPUT_OUTPUT,   
         PRODUCTION_DETAIL.MAIN_OUTPUT_YN,  
         PRODUCTION_DETAIL.MAIN_INPUT_YN,  
         PRODUCTION_DETAIL.BOM_ref_ID,  
         PRODUCTION_DETAIL.BOM_ref_table,
         PRODUCTION_DETAIL.oper_ID,
         PRODUCTION_DETAIL.spec_id, 
         PRODUCTION_DETAIL.PICKED_YN,  
         PRODUCTION_DETAIL.WEIGHTED_YN,  
         PRODUCTION_DETAIL.MIXED_YN,  
         PRODUCTION_DETAIL.UOM_ID,    
         PRODUCTION_DETAIL.BOM_QTY,
         PRODUCTION_DETAIL.PLUS_QTY,
         PRODUCTION_DETAIL.MINUS_QTY, 
         PRODUCTION_DETAIL.ACT_QTY,   
         PRODUCTION_DETAIL.ACT_QTY_SKU,
         PRODUCTION_DETAIL.COGS, 
         PRODUCTION_DETAIL.Picked_barcode,
         PRODUCTION_DETAIL.line_no,
         PRODUCTION_DETAIL.WH_ID,  
         PRODUCTION_DETAIL.loc_id,  
         PRODUCTION_DETAIL.subaccount, 
         PRODUCTION_DETAIL.item_name,  
         PRODUCTION_DETAIL.OBJECT_REF_ID,   
         PRODUCTION_DETAIL.OBJECT_REF_TABLE,   
         PRODUCTION_DETAIL.COMPANY_ID,   
         PRODUCTION_DETAIL.BRANCH_ID,   
         PRODUCTION_DETAIL.doc_version,   
         PRODUCTION_DETAIL.CREATED_BY,    
         PRODUCTION_DETAIL.CREATED_DATE,  
         PRODUCTION_DETAIL.LAST_MDF_BY,   
         PRODUCTION_DETAIL.LAST_MDF_DATE,
		PRODUCTION_DETAIL.CHANGE_QTY_YN,
		PRODUCTION_DETAIL.CHANGED_QTY,
		PRODUCTION_DETAIL.DIFFERENCE_QTY,
		PRODUCTION_DETAIL.CHANGED_QTY_SKU,
		o_bom.name,
		o_item.code,
         o_item.short_name,
         vv.code,
         vv.name,
         uom.code ,
         item_spec.code,
         item_spec.name,
         o_wh.code,
         o_wh.name         
    FROM PRODUCTION_DETAIL   
	left join object o_item on o_item.id = PRODUCTION_DETAIL.ITEM_ID
    left join object o_bom on o_bom.id = PRODUCTION_DETAIL.BOM_ref_ID
    left join valueset_value vv on vv.id = PRODUCTION_DETAIL.oper_ID
    left join uom on uom.id = PRODUCTION_DETAIL.UOM_ID
    left join item_spec on item_spec.id = PRODUCTION_DETAIL.spec_id
    left join object o_wh on  o_wh.id = PRODUCTION_DETAIL.WH_ID
    WHERE PRODUCTION_DETAIL.INPUT_OUTPUT = 'I'" update="PRODUCTION_DETAIL" updatewhere=1 updatekeyinplace=yes  sort="object_ref_id A line_no A " )
