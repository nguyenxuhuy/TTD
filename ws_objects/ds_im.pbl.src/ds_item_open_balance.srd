 column=(type=number updatewhereclause=yes name=in_base_value dbname="item_balance.in_base_value" dbalias=".in_base_value" )
 column=(type=number updatewhereclause=yes name=out_base_value dbname="item_balance.out_base_value" dbalias=".out_base_value" )
 column=(type=number updatewhereclause=yes name=close_base_value dbname="item_balance.close_base_value" dbalias=".close_base_value" )
 column=(type=datetime updatewhereclause=yes name=balance_date dbname="item_balance.balance_date" dbalias=".balance_date" )
 retrieve="  SELECT    
         item_balance.open_qty,   
         item_balance.in_qty,   
         item_balance.out_qty,   
         item_balance.close_qty,      
         item_balance.open_base_value,  
         item_balance.in_base_value, 
         item_balance.out_base_value,
         item_balance.close_base_value ,
	item_balance.balance_date
    FROM item_balance    
    join (select max(t.balance_date) balance_date from item_balance t where t.balance_date <:ad_BALANCE_DATE
                                                                                  and t.ITEM_ID = :an_ITEM_ID 
                                                                                  and t.COMPANY_ID = :an_COMPANY_ID
                                                                                  and nvl(t.SPEC_ID,0) = nvl(:an_SPEC_ID,0)
                                                                                  and nvl(t.LOC_ID,0) = nvl(:an_LOC_ID,0)
                                                                                  and t.OBJECT_ID = :an_OBJECT_ID
                                                                                  and nvl(t.LOT_NO,'@') = nvl(:as_LOT_NO,'@')
                                                                                  and nvl(t.value_yn,'N') = nvl(:as_value_yn,'N')
                                                                                  and t.sob = :as_sob
                                                                                  and nvl(t.SERIAL_NO,'@') = nvl(:as_SERIAL_NO,'@')) maxbal on maxbal.balance_date = item_balance.balance_date
where
        item_balance.ITEM_ID = :an_ITEM_ID 
        and item_balance.COMPANY_ID = :an_COMPANY_ID
        and nvl(item_balance.SPEC_ID,0) = nvl(:an_SPEC_ID,0)
        and nvl(item_balance.LOC_ID,0) = nvl(:an_LOC_ID,0)
        and item_balance.OBJECT_ID = :an_OBJECT_ID
        and nvl(item_balance.LOT_NO,'@') = nvl(:as_LOT_NO,'@')
        and nvl(item_balance.SERIAL_NO,'@') = nvl(:as_SERIAL_NO,'@')
        and nvl(item_balance.value_yn,'N') = nvl(:as_value_yn,'N')		
        and item_balance.sob = :as_sob" arguments=(("an_item_id", number),("an_COMPANY_ID", number),("an_SPEC_ID", number),("an_LOC_ID", number),("ad_BALANCE_DATE", datetime),("an_OBJECT_ID", number),("as_LOT_NO", string),("as_SERIAL_NO", string),("as_sob", string),("as_value_yn", string)) )
