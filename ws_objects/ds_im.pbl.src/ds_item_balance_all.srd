 retrieve="select 
  m.ID,
  m.ITEM_ID,  
  m.SPEC_ID,
  m.OBJECT_ID,  
  m.COMPANY_ID,
  m.BRANCH_ID,
  m.LOC_ID,
  m.LOT_NO,
  m.SERIAL_NO,
  m.sob, 
  m.value_yn, 
  m.BALANCE_DATE,
  m.OPEN_QTY,
  m.OPEN_BASE_VALUE,
  m.IN_QTY,
  m.IN_BASE_VALUE,
  m.OUT_QTY,
  m.OUT_BASE_VALUE,
  m.CLOSE_QTY,
  m.CLOSE_BASE_VALUE,
  m.LAST_MDF_BY ,
  m.LAST_MDF_DATE,
  m.PERIOD_AVG_PRICE
 from item_balance m
 join (
        select 
              itb1.ITEM_ID,  
              itb1.SPEC_ID,
              itb1.OBJECT_ID,  
              itb1.COMPANY_ID,
              itb1.branch_ID,
              itb1.LOC_ID,
              itb1.LOT_NO,
              itb1.SERIAL_NO,
              itb1.sob,
              itb1.value_yn
         from (
                    select ib1.ITEM_ID,  
                              ib1.SPEC_ID,
                              ib1.OBJECT_ID,  
                              ib1.COMPANY_ID,
                              ib1.branch_ID,
                              ib1.LOC_ID,
                              ib1.LOT_NO,
                              ib1.SERIAL_NO,
                              ib1.sob,
                              ib1.value_yn
                   from item_balance ib1
                     where ib1.balance_date = :ad_source_date                 
                            AND ib1.sob = :as_sob
                          and ib1.ITEM_ID = :an_ITEM_ID 
                            AND  ib1.COMPANY_ID = :an_COMPANY_ID
                            AND  ib1.branch_ID = :an_branch_ID
                            AND  ib1.OBJECT_ID = :an_OBJECT_ID
                            AND  nvl(ib1.SPEC_ID,0) = NVL(:an_SPEC_ID,0)
                            AND  NVL(ib1.LOC_ID,0) = NVL(:an_LOC_ID,0)
                            AND  nvl(ib1.LOT_NO,'@') =  NVL(:as_LOT_NO,'@')
                            AND  NVL(ib1.SERIAL_NO,'@') = NVL(:as_SERIAL_NO,'@') 
                            AND  NVL(ib1.value_yn,'N') = NVL(:as_value_yn,'N')                                                       
                        ) itb1
        minus       
        select 
              ib2.ITEM_ID,  
              ib2.SPEC_ID,
              ib2.OBJECT_ID,  
              ib2.COMPANY_ID,
              ib2.branch_ID,
              ib2.LOC_ID,
              ib2.LOT_NO,
              ib2.SERIAL_NO,
              ib2.sob,
              ib2.value_yn
         from item_balance ib2
         where ib2.balance_date = :ad_destination_date
            AND ib2.sob = :as_sob
            and ib2.ITEM_ID = :an_ITEM_ID 
            AND  ib2.COMPANY_ID = :an_COMPANY_ID
            AND  ib2.branch_ID = :an_branch_ID
            AND  ib2.OBJECT_ID = :an_OBJECT_ID
            AND  nvl(ib2.SPEC_ID,0) = NVL(:an_SPEC_ID,0)
            AND  NVL(ib2.LOC_ID,0) = NVL(:an_LOC_ID,0)
            AND  nvl(ib2.LOT_NO,'@') =  NVL(:as_LOT_NO,'@')
            AND  NVL(ib2.SERIAL_NO,'@') = NVL(:as_SERIAL_NO,'@') 
            AND  NVL(ib2.value_yn,'N') = NVL(:as_value_yn,'N')                                        
            ) upd_tbl  on upd_tbl.OBJECT_ID = m.OBJECT_ID
                            and upd_tbl.ITEM_ID = m.ITEM_ID
                            and upd_tbl.COMPANY_ID = m.COMPANY_ID
                            and upd_tbl.branch_ID = m.branch_ID
                            and nvl(upd_tbl.SPEC_ID,0) = nvl(m.SPEC_ID,0)
                            and nvl(upd_tbl.LOC_ID,0) = nvl(m.LOC_ID,0)
                            and nvl(upd_tbl.LOT_NO,'@') = nvl(m.LOT_NO,'@')
                            and nvl(upd_tbl.SERIAL_NO,'@') = nvl(m.SERIAL_NO,'@')
                            and nvl(upd_tbl.value_yn,'N') = nvl(m.value_yn,'N')
                            and upd_tbl.sob = m.sob
    where  m.BALANCE_DATE = :ad_source_date" update="item_balance" updatewhere=1 updatekeyinplace=yes arguments=(("ad_source_date", date),("ad_destination_date", date),("as_sob", string),("an_company_id", number),("an_item_id", number),("an_object_id", number),("an_spec_id", number),("an_loc_id", number),("as_lot_no", string),("as_serial_no", string),("as_value_yn", string),("an_branch_id", number))  sort="balance_date A " )
