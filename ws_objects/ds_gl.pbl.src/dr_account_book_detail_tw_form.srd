 retrieve="SELECT
			LEGAL_ENTITY.NAME COM_NAME,       
			BUSINESS_ADDRESS.ADDRESS COM_ADD, 
			TMP.OPEN_BASE_AMOUNT        ,
			TMP.BASE_AMOUNT            ,
			TMP.CLOSE_BASE_AMOUNT       ,  
			TMP.S_DATE                  ,
			TMP.E_DATE                 ,    
			TMP.TRANS_DATE,
			to_char(TMP.TRANS_DATE,'mm'),
			OBJ.CODE,
			OBJ.name, 
			OBJ2.CODE,
			OBJ3.CODE,
			OBJ3.NAME,
			DOC.CODE,
			DOC.DOCUMENT_DATE,
			TMP.TRANS_TEXT, 
			TMP.COL_TMP
FROM OBJECT_BALANCE_TMP TMP
		LEFT JOIN LEGAL_ENTITY ON TMP.COMPANY_ID = LEGAL_ENTITY.ID
		LEFT JOIN BUSINESS_ADDRESS ON LEGAL_ENTITY.ID = BUSINESS_ADDRESS.OBJECT_REF_ID
		LEFT JOIN OBJECT OBJ ON OBJ.ID = TMP.OBJECT_ID
		LEFT JOIN OBJECT OBJ2 ON OBJ2.ID = TMP.CORRESPONDENCE_ACCOUNT
		LEFT JOIN DOCUMENT DOC ON DOC.ID  = TMP.DOC_REF_ID
		LEFT JOIN BOOKED_SLIP BKS ON BKS.ID = DOC.VERSION_ID
        LEFT JOIN OBJECT OBJ3 ON BKS.DR_CR_OBJECT = OBJ3.ID
WHERE TMP.USER_ID = :AN_USER_ID   
" arguments=(("AN_USER_ID", number))  sort="object_code A compute_0009 A trans_date A col_tmp A " )
