 retrieve="SELECT
			LEGAL_ENTITY.NAME,
			BUSINESS_ADDRESS.ADDRESS,
			TMP.DR_BASE_AMT   , 
			TMP.CR_BASE_AMT   , 
			TMP.S_DATE            ,    
			TMP.E_DATE            ,  
			TMP.CODE,  
			substr(TMP.NAME  ,1,( case when instr(TMP.NAME, ':')= 0 then 1 else instr(TMP.NAME, ':')  end)-1) ACCOUNT_NAME ,
    			 substr(TMP.NAME,instr(TMP.NAME, ':')+1) ACCOUNT_NAME_TW 
FROM TRIAL_BALANCE_TMP TMP 
		LEFT JOIN LEGAL_ENTITY ON LEGAL_ENTITY.ID = TMP.COMPANY_ID
		LEFT JOIN BUSINESS_ADDRESS ON LEGAL_ENTITY.ID = BUSINESS_ADDRESS.OBJECT_REF_ID
WHERE TMP.USER_ID = :AN_USER_ID" arguments=(("AN_USER_ID", number))  sort="code A " )
