table(column=(type=char(100) updatewhereclause=yes name=company_name dbname="legal_entity.company_name" dbalias=".company_name" )
 column=(type=char(200) updatewhereclause=yes name=company_address dbname="business_address.company_address" dbalias=".company_address" )
 column=(type=char(30) updatewhereclause=yes name=object_code dbname="balance_sheet_tmp.object_code" dbalias="tmp.object_code" )
 column=(type=number updatewhereclause=yes name=account_level dbname="balance_sheet_tmp.account_level" dbalias="tmp.account_level" )
 column=(type=char(164) updatewhereclause=yes name=object_name dbname="object_name" )
 column=(type=char(10) updatewhereclause=yes name=ref_code dbname="balance_sheet_tmp.ref_code" dbalias="tmp.ref_code" )
 column=(type=char(30) updatewhereclause=yes name=description dbname="balance_sheet_tmp.description" dbalias="tmp.description" )
 column=(type=number updatewhereclause=yes name=prior_period_amt dbname="prior_period_amt" )
 column=(type=number updatewhereclause=yes name=this_period_amt dbname="this_period_amt" )
 column=(type=datetime updatewhereclause=yes name=prior_period_date dbname="balance_sheet_tmp.prior_period_date" dbalias="tmp.prior_period_date" )
 column=(type=datetime updatewhereclause=yes name=this_period_date dbname="balance_sheet_tmp.this_period_date" dbalias="tmp.this_period_date" )
 column=(type=number updatewhereclause=yes name=branch_id dbname="balance_sheet_tmp.branch_id" dbalias="tmp.branch_id" )
 retrieve="SELECT 
					legal_entity.name AS company_name,
					business_address.address AS company_address,
					tmp.object_code AS object_code,
					tmp.account_level,
					CASE tmp.account_level
					WHEN 1 THEN tmp.object_name 
					WHEN 2 THEN tmp.object_name
					WHEN 3 THEN '   '|| tmp.object_name
					WHEN 4 THEN '    '|| tmp.object_name
					ELSE '              '|| tmp.object_name 
					END AS object_name, 
					tmp.ref_code AS ref_code,
					tmp.ref_description AS description,
					sum(tmp.prior_period_amt) AS prior_period_amt,
					sum(tmp.this_period_amt) AS this_period_amt,  
					tmp.prior_period_date AS prior_period_date,
					tmp.this_period_date AS this_period_date,
					tmp.branch_id
FROM balance_sheet_tmp tmp 
        LEFT JOIN legal_entity ON legal_entity.id = TMP.branch_id
        LEFT JOIN business_address ON legal_entity.id = business_address.object_ref_id
WHERE tmp.user_id = :an_user_id 
group by legal_entity.name ,
					business_address.address ,
					tmp.object_code ,
					tmp.account_level,
					tmp.object_name ,
					tmp.ref_code,
					tmp.ref_description ,
					tmp.prior_period_date ,
					tmp.this_period_date ,
					tmp.branch_id" arguments=(("an_user_id", number))  sort="ref_code A " )
