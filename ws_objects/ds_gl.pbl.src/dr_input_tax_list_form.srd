            COM_NAME,
            TAX_NUMBER,
            INVOICE_CODE,
            INVOICE_SIGN,
            TRANS_DATE,
            CODE,
            PUR_NAME,
            PUR_TAX,
            TAX_DESC,
            BASE_AMOUNT_EX_TAX,
            TAX_PCT,
            TAX_AMT,
            LINE_NO,
            NAME,
            NOTE,
			SOB,
			BRANCH_ID
FROM V_INPUT_TAX
WHERE   NVL(TRANS_DATE,:AD_S_DATE) BETWEEN :AD_S_DATE AND :AD_E_DATE
