 retrieve="  SELECT 
            sum(vost.BASE_AMOUNT) BASE_AMOUNT, 
            sum(vost.tax_amt) tax_amt,
            vost.tax_group_code,
            vost.tax_group_name,
            vost.BRANCH_CODE,
            vost.BRANCH_ID,
            vost.SOB
from V_INPUT_SALES_TAX vost 
where vost.TRANS_DATE between :ad_s_date and :ad_e_date and vost.BRANCH_ID = :an_branch_id
		and vost.SOB = :as_sob
group by vost.EXCHANGE_RATE,vost.tax_group_code,
              vost.tax_group_name, vost.BRANCH_CODE,
              vost.BRANCH_ID,vost.SOB" arguments=(("ad_s_date", date),("ad_e_date", date),("an_branch_id", number),("as_sob", string)) )
