 retrieve="SELECT
v_sal.company_name,
v_sal.company_address,
v_sal.branch_id,
v_sal.branch_code,
v_sal.branch_name,
v_sal.branch_address,
v_sal.sal_code, 
v_sal.ref_code,
v_sal.document_date,
v_sal.trans_desc,
vv1.name as STATUS,
v_sal.group_name,
v_sal.cus_code,
v_sal.cus_name,
v_sal.item_code,
v_sal.line_text,
v_sal.warehouse_code,
v_sal.sal_note,
v_sal.qty,
v_sal.price,
v_sal.amount,
v_sal.act_qty,
v_sal.act_price,
v_sal.act_amount,
v_sal.comm_amt,
v_sal.bks_description,
v_sal.uom_code,
v_sal.sales_name,
v_sal.trans_date,
v_sal.doc_type,
v_sal.AMOUNT_NONE_INVOICE,
v_sal.sob
FROM v_sal_invoice_detail v_sal left join valueset_value vv1 on vv1.code = v_sal.status and vv1.object_ref_id = 9692328
WHERE REGEXP_LIKE (v_sal.trans_desc||';'||v_sal.doc_type, :as_doc_type)
AND ((v_sal.trans_date BETWEEN :ad_s_date AND :ad_e_date AND REGEXP_LIKE(:as_trans_date,'Y')) 
OR (v_sal.document_date BETWEEN :ad_s_date AND :ad_e_date AND REGEXP_LIKE(:as_trans_date,'N')))
AND REGEXP_LIKE(v_sal.status, :as_status)" arguments=(("ad_s_date", date),("ad_e_date", date),("as_status", string),("as_doc_type", string),("as_trans_date", string))  sort="sal_code A " )
