 retrieve="SELECT
V_RECEIPT_RETAIL_DETAIL.company_name,
V_RECEIPT_RETAIL_DETAIL.company_address,
V_RECEIPT_RETAIL_DETAIL.branch_name,
V_RECEIPT_RETAIL_DETAIL.receipt_code,
V_RECEIPT_RETAIL_DETAIL.document_date,
V_RECEIPT_RETAIL_DETAIL.trans_desc,
      CASE UPPER (V_RECEIPT_RETAIL_DETAIL.status)
          WHEN N'BOOKED' THEN N'Đã ghi sổ' 
          WHEN N'NEW' THEN N'Mới'
       END
          AS STATUS,
V_RECEIPT_RETAIL_DETAIL.handled_by,
V_RECEIPT_RETAIL_DETAIL.object_code, 
V_RECEIPT_RETAIL_DETAIL.object_name,
V_RECEIPT_RETAIL_DETAIL.line_text, 
V_RECEIPT_RETAIL_DETAIL.qty,
V_RECEIPT_RETAIL_DETAIL.price,
V_RECEIPT_RETAIL_DETAIL.amount,
V_RECEIPT_RETAIL_DETAIL.base_amount,
V_RECEIPT_RETAIL_DETAIL.description,
V_RECEIPT_RETAIL_DETAIL.trans_date,
V_RECEIPT_RETAIL_DETAIL.receipt_person,
V_RECEIPT_RETAIL_DETAIL.dr_cr_object,
V_RECEIPT_RETAIL_DETAIL.uom_code,
V_RECEIPT_RETAIL_DETAIL.sob,
item.moving_avg,
valueset_value.code ,
nvl(valueset_value.name, 'Thu giảm công nợ') paymethod_name
FROM V_RECEIPT_RETAIL_DETAIL left join valueset_value on valueset_value.id = V_RECEIPT_RETAIL_DETAIL.payment_method
											 left join item on item.object_ref_id = V_RECEIPT_RETAIL_DETAIL.object_id
WHERE
            ((V_RECEIPT_RETAIL_DETAIL.trans_date BETWEEN :ad_s_date AND :ad_e_date AND REGEXP_LIKE(:as_trans_date,'Y')) 
                    OR (V_RECEIPT_RETAIL_DETAIL.document_date BETWEEN :ad_s_date AND :ad_e_date AND REGEXP_LIKE(:as_trans_date,'N')))
            AND REGEXP_LIKE(UPPER(V_RECEIPT_RETAIL_DETAIL.status),UPPER(:as_status))" arguments=(("ad_s_date", date),("ad_e_date", date),("as_status", string),("as_trans_date", string))  sort="paymethod_name A document_date A receipt_code A " )
