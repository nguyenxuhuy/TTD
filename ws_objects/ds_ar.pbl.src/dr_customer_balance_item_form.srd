 retrieve="select 'a' grp_idx , '1' grp_idx2, bs.trans_date trans_date, d.ref_code code_dr, null code_cr , sl.line_text,sl.qty, sl.price,sl.amount, tl.tax_correction,
         obs.OPEN_BASE_AMOUNT, obe.CLOSE_BASE_AMOUNT, null CR_BASE_AMOUNT,le.name,ba.address,
        o_cust.code, o_cust.name, vv.name,d.id
 from sal_invoice_line sl
 join document d on d.version_id = sl.object_ref_id and d.status not in ('new','cancelled')
 join booked_slip bs on bs.id = sl.object_ref_id and (bs.dr_cr_object = :a_obj_id or coalesce(:a_obj_id,0) = 0) and bs.trans_date between :a_s_date and :a_e_date
 left join valueset_value vv on vv.id= bs.loc_id
 join object o_cust on o_cust.id = bs.dr_cr_object
 left join object_balance obs on obs.object_id = bs.dr_cr_object and obs.balance_date = :a_s_date
 left join object_balance obe on obe.object_id = bs.dr_cr_object and obe.balance_date = :a_e_date
 left join tax_line tl on tl.object_ref_id = sl.id 
 join legal_entity le on le.id = d.branch_id
 join business_address ba on ba.object_ref_id = le.id
 union all
 select 'a' grp_idx , '2' grp_idx2, ot.trans_date  trans_date, null code_dr, ds.code code_cr ,
         bs.description,null, null,null, null,
         obs.OPEN_BASE_AMOUNT, obe.CLOSE_BASE_AMOUNT,
          ot.CR_BASE_AMOUNT
         ,le.name,ba.address, 
        o_cust.code, o_cust.name, null,null
 from  object_trans ot  
 join document ds on ds.id = ot.doc_ref_id 
                        and (ot.object_id= :a_obj_id or coalesce(:a_obj_id,0) = 0) 
                        and ot.trans_date between  :a_s_date and :a_e_date 
                        and ot.doc_ref_type in ('RECEIPT','BANK_VOUCHER')
                        and ot.DR_AMOUNT is null
 join booked_slip bs on bs.id = ds.version_id 
 join object o_cust on o_cust.id = ot.object_id
 left join object_balance obs on obs.object_id = ot.object_id and obs.balance_date = :a_s_date
 left join object_balance obe on obe.object_id = ot.object_id and obe.balance_date = :a_e_date
 join legal_entity le on le.id = ds.branch_id
 join business_address ba on ba.object_ref_id = le.id
 union all 
 select  'b' grp_idx ,'3' grp_idx2, null, null, null , sl.line_text,sum(sl.qty), sl.price, sum(sl.amount), sum(tl.tax_correction), null, null, null, le.name,ba.address,
    o_cust.code, o_cust.name, null,null
 from sal_invoice_line sl
 join document d on d.version_id = sl.object_ref_id and d.status not in ('new','cancelled')
 join booked_slip bs on bs.id = sl.object_ref_id and (bs.dr_cr_object = :a_obj_id or coalesce(:a_obj_id,0) = 0) and bs.trans_date between :a_s_date and :a_e_date
 join object o_cust on o_cust.id = bs.dr_cr_object
 left join tax_line tl on tl.object_ref_id = sl.id 
  join legal_entity le on le.id = d.branch_id 
 join business_address ba on ba.object_ref_id = le.id 
 group by sl.line_text, sl.price,le.name,ba.address, o_cust.code, o_cust.name" arguments=(("a_s_date", date),("a_e_date", date),("a_obj_id", number))  sort="legal_entity_name A business_address_address A object_code A object_name A grp_idx A grp_idx2 A booked_slip_trans_date A document_id A document_code_dr A code_cr A sal_invoice_line_line_text A " )
