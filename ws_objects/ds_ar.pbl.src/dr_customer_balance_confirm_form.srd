 retrieve="SELECT
COMPANY_NAME,
COMPANY_ADDRESS ,
OBJECT_CODE,
OBJECT_NAME,
DOCUMENT_CODE ,
DESCRIPTION   ,
TRANS_DATE   ,
OPEN_AMT ,
DR_AMT   , 
CR_AMT,
CLOSE_AMT  ,
COL_TMP      ,
S_DATE   ,
E_DATE,
BUSINESS_ADDRESS.TEL,
BUSINESS_ADDRESS.FAX,
V_OBJECT_SALES.ADDRESS,
V_OBJECT_SALES.PHONE,
V_OBJECT_SALES.FAX
FROM CUSTOMER_BALANCE_TMP JOIN V_OBJECT_SALES ON V_OBJECT_SALES.CODE = OBJECT_CODE
											JOIN LEGAL_ENTITY ON LEGAL_ENTITY.NAME = COMPANY_NAME
											JOIN BUSINESS_ADDRESS ON BUSINESS_ADDRESS.OBJECT_REF_ID = LEGAL_ENTITY.ID
WHERE  USER_ID = :AN_USER_ID" arguments=(("AN_USER_ID", number)) )
