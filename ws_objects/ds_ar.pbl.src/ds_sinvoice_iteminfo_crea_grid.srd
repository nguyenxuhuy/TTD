table(column=(type=number updatewhereclause=yes name=linenumber dbname="sal_invoice_line.line_no" dbalias="sal.line_no" )
 column=(type=char(100) updatewhereclause=yes name=itemcode dbname="object.code" dbalias="it.code" )
 column=(type=char(500) updatewhereclause=yes name=itemname dbname="object.name" dbalias="it.name" )
 column=(type=char(20) updatewhereclause=yes name=unitname dbname="uom.code" dbalias=".code" )
 column=(type=number updatewhereclause=yes name=unitprice dbname="sal_invoice_line.unitprice" dbalias=".unitprice" )
 column=(type=number updatewhereclause=yes name=quantity dbname="sal_invoice_line.qty" dbalias="sal.qty" )
 column=(type=number updatewhereclause=yes name=itemtotalamountwithouttax dbname="sal_invoice_line.itemtotalamountwithouttax" dbalias=".itemtotalamountwithouttax" )
 column=(type=number updatewhereclause=yes name=taxpercentage dbname="tax_line.tax_pct" dbalias="tax.tax_pct" )
 column=(type=number updatewhereclause=yes name=taxamount dbname="sal_invoice_line.taxamount" dbalias=".taxamount" )
 column=(type=number updatewhereclause=yes name=discount dbname="sal_invoice_line.discount" dbalias=".discount" )
 column=(type=number updatewhereclause=yes name=itemdiscount dbname="sal_invoice_line.itemdiscount" dbalias=".itemdiscount" )
 retrieve="select sal.line_no,
        it.code,
        it.name,
        uom.code,
        case when sal.price is null then 0
            else sal.price
        end unitPrice,
        sal.qty,
        case when sal.amount_ex_tax is null then 0
            else sal.amount_ex_tax
        end itemTotalAmountWithoutTax ,
        tax.tax_pct,
        case when tax.tax_correction is null then 0
            else tax.tax_correction
        end taxAmount,
        case when sal.disc_pct is null then 0
            else  sal.disc_pct
        end discount,
        case when sal.disc_amount is null then 0
            else sal.disc_amount
        end itemDiscount
from sal_invoice_line sal join document doc on doc.version_id = sal.object_ref_id
                                    join object it on it.id = sal.object_id
                                    join uom on uom.id = sal.trans_uom
                                    join tax_line tax on tax.object_ref_id = sal.id
where doc.id = :an_doc_id" arguments=(("an_doc_id", number)) )
