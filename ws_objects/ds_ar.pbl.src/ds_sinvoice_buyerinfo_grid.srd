table(column=(type=char(50) updatewhereclause=yes name=buyername dbname="booked_slip.buyer_person" dbalias="bks.buyer_person" )
 column=(type=char(500) updatewhereclause=yes name=buyerlegalname dbname="object.name" dbalias="obj.name" )
 column=(type=char(20) updatewhereclause=yes name=buyertaxcode dbname="booked_slip.buyertaxcode" dbalias=".buyertaxcode" )
 column=(type=char(200) updatewhereclause=yes name=buyeraddressline dbname="booked_slip.invoice_address" dbalias="bks.invoice_address" )
 column=(type=char(30) updatewhereclause=yes name=buyerphonenumber dbname="booked_slip.buyerphonenumber" dbalias=".buyerphonenumber" )
 column=(type=char(100) updatewhereclause=yes name=buyeridno dbname="object.code" dbalias="obj.code" )
 column=(type=char(1) updatewhereclause=yes name=buyeridtype dbname="buyeridtype" )
 retrieve="select bks.buyer_person,
            obj.name,
            case when org1.tax_number is null then org2.tax_number
                else org1.tax_number
            end buyerTaxCode,
            bks.invoice_address,
            case when bus1.tel is null then bus2.tel
                else bus1.tel
            end buyerPhoneNumber,
            obj.code,
            '1' as buyerIdType
from booked_slip bks join document doc on doc.version_id = bks.id and doc.DOC_TYPE = 'SAL_INVOICE'
							join object obj on obj.id = bks.invoice_object and obj.object_ref_table in ('CUSTOMER','VENDOR')
                                left join customer cus on cus.object_ref_id = obj.id
                                left join ORGANIZATION org1 on org1.object_ref_id = cus.id
                                left join vendor ven on ven.object_ref_id = obj.id
                                left join ORGANIZATION org2 on org2.object_ref_id = ven.id
                                left join BUSINESS_ADDRESS bus1 on bus1.object_ref_id = cus.id
                                left join BUSINESS_ADDRESS bus2 on bus2.object_ref_id = ven.id
where doc.id = :an_doc_id" arguments=(("an_doc_id", number)) )
