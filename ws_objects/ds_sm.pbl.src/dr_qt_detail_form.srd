 retrieve="SELECT
V_QT_LINE.COMPANY_CODE,
V_QT_LINE.COMPANY_NAME,
V_QT_LINE.COMP_ADDRESS,
V_QT_LINE.doc_code,
V_QT_LINE.DOCUMENT_DATE,
V_QT_LINE.TRANS_DESC,
V_QT_LINE.status_name,
V_QT_LINE.PAYMENT_TERM_NAME,
V_QT_LINE.STAFF_NAME,
V_QT_LINE.CUS_CODE,
V_QT_LINE.CUS_NAME,
V_QT_LINE.CUS_ADDRESS,
V_QT_LINE.LAST_APPR_NAME,
V_QT_LINE.ITEM_CODE,
V_QT_LINE.ITEM_NAME,
V_QT_LINE.OTHER_NOTE,  
V_QT_LINE.QTY, 
V_QT_LINE.PRICE,
V_QT_LINE.amount_ex_tax,
V_QT_LINE.act_amount_ex_tax,
V_QT_LINE.MARKUP,
V_QT_LINE.COMM_AMT,
V_QT_LINE.ORDERS_NOTE,
V_QT_LINE.INVOIcE_REQUEST_name,
V_QT_LINE.USER_ID,
V_QT_LINE.base_price,
V_QT_LINE.base_amount_ex_tax
FROM V_QT_LINE 
WHERE V_QT_LINE.doc_type = 'QT'
AND V_QT_LINE.document_date BETWEEN :ad_s_date AND :ad_e_date
AND REGEXP_LIKE(TRIM(V_QT_LINE.status_code),TRIM(:as_status) )
AND REGEXP_LIKE(TRIM(V_QT_LINE.invoice_request_code) , TRIM(:as_req)  )
AND REGEXP_LIKE(UPPER(TRIM(V_QT_LINE.staff_code)) , UPPER(TRIM(:as_staff_code) ))" arguments=(("ad_s_date", date),("ad_e_date", date),("as_status", string),("as_req", string),("as_staff_code", string)) )
