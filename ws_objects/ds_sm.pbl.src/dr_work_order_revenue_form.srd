table(column=(type=char(100) updatewhereclause=yes name=company_name dbname="legal_entity.company_name" dbalias="comp.company_name" )
 column=(type=char(200) updatewhereclause=yes name=company_address dbname="business_address.company_address" dbalias="adr1.company_address" )
 column=(type=char(30) updatewhereclause=yes name=code dbname="document.code" dbalias="doc.code" )
 column=(type=datetime updatewhereclause=yes name=document_date dbname="document.document_date" dbalias="doc.document_date" )
 column=(type=number updatewhereclause=yes name=handled_by dbname="document.handled_by" dbalias="doc.handled_by" )
 column=(type=char(500) updatewhereclause=yes name=cus_name dbname="object.name" dbalias="obj.name" )
 column=(type=char(200) updatewhereclause=yes name=ship_to_addtext dbname="orders.ship_to_addtext" dbalias="ord.ship_to_addtext" )
 column=(type=char(100) updatewhereclause=yes name=item_code dbname="object.code" dbalias="obj1.code" )
 column=(type=char(500) updatewhereclause=yes name=item_name dbname="object.name" dbalias="obj1.name" )
 column=(type=char(250) updatewhereclause=yes name=note dbname="so_line.note" dbalias="so.note" )
 column=(type=number updatewhereclause=yes name=amount dbname="so_line.amount" dbalias="so.amount" )
 column=(type=number updatewhereclause=yes name=sal_amount dbname="sal_invoice_line.amount" dbalias="sal.amount" )
 retrieve="SELECT 
comp.name AS company_name,
adr1.address AS company_address,
doc.code,
doc.document_date,
doc.handled_by,
obj.name,
ord.ship_to_addtext,
obj1.code,
obj1.name,
so.note, 
so.amount, 
sal.amount       
FROM document doc
JOIN orders ord ON ord.id = doc.version_id
JOIN so_line so ON so.object_ref_id = doc.version_id 
JOIN matching mat1 ON mat1.f_ref_id = so.id AND LOWER(mat1.f_ref_table) = 'so_line'
JOIN matching mat2 ON mat2.f_ref_id =  mat1.t_ref_id  AND LOWER(mat2.f_ref_table) = 'so_line'
JOIN matching mat3 ON mat3.f_ref_id = mat2.t_ref_id AND LOWER(mat3.f_ref_table) = 'inventory_line'
JOIN sal_invoice_line sal ON sal.id = mat3.t_ref_id
JOIN document doc1 ON doc1.version_id = sal.object_ref_id
JOIN object obj ON obj.id = ord.object_id
JOIN object obj1 ON obj1.id = so.item_id
JOIN ad.legal_entity comp ON doc.company_id = comp.id
LEFT JOIN ad.business_address adr1 ON comp.id = adr1.object_ref_id
WHERE LOWER(doc.doc_type) = 'work_order'
AND doc1.status NOT IN ('new', 'cancelled')
AND doc1.document_date BETWEEN :ad_s_date AND :ad_e_date
AND REGEXP_LIKE(LOWER(obj1.code) , LOWER(:as_item_code))
AND REGEXP_LIKE(LOWER(obj.code) , LOWER(:as_cus_code))" arguments=(("ad_s_date", date),("ad_e_date", date),("as_item_code", string),("as_cus_code", string)) )
