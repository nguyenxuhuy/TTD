 retrieve="SELECT '' as gutter,
object.id,
object.code,   
object.name,   
object.short_name,    
object.manage_group, 
object.posting_group, 
object.handled_by, 
object.active_yn, 
object.company_id,  
object.branch_id,
object.object_ref_id,
object.object_ref_table,
object.created_by,
object.created_date,  
object.last_mdf_by,  
object.last_mdf_date,
object.note,
VA1X.CODE, 
VA1X.NAME,
VA2X.CODE, 
VA2X.NAME,
O_STAFF.CODE, 
O_STAFF.NAME, 
ATTACHMENT.LINE_NO,
0 AS PROSPECT_ID
FROM OBJECT 
 LEFT JOIN VALUESET_VALUE VA1X ON OBJECT.MANAGE_GROUP=VA1X.ID 
LEFT JOIN VALUESET_VALUE VA2X ON OBJECT.POSTING_GROUP=VA2X.ID
LEFT JOIN ROLE_USER ON ROLE_USER.ID=OBJECT.HANDLED_BY
    AND ROLE_USER.TYPE='user'
LEFT JOIN OBJECT O_STAFF ON O_STAFF.ID=ROLE_USER.OBJECT_STAFF
LEFT JOIN ATTACHMENT ON ATTACHMENT.DWOBJECT='d_objects_form'
                                     AND ATTACHMENT.OBJECT_REF_ID=OBJECT.ID
                                     AND ATTACHMENT.BRANCH_ID=OBJECT.BRANCH_ID
                                     AND ATTACHMENT.LINE_NO=1" update="OBJECT" updatewhere=1 updatekeyinplace=yes )
