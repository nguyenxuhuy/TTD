table(column=(type=char(500) updatewhereclause=yes name=label_text dbname="label.text" dbalias="L.text" )
 retrieve="  SELECT L.TEXT
    FROM MENU m
    JOIN LABEL  L  ON M.CODE = L.CODE
    left join  identification_key ik on ik.object_ref_code = M.code
    left join  policy_hdr ph  on ik.ID = PH.IDENTIFICATION_ID
    left join  APPLICABLE_RULE_HDR arh on ARH.OBJECT_REF_ID = PH.ID
  where 
         L.LANG = :as_language
         and (nvl(ph.ACCESS_YN,'N') = 'Y' or  :an_user_id in(0, 7677, 8829))
         and  nvl(ph.company_id,:an_company_id) = :an_company_id
         and nvl(ph.branch_id, :an_branch_id) =  :an_branch_id
         and M.MENU_ID = :an_menu_id " arguments=(("as_language", string),("an_user_id", number),("an_company_id", number),("an_branch_id", number),("an_menu_id", number)) )
