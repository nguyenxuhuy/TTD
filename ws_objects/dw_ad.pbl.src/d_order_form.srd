 retrieve="select
  ord.ID, 
  ord.OBJECT_REF_ID ,
  ord.OBJECT_REF_TABLE,
  ord.COMPANY_ID,
  ord.BRANCH_ID, 
  ord.CREATED_BY,
  ord.CREATED_DATE, 
  ord.LAST_MDF_BY, 
  ord.LAST_MDF_DATE,     
  ord.OBJECT_ID,  
  ord.REQUEST_SHIP_DATE , 
  ord.REQUEST_RECEIVE_DATE, 
  ord.CONFIRM_RECEIVE_DATE,
  ord.CURRENCY_ID,  
  ord.VERSION_NO,   
  ord.INVOICE_REQUEST,  
  ord.PAYMENT_TERM, 
  ord.PAYMENT_METHOD,  
  ord.DELIVERY_MODE, 
  ord.NOTE,
  ord.DOC_TYPE,
  ord.CONFIRM_SHIP_DATE, 
  ord.EXCHANGE_RATE,
  ord.SHIP_TO_ADDRESS,
  ord.BILL_TO_ADDRESS,
  ord.SHIP_TO_ADDTEXT,
  ord.BILL_TO_ADDTEXT,
  ord.CONTACT_ID,
  ord.BILL_TO_OBJECT,
  ord.DUE_DATE,
  ord.BASE_DATE,
  ord.INCLUDE_TAX_YN,  
  ord.contact_person,  
  O_VENDX.CODE,
  O_VENDX.NAME,  
  PAYMENT_TERM.CODE,
  PAYMENT_TERM.NAME,
  VALUE1.CODE,
  VALUE1.NAME, 
  VALUE2.CODE, 
  VALUE2.NAME,
  O_ALL.CODE,
  O_ALL.NAME, 
  UOM.CODE,
  UOM.NAME,  
  O_VENDX.OBJECT_REF_TABLE 
FROM ORDERS ORD
LEFT JOIN OBJECT O_VENDX ON ORD.OBJECT_ID=O_VENDX.ID
LEFT JOIN PAYMENT_TERM ON ORD.PAYMENT_TERM=PAYMENT_TERM.ID
LEFT JOIN VALUESET_VALUE VALUE1 ON ORD.PAYMENT_METHOD=VALUE1.ID
LEFT JOIN VALUESET_VALUE VALUE2 ON ORD.DELIVERY_MODE=VALUE2.ID

LEFT JOIN UOM ON ORD.CURRENCY_ID=UOM.ID
LEFT JOIN OBJECT O_ALL ON ORD.BILL_TO_OBJECT=O_ALL.ID 
        AND O_ALL.OBJECT_REF_TABLE IN ('CUSTOMER','VENDOR','STAFF')" update="ORDERS" updatewhere=1 updatekeyinplace=yes )
