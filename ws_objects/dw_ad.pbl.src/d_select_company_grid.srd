 retrieve="SELECT '' as gutter,
         business_group.code,
         BUSINESS_GROUP.NAME,  
         LEGAL_ENTITY.code,
         LEGAL_ENTITY.name,         
         branch.code,   
         branch.NAME,
         LEGAL_ENTITY.id,   
         LEGAL_ENTITY.branch_yn,   
         nvl(LEGAL_ENTITY.up_id,0) ,  
         LEGAL_ENTITY.bus_grp_id,
         branch.id
    FROM legal_entity   
    left join    legal_entity branch on branch.up_id =  LEGAL_ENTITY.id
    left join business_group on legal_entity.bus_grp_id = business_group.id
    where nvl(legal_entity.branch_yn,'N') <> 'Y'" )
