 retrieve="select '' gutter,
        tasklist.ID,
        tasklist.OBJECT_REF_ID,
        tasklist.OBJECT_REF_TABLE,
        tasklist.COMPANY_ID,
        tasklist.BRANCH_ID,
        tasklist.CREATED_BY,
        tasklist.CREATED_DATE,
        tasklist.LAST_MDF_BY,
        tasklist.LAST_MDF_DATE,
        tasklist.F_TASK_DATE,
        tasklist.T_TASK_DATE,
        tasklist.F_TASK_TIME,
        tasklist.T_TASK_TIME,
        tasklist.ASSIGNED_DATE,
        tasklist.PLAN_SOLVE_DATE,
        tasklist.ACT_SOLVE_DATE,
        tasklist.ASSIGNED_DOC,
        tasklist.ASSIGNED_OBJECT,
        tasklist.ASSIGNED_BY,
        tasklist.TASK_FREQUENCY,
        tasklist.NEXT_STEP,
        tasklist.STAFF_REQ_SUPPORT,
        tasklist.CUST_REQ_SUPPORT,
        tasklist.NOTE,
        tasklist.DESCRIPTION,
        tasklist.SALES_STEP,
        tasklist.STATUS,
        tasklist.APPR_ID,
		tasklist.line_no,
		vru.staff_name,
		obj.code,
		obj.name,
		doc.code,
		vapm.staff_name,
		vna.next_appr_name,
		st.name,
		tf.name
from tasklist  left join v_role_user vru on vru.id = tasklist.ASSIGNED_BY
				  left join object obj on obj.id = tasklist.ASSIGNED_OBJECT
				  left join document doc on doc.id = tasklist.ASSIGNED_DOC
				  left join V_APPR_PROFILE_MAX vapm on vapm.object_ref_id = tasklist.id
				  left join V_NEXT_APPR vna on vna.object_ref_id = tasklist.id
				  left join valueset_value st on st.code = tasklist.status
			 	  left join valueset_value tf on tf.code = tasklist.TASK_FREQUENCY" update="tasklist" updatewhere=0 updatekeyinplace=no )
