 retrieve="SELECT '' gutter,
		TH.ID,
        TH.OBJECT_REF_ID,
        TH.OBJECT_REF_TABLE,
        TH.COMPANY_ID,
        TH.BRANCH_ID,
        TH.CREATED_BY,
        TH.CREATED_DATE,
        TH.LAST_MDF_BY,
        TH.LAST_MDF_DATE,
        TH.F_TASK_DATE,
        TH.T_TASK_DATE,
        TH.F_TASK_TIME,
        TH.T_TASK_TIME,
        TH.TASK_FREQUENCY,
        TH.NOTE,
        TH.DESCRIPTION,
        TH.APPR_ID,
        TH.LOOP_YN,
		TH.OBJECT_ID,
		FRE.NAME,
		ST.NAME,
		VAPM.STAFF_NAME,
		VNA.NEXT_APPR_NAME,
		OBJ.CODE,
		OBJ.NAME,
		0 desc_id,
		TH.STATUS,
		attachment.line_no,
		TH.SOLVE_PERSON,
		VRU1.STAFF_NAME
FROM TASKLIST_HDR TH LEFT JOIN VALUESET_VALUE FRE ON FRE.CODE = TH.TASK_FREQUENCY
                                     LEFT JOIN FLEXIBLE_DATA FD ON FD.ID = FRE.OBJECT_REF_ID AND FD.CODE = 'u_valueset_entry@taskfrequency'
								 LEFT JOIN VALUESET_VALUE ST ON ST.CODE = TH.STATUS
                                     LEFT JOIN FLEXIBLE_DATA FD1 ON FD1.ID = ST.OBJECT_REF_ID AND FD1.CODE = 'u_valueset_entry@docstatus'
								 LEFT JOIN V_APPR_PROFILE_MAX VAPM ON VAPM.OBJECT_REF_ID = TH.ID
                                     LEFT JOIN V_NEXT_APPR VNA ON VNA.OBJECT_REF_ID = TH.ID
								LEFT JOIN OBJECT OBJ ON OBJ.ID = TH.OBJECT_ID
								LEFT JOIN V_ROLE_USER VRU1 ON VRU1.ID = TH.SOLVE_PERSON
								LEFT JOIN ATTACHMENT ON ATTACHMENT.DWOBJECT='d_tasklist_hdr_grid'
									  AND ATTACHMENT.OBJECT_REF_ID=TH.ID
									  AND ATTACHMENT.BRANCH_ID=TH.BRANCH_ID
									  AND ATTACHMENT.LINE_NO=1" update="TASKLIST_HDR" updatewhere=1 updatekeyinplace=yes )
