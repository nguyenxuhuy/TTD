 retrieve="select 
PEOPLE.CREATED_BY,
PEOPLE.CREATED_DATE,
PEOPLE.DATE_OF_BIRTH,
PEOPLE.EMAIL,
PEOPLE.FIRST_NAME, 
PEOPLE.LAST_NAME,
PEOPLE.ID,
PEOPLE.LAST_MDF_BY, 
PEOPLE.LAST_MDF_DATE,
PEOPLE.MAIN_YN,
PEOPLE.OBJECT_REF_ID, 
PEOPLE.OBJECT_REF_TABLE,
PEOPLE.PHONE,
PEOPLE.TITLE,
PEOPLE.GENDER, 
PEOPLE.NO_OF_CHILDREN,
PEOPLE.IDENTIFICATION_CARD,
PEOPLE.CARD_ISSUED_DATE,
PEOPLE.CARD_ISSUED_PLACE,
PEOPLE.NATIVE_PLACE,
PEOPLE.NATIONAL_ID,
PEOPLE.ETHNIC_ID,
PEOPLE.RELIGION_ID,
PEOPLE.PAYMENT_METHOD,
PEOPLE.PLACE_OF_BIRTH,
PEOPLE.BANK_ACCT,
PEOPLE.BANK_NAME,
PEOPLE.STRENGTH,
PEOPLE.WEAKNESS,
PEOPLE.PERSONALITY,
PEOPLE.INTEREST,
PEOPLE.RELATIVE_YN,
NT.CODE,
NT.NAME,
EN.CODE,
EN.NAME,
RG.CODE,
RG.NAME,
PEOPLE.marriage_status,
PA.CODE,
PA.NAME
from PEOPLE left join valueset_value nt on nt.id = PEOPLE.NATIONAL_ID
                    left join valueset_value en on en.id = PEOPLE.ETHNIC_ID
                    left join valueset_value rg on rg.id = PEOPLE.RELIGION_ID
                    left join valueset_value pa on pa.id = PEOPLE.PAYMENT_METHOD
left join staff s on s.id= people.object_ref_id
left join role_user ru on ru.object_staff = s.object_ref_id" update="PEOPLE" updatewhere=1 updatekeyinplace=yes )
