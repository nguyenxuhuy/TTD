 retrieve="SELECT '' gutter,
		TL.ID,
        TL.OBJECT_REF_ID,
        TL.OBJECT_REF_TABLE,
        TL.COMPANY_ID,
        TL.BRANCH_ID,
        TL.CREATED_BY,
        TL.CREATED_DATE,
        TL.LAST_MDF_BY,
        TL.LAST_MDF_DATE,
        TL.F_TASK_DATE,
        TL.T_TASK_DATE,
        TL.F_TASK_TIME,
        TL.T_TASK_TIME,
        TL.ASSIGNED_DATE,
        TL.PLAN_SOLVE_DATE,
        TL.ACT_SOLVE_DATE,
        TL.ASSIGNED_DOC,
        TL.ASSIGNED_OBJECT,
        TL.ASSIGNED_BY,
        TL.NEXT_STEP,
        TL.STAFF_REQ_SUPPORT,
        TL.CUST_REQ_SUPPORT,
        TL.NOTE,
        TL.DESCRIPTION,
        TL.SALES_STEP,
        TL.APPR_ID,
        TL.LINE_NO,
		TL.SOLVE_PERSON,
        ST.NAME,
        VAP.APPR_NAME,
        OBJ.CODE,
        OBJ.NAME,
        VRU.STAFF_NAME,
		VRU1.STAFF_NAME,
		TL.STATUS,
		TL.FIRST_VALUE,
		TL.SECOND_VALUE,
		TL.THIRD_VALUE,
		attachment.line_no,
		VAWT.NUMBER_EXPIRED,
		VNA.NEXT_APPR_ASSIGNED,
		VNA.NEXT_APPR_NAME
FROM TASKLIST_LINE TL LEFT JOIN VALUESET_VALUE ST ON ST.CODE = TL.STATUS
                                       LEFT JOIN FLEXIBLE_DATA FD ON FD.ID = ST.OBJECT_REF_ID AND FD.CODE = 'u_valueset_entry@docstatus'
                                       LEFT JOIN V_APPR_PROFILE VAP ON VAP.OBJECT_REF_ID = TL.ID
                                       LEFT JOIN OBJECT OBJ ON OBJ.ID = TL.ASSIGNED_OBJECT
                                       LEFT JOIN V_ROLE_USER VRU ON VRU.ID = TL.ASSIGNED_BY and vru.lang = 'vi-vn'
									LEFT JOIN V_ROLE_USER VRU1 ON VRU1.ID = TL.SOLVE_PERSON and vru1.lang = 'vi-vn'
									LEFT JOIN V_APPR_WAITING_TASKLIST VAWT ON VAWT.OBJECT_REF_ID = TL.ID
									LEFT JOIN V_NEXT_APPR VNA ON VNA.OBJECT_REF_ID = TL.ID
									LEFT JOIN ATTACHMENT ON ATTACHMENT.DWOBJECT='d_tasklist_line_grid'
									  AND ATTACHMENT.OBJECT_REF_ID=TL.ID
									  AND ATTACHMENT.BRANCH_ID=TL.BRANCH_ID
									  AND ATTACHMENT.LINE_NO=1" update="TASKLIST_LINE" updatewhere=1 updatekeyinplace=yes )
