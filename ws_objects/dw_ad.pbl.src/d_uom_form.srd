 retrieve="select 
UOM.ID,
UOM.CODE,
UOM.NAME,
UOM.CLASS_ID,
UOM.BASE_YN,
UOM.UOM_ROUND_ID,
UOM.CREATED_BY,
UOM.CREATED_DATE,
UOM.LAST_MDF_BY,
UOM.LAST_MDF_DATE,
UNIT_CLASS.CODE,
ROUNDING.CODE
from UOM 
		left join UNIT_CLASS on UNIT_CLASS.ID = UOM.CLASS_ID
		left join ROUNDING on ROUNDING.ID = UOM.UOM_ROUND_ID
where nvl( UNIT_CLASS.currency_yn,'N') = 'N'" update="UOM" updatewhere=1 updatekeyinplace=yes )
