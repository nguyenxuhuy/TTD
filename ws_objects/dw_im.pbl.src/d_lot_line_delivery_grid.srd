 column=(type=char(100) updatewhereclause=yes name=combination_name dbname="valueset_value.name" dbalias="val.name" )
 column=(type=char(5) updatewhereclause=yes name=lot_number dbname="lot_number" )
 column=(type=char(100) updatewhereclause=yes name=cus_code dbname="object.code" dbalias="obj.code" )
 column=(type=char(500) updatewhereclause=yes name=cus_name dbname="object.name" dbalias="obj.name" )
 column=(type=datetime updatewhereclause=yes name=delivery_date dbname="document.document_date" dbalias="doc.document_date" )
 retrieve="select '' as gutter,
        lot.LOT_NO,
        obj1.code,
        obj1.name,
        val.name,
        substr(lot.lot_no,-9,5) as lot_number,
        obj.code,
        obj.name,
        doc.document_date            
from inventory_line inv join lot_line lot  on inv.id = lot.object_ref_id  
                       left join document doc on doc.version_id = inv.object_ref_id 
                       left join object obj on obj.id = inv.object_id
                       left join object obj1 on obj1.id = inv.item_id
                       left join valueset_value val on val.code = obj1.code
where doc.doc_type = 'ASSEMBLE' " )
