 retrieve="SELECT '' as gutter,
object.id,
object.code,   
object.name,   
object.short_name,    
object.print_name,    
object.manage_group, 
object.location, 
object.company_id, 
object.branch_id,
object.created_by,
object.created_date,   
object.last_mdf_by,    
object.last_mdf_date,
object.note,
VA1X.CODE,   
VA1X.NAME ,
i.manufacturers, 
i.ingredient,
i.spec_desc,
i.origin
FROM OBJECT 
Left join item i on i.object_ref_id = object.id
LEFT JOIN VALUESET_VALUE VA1X ON OBJECT.MANAGE_GROUP=VA1X.ID 
WHERE object.active_yn = 'Y' and coalesce(object.location,0) = 0 and OBJECT.object_ref_Table = 'ITEM'" update="OBJECT" updatewhere=1 updatekeyinplace=yes )
