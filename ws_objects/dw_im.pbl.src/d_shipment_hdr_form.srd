 retrieve="SELECT
    bks.ID,  
    bks.BRANCH_ID,
    bks.COMPANY_ID,
    bks.OBJECT_REF_ID,
    bks.OBJECT_REF_TABLE,
    bks.CREATED_BY, 
    bks.CREATED_DATE, 
    bks.LAST_MDF_BY,    
    bks.LAST_MDF_DATE,   
    bks.DOC_TYPE, 
    bks.DESCRIPTION, 
    bks.VERSION_NO, 
    bks.TRANS_DATE,
    bks.TRANS_ADDRESS,              
    bks.DELIVERY_PERSON,
    bks.RECEIPT_PERSON,    
    bks.DELIVERY_MODE,
    bks.INVOICE_OBJECT,  
    bks.INVOICE_SIGN, 
    bks.INVOICE_CODE, 
    bks.BANK_STATEMENT_NO,
    bks.VENDOR_DELIVERY_NO,
    bks.INVOICE_YN, 
    bks.QUANTITY_YN,
    bks.VALUE_YN,
    bks.ALLOCATION_YN,
    bks.DR_CR_OBJECT,
    bks.INVOICE_ADDRESS,
    bks.exchange_rate,
    bks.payment_term,
    bks.payment_method,
    bks.due_date,
    bks.invoice_received_date,
    bks.include_tax_yn,
    bks.currency_id,
    bks.REPLACE_YN          ,   
    bks.BE_REPLACED_YN         ,
    bks.SUBACCOUNT, 
    bks.CUSTOMS_DECLARATION_NO,
    bks.tax_desc,
    bks.NOTE,
    bks.qty,
    bks.qty_sku,
    bks.cogs,
    bks.f_rec_date,
    bks.t_rec_date,
    bks.f_shipdate,
    bks.t_shipdate,
    o_cust.code,
    o_cust.name,
    o_item.code,
    o_item.name,
    item_spec.code,
    item_spec.name,
    uom.code
FROM BOOKED_SLIP bks
left join object o_cust on o_cust.id = bks.DR_CR_OBJECT 
left join object o_item on o_item.id = bks.INVOICE_OBJECT 
left join item_spec on item_spec.id = bks.SUBACCOUNT
left join uom on uom.id= bks.currency_id
where bks.doc_type = 'SHIPMENT'" update="BOOKED_SLIP" updatewhere=1 updatekeyinplace=yes )
