table(column=(type=number updatewhereclause=yes name=line_no dbname="inventory_line.line_no" )
 column=(type=char(100) updatewhereclause=yes name=object_code dbname="object.code" )
 column=(type=char(200) updatewhereclause=yes name=line_text dbname="inventory_line.line_text" )
 column=(type=number updatewhereclause=yes name=qty dbname="inventory_line.qty" )
 column=(type=number updatewhereclause=yes name=so_qty dbname="so_qty" )
 column=(type=number updatewhereclause=yes name=sal_r_qty dbname="sal_r_qty" )
 column=(type=number updatewhereclause=yes name=sal_qty dbname="sal_qty" )
 column=(type=number updatewhereclause=yes name=inv_r_qty dbname="inv_r_qty" )
 retrieve="select inv.line_no,
 object.code,
inv.line_text,
qty,
sum(nvl(VIE.SO_QTY,0)) so_qty,
sum(nvl(VIE.SO_R_QTY,0)) sal_r_qty,
sum(nvl(VIE.SAL_QTY,0)) sal_qty,
sum(nvl(vie.inv_r_qty,0)) inv_r_qty
from document doc
left join booked_slip bks on doc.version_id=bks.id
left join inventory_line inv on bks.id=inv.object_ref_id
left join object on inv.item_id = object.id
left join v_doc_detail vie on vie.inv_id=inv.id
where doc.id= :an_doc_id
group by inv.line_no,
 object.code,
inv.line_text,
qty
order by  inv.line_no asc" arguments=(("an_doc_id", number)) )
