table(column=(type=datetime updatewhereclause=yes name=trans_date dbname="booked_slip.trans_date" dbalias="bks.trans_date" )
 column=(type=char(30) updatewhereclause=yes name=document_code dbname="document.code" dbalias="doc.code" )
 column=(type=char(50) updatewhereclause=yes name=trans_desc dbname="trans_setup_hdr.trans_desc" dbalias="trhd.trans_desc" )
 column=(type=char(100) updatewhereclause=yes name=warehouse_name dbname="warehouse_name" )
 column=(type=char(500) updatewhereclause=yes name=object_name dbname="object_name" )
 column=(type=char(100) updatewhereclause=yes name=serial_no dbname="lot_line.serial_no" dbalias="lot.serial_no" )
 retrieve="SELECT
bks.trans_date,
doc.code,
trhd.trans_desc,
CASE doc.doc_type
WHEN N'GOODS_RECEIPT' then wh.code
ELSE obj.code
END as warehouse_name, 
CASE doc.doc_type
WHEN N'GOODS_RECEIPT' then obj.name 
ELSE N''
END as object_name,
lot.serial_no
FROM document doc
LEFT JOIN trans_setup_hdr trhd ON trhd.id = doc.trans_hdr_id
JOIN  booked_slip bks ON bks.id = doc.version_id
JOIN inventory_line inv on inv.object_ref_id = bks.id
LEFT JOIN lot_line lot on lot.object_ref_id = inv.id
LEFT JOIN item ON item.object_ref_id = inv.item_id AND item.serial_yn = 'Y'
LEFT JOIN object wh on wh.id = inv.warehouse_id
LEFT JOIN object obj on obj.id = inv.object_id
WHERE REGEXP_LIKE(upper(lot.serial_no), upper( :as_serial_no) )
AND doc.doc_type IN ('GOODS_RECEIPT','GOODS_TRANSFER')
ORDER BY bks.trans_date ASC" arguments=(("as_serial_no", string)) )
