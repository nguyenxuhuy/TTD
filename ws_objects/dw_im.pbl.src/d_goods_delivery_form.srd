 retrieve="SELECT 
            BOOKED_SLIP.ID,
            BOOKED_SLIP.BRANCH_ID, 
            BOOKED_SLIP.COMPANY_ID    , 
            BOOKED_SLIP.OBJECT_REF_ID    ,
            BOOKED_SLIP.OBJECT_REF_TABLE    ,
            BOOKED_SLIP.CREATED_BY    , 
            BOOKED_SLIP.CREATED_DATE        ,
            BOOKED_SLIP.LAST_MDF_BY         , 
            BOOKED_SLIP.LAST_MDF_DATE       ,
            BOOKED_SLIP.DOC_TYPE           ,
            BOOKED_SLIP.DESCRIPTION        , 
            BOOKED_SLIP.VERSION_NO          ,
            BOOKED_SLIP.TRANS_DATE          , 
            BOOKED_SLIP.TRANS_ADDRESS      ,  
            BOOKED_SLIP.DELIVERY_PERSON     ,
            BOOKED_SLIP.RECEIPT_PERSON     ,  
            BOOKED_SLIP.DELIVERY_MODE      ,  
            BOOKED_SLIP.INVOICE_OBJECT           ,  
            BOOKED_SLIP.INVOICE_SIGN        ,
            BOOKED_SLIP.INVOICE_CODE       ,
            BOOKED_SLIP.BANK_STATEMENT_NO  , 
            BOOKED_SLIP.VENDOR_DELIVERY_NO,
            BOOKED_SLIP.quantity_yn,
            BOOKED_SLIP.value_yn,  
            BOOKED_SLIP.REPLACE_YN          ,   
            BOOKED_SLIP.BE_REPLACED_YN         ,
            BOOKED_SLIP.CURRENCY_ID,
            BOOKED_SLIP.EXCHANGE_RATE,
            BOOKED_SLIP.INVOICE_ADDRESS,
            BOOKED_SLIP.PAYMENT_TERM, 
            BOOKED_SLIP.PAYMENT_METHOD,
            BOOKED_SLIP.invoice_request, 
            BOOKED_SLIP.ATTRIBUTE,  
            BOOKED_SLIP.WH_RECEIPT_DESC,
            BOOKED_SLIP.NOTE,
            BOOKED_SLIP.include_tax_yn,
            VAL.CODE,
            VAL.NAME,
            PAY.CODE,  
            PAY.NAME,
            VA1.CODE,
            VA1.NAME,
            UOM.CODE,
            UOM.NAME,
            ORGANIZATION.TAX_NUMBER,
            BOOKED_SLIP.DR_CR_OBJECT,
            OJCTM.CODE,
            OJCTM.NAME
            FROM BOOKED_SLIP 
            LEFT JOIN VALUESET_VALUE VAL ON BOOKED_SLIP.DELIVERY_MODE=VAL.ID
            LEFT JOIN PAYMENT_TERM PAY ON BOOKED_SLIP.PAYMENT_TERM=PAY.ID
            LEFT JOIN VALUESET_VALUE VA1 ON BOOKED_SLIP.PAYMENT_METHOD=VA1.ID
            LEFT JOIN UOM ON BOOKED_SLIP.CURRENCY_ID=UOM.ID
            LEFT JOIN CUSTOMER CUS ON CUS.OBJECT_REF_ID = BOOKED_SLIP.INVOICE_OBJECT
            LEFT JOIN ORGANIZATION ON CUS.ID=ORGANIZATION.OBJECT_REF_ID
            LEFT JOIN OBJECT OJCTM ON BOOKED_SLIP.INVOICE_OBJECT=OJCTM.ID 
                AND OJCTM.OBJECT_REF_TABLE IN ( 'CUSTOMER','VENDOR','WAREHOUSE')
" update="BOOKED_SLIP" updatewhere=1 updatekeyinplace=yes )
