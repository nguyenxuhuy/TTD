 retrieve="select '' as gutter,
        sva.id,
        sv.trans_date,
         doc.code,
         obj_item.code item_code,
         obj_item.name item_name,
         obj_wh.code wh_code,
         obj_wh.name wh_name,
         vv_spec.code spec_code,
         vv_spec.name spec_name,
         sv.lot_no,
         sv.serial_no,
         vv_loc.code loc_code,
         vv_loc.name loc_name,
         uom.code unit_code,
         sv.qty,
         sv.base_value temp_value, 
         obj_dr.code account_dr,
         obj_cr.code account_cr,
        sva.base_value ,
         sva.cost_method,
        sva.object_ref_id,
        sva.object_ref_table,
        sva.QTY,
        sva.TRANS_VALUE,
        sva.ADJ_DATE,
        sva.CREATED_BY,
        sva.CREATED_DATE,
        sva.LAST_MDF_BY,
        sva.LAST_MDF_DATE,
        sva.stream_value_ref_id         
from stream_value_adj sva
join stream_value sv on sva.STREAM_VALUE_REF_ID = sv.id
join post_line pl on pl.OBJECT_REF_TABLE = sv.ITEM_REF_TABLE and pl.OBJECT_REF_ID = sv.ITEM_REF_ID and pl.post_type <> 'revenue'
join document doc on sv.doc_ref_id = doc.id
join object obj_item on obj_item.id = sv.item_id
join object obj_wh on obj_wh.id = sv.f_object_id
join object obj_dr on obj_dr.id = pl.dr_account_id
join object obj_cr on obj_cr.id = pl.cr_account_id
join uom on uom.id = sv.trans_cur_id
left join valueset_value vv_spec on vv_spec.id = sv.spec_id
left join  valueset_value vv_loc on vv_loc.id = sv.loc_id" )
