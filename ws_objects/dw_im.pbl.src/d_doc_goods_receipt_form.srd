 retrieve="SELECT
DOCUMENT.ID,
DOCUMENT.CODE,
DOCUMENT.REF_CODE,
DOCUMENT.HANDLED_BY, 
DOCUMENT.STATUS, 
DOCUMENT.REQ_APPR_NOTE,  
DOCUMENT.APPR_NOTE, 
DOCUMENT.DOCUMENT_DATE, 
DOCUMENT.DOC_TYPE,  
DOCUMENT.VERSION_ID, 
DOCUMENT.TRANS_HDR_ID, 
DOCUMENT.MANAGE_GROUP,
DOCUMENT.COMPANY_ID,
DOCUMENT.BRANCH_ID, 
DOCUMENT.OBJECT_REF_ID,
DOCUMENT.OBJECT_REF_TABLE, 
DOCUMENT.CREATED_BY, 
DOCUMENT.CREATED_DATE,
DOCUMENT.LAST_MDF_BY ,
DOCUMENT.LAST_MDF_DATE, 
DOCUMENT.ROSTER_YN, 
DOCUMENT.SOB,
DOCUMENT.FSOB_ID,
DOCUMENT.SOB_YN         ,
DOCUMENT.SOB_POPUP   ,
O_STAFF.CODE,
O_STAFF.NAME,
TSH.TRANS_DESC,
BO.TRANS_DATE,
BO.ID ,
BO.OBJECT_REF_ID ,
LE.CODE ,
LE.NAME  ,
BO.DESCRIPTION,
o_cust.short_name ,
bo.invoice_object,
bo.dr_cr_object,
bo.payment_term,
bo.payment_method,
bo.delivery_mode,
bo.due_date,
bo.INVOICE_RECEIVED_DATE,
bo.currency_id,
bo.exchange_rate,
bo.invoice_yn,
bo.quantity_yn,
bo.value_yn,
bo.ALLOCATION_YN,
bo.INCLUDE_TAX_YN,
bo.SUBACCOUNT,
bo.INVOICE_ADDRESS,
bo.TRANS_ADDRESS,
bo.VENDOR_DELIVERY_NO,
bo.RECEIPT_PERSON	,
bo.ATTRIBUTE,	
bo.lot_no,	
bo.serial_no,	
vv1.code delivery_mode_code,
VVLL.name status_name
FROM DOCUMENT
LEFT JOIN ROLE_USER RO ON RO.ID=DOCUMENT.HANDLED_BY 
            AND RO.TYPE='user'
LEFT JOIN OBJECT O_STAFF ON O_STAFF.ID=RO.OBJECT_STAFF 
            AND O_STAFF.OBJECT_REF_TABLE='STAFF'
JOIN BOOKED_SLIP BO ON BO.OBJECT_REF_ID=DOCUMENT.ID
LEFT JOIN TRANS_SETUP_HDR TSH ON DOCUMENT.TRANS_HDR_ID=TSH.ID
LEFT JOIN LEGAL_ENTITY LE ON LE.ID=DOCUMENT.BRANCH_ID 
LEFT JOIN VALUESET_VALUE VVLL ON VVLL.CODE=DOCUMENT.STATUS
left join OBJECT o_cust on o_cust.id = BO.DR_CR_OBJECT
LEFT JOIN VALUESET_VALUE Vv1 ON VV1.id=bo.delivery_mode
" update="DOCUMENT" updatewhere=1 updatekeyinplace=yes )
