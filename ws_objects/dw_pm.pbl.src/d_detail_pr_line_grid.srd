 column=(type=char(200) updatewhereclause=yes name=line_text dbname="pr_line.line_text" dbalias="pRl.line_text" )
 column=(type=number updatewhereclause=yes name=qty dbname="pr_line.qty" dbalias="pRl.qty" )
 column=(type=number updatewhereclause=yes name=po_qty dbname="po_qty" )
 column=(type=number updatewhereclause=yes name=inv_qty dbname="inv_qty" )
 column=(type=number updatewhereclause=yes name=pur_qty dbname="pur_qty" )
