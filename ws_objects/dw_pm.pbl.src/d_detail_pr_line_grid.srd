 retrieve="select pRl.line_no,
object.code,
pRl.line_text,
pRl.qty,
sum(nvl(vie.po_qty,0)) po_qty,
sum(nvl(vie.inv_qty,0)) inv_qty,
sum(nvl(vie.pur_qty,0)) pur_qty,
sum(nvl(vie.pR_r_qty,0)) pRr_qty,
sum(nvl(vie.inv_r_qty,0)) inv_r_qty
from document doc
left join orders ord on doc.version_id=ord.id
left join pR_line pRl on ord.id=pRl.object_ref_id
left join object on pRl.item_id = object.id
join v_doc_detail_3 vie on vie.pR_id=pRl.id
where doc.id= :an_doc_id
group by pRl.line_no,
object.code,
pRl.qty,
pRl.line_text
order by prl.line_no
" arguments=(("an_doc_id", number)) )
