 retrieve="select '' as gutter,
ip_d.id,
ip_d.OBJECT_MANAGE_GROUP,
ip_d.APPLIED_OBJECT_ID,
ip_d.DISCOUNT_PCT, 
ip_d.COMM_STAFF_PCT,
ip_d.COMM_CUST_PCT, 
o.code,  
o.name,
vv.code, 
ip_d.ITEM_APPLIED_TYPE,
ip_d.item_id,
ip_d.TRANS_UOM,
ip_d.PRICE_TYPE, 
ip_d.F_DATE, 
ip_d.T_DATE,
ip_d.CURRENCY_ID,
ip_d.OBJECT_TYPE,
ip_d.OBJECT_APPLIED_TYPE,
ip_d.company_id, 
ip_d.branch_id,
ip_d.created_by,
ip_d.created_date,
ip_d.last_mdf_by,
ip_d.last_mdf_date ,
ip_s.retail_price,
v.uom_code,
i.spec_desc,
i.manufacturers, 
o_dist.short_name,
o_vend.short_name
from item_price ip_d
left join object o on o.id= ip_d.item_id
left join valueset_value vv on vv.id = o.manage_group
left join item_price ip_s on ip_s.item_id = ip_d.item_id and ip_s.PRICE_TYPE = 'SALES' and sysdate between ip_s.f_date and coalesce(ip_s.t_date, sysdate)
 join V_MAX_UOM_ITEM v on v.item_id = ip_s.item_id and v.uom_id= ip_s.trans_uom
left join item i on i.object_ref_id =  ip_d.item_id
left join object o_dist on o_dist.id= ip_d.OBJECT_MANAGE_GROUP
left join object o_vend on o_vend.id= ip_d.APPLIED_OBJECT_ID
where 
ip_d.OBJECT_TYPE = 'VENDOR'
and ip_d.PRICE_TYPE = 'PURCHASE'" update="item_price" updatewhere=1 updatekeyinplace=yes )
