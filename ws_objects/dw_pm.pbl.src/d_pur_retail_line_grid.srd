pil.line_no,
pil.object_id, 
pil.line_text,
pil.spec_id,
pil.qty,  
pil.trans_uom, 
pil.price,
pil.amount, 
pil.disc_amount,
pil.disc_pct,
pil.base_amount,
pil.foc_yn,
pil.note,
pil.auto_alloc_yn,
pil.start_alloc_date,
pil.alloc_amount,
pil.nbr_of_alloc_period,
pil.alloc_object,
pil.warehouse_id,
pil.BASE_AMOUNT_EX_TAX,
pil.AMOUNT_EX_TAX,
pil.PRICE_EX_TAX,
pil.subaccount,
pil.loc_id,
pil.QTY_SKU,
pil.vat,
pil.vat_amount,
objl.code,
objl.name,
objl.object_ref_table,
objl.short_name obj_short_name,
uml.code, 
whpur.code,
whpur.name,
subpur.short_name vend_short_name
FROM PUR_INVOICE_LINE pil 
LEFT JOIN OBJECT objl ON pil.object_id = objl.id
LEFT JOIN uom uml ON pil.trans_uom = uml.id
LEFT JOIN OBJECT whpur ON pil.warehouse_id = whpur.id 
    AND whpur.OBJECT_REF_TABLE = 'WAREHOUSE'
LEFT JOIN OBJECT subpur ON pil.subaccount = subpur.id" update="PUR_INVOICE_LINE" updatewhere=1 updatekeyinplace=yes  sort="vend_short_name A " )
