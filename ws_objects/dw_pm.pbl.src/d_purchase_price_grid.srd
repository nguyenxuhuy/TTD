 retrieve="select  '' as gutter,
          item_price.ID                   ,
          item_price.item_manage_group,
          vv1.code,
          vv1.name,
          item_price.ITEM_ID              ,
          obj1.code,
          obj1.name,
          item_price.ITEM_APPLIED_TYPE    ,
          item_price.COMPANY_ID           , 
          item_price.BRANCH_ID            , 
          item_price.OBJECT_APPLIED_TYPE , 
          item_price.object_manage_group,       
          vv2.code,
          vv2.name, 
          item_price.APPLIED_OBJECT_ID    ,
          obj2.code, 
          obj2.name,
          item_price.OBJECT_TYPE          ,
          item_price.RETAIL_PRICE         ,
          item_price.WHOLESALES_PRICE     ,
          item_price.FROM_QTY             ,
          item_price.TO_QTY               ,
          item_price.F_DATE               ,
          item_price.T_DATE               ,
          item_price.CURRENCY_ID,
          item_price.SPEC_ID,
          item_spec.code,
          item_spec.name,
          uom.code,   
          item_price.price_type,                 
          item_price.CREATED_BY           ,
          item_price.CREATED_DATE         ,
          item_price.LAST_MDF_BY          ,
          item_price.LAST_MDF_DATE,
           vie.APPLIED_OBJECT_ID                              
from item_price          
 join object obj1 on obj1.id = item_price.item_id and obj1.object_ref_table = 'ITEM'
 left join uom on uom.id = item_price.currency_id
 left join object obj2 on obj2.id = item_price.applied_object_id
 left join item_spec on item_spec.id = item_price.spec_id
 left join  VALUESET_VALUE vv1 on vv1.id = item_price.item_manage_group
 left join  VALUESET_VALUE vv2 on vv2.id = item_price.object_manage_group
 left join v_vendor_purchase_price vie  on vie.id=item_price.id" update="item_price" updatewhere=1 updatekeyinplace=yes )
