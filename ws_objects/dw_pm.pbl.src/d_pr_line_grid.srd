 retrieve="SELECT '' AS gutter,
       PR_LINE.AMOUNT,
       PR_LINE.BRANCH_ID,
       PR_LINE.COMPANY_ID,
       PR_LINE.CREATED_BY,
       PR_LINE.CREATED_DATE,
       PR_LINE.ID, 
       PR_LINE.ITEM_ID,
       PR_LINE.LAST_MDF_BY,
       PR_LINE.LAST_MDF_DATE,
       PR_LINE.LINE_TEXT,
       PR_LINE.NOTE,
       PR_LINE.OBJECT_REF_ID,
       PR_LINE.OBJECT_REF_TABLE,
       PR_LINE.PRICE, 
       PR_LINE.QTY, 
       PR_LINE.SPEC_ID,
       PR_LINE.TRANS_UOM,
       PR_LINE.VENDOR_ID, 
       PR_LINE.LINE_NO,
       O_ITEM.CODE,
       O_ITEM.NAME,
       ITEM_SPEC.CODE, 
       ITEM_SPEC.NAME, 
		ITEM_SPEC.LENGTH,
		ITEM_SPEC.WIDTH,
		ITEM_SPEC.DEPTH,
       UOM.CODE,
       O_ITEM.OBJECT_REF_TABLE,
       PR_LINE.REQUEST_RECEIVE_DATE,
       PR_LINE.CONFIRM_RECEIVE_DATE,
	m.t_ref_table,
	coalesce(v1.VEND_NAME,v.VENDOR_NAME_LIST) VEND_NAME,
	coalesce(v1.DIST_NAME,v.DISTRIBUTE_NAME_LIST) DIST_NAME,
	coalesce(v1.OBJECT_ID_LIST,v.DISTRIBUTE_ID_LIST) DIST_ID_list,
	coalesce(v1.BILL_TO_OBJECT_LIST,v.VENDOR_ID_LIST) VEND_ID_list,
	i.manufacturers,
	v1.price,
	v1.price * PR_LINE.QTY best_amount,
	v1.price * PR_LINE.QTY best_base_amount,
	v1.price * PR_LINE.QTY best_base_amount_ex_tax,
	vv.code
  FROM PR_LINE
       LEFT JOIN OBJECT O_ITEM
          ON O_ITEM.ID = PR_LINE.ITEM_ID 
              AND O_ITEM.OBJECT_REF_TABLE = 'ITEM'
       LEFT JOIN ITEM_SPEC
          ON ITEM_SPEC.ID = PR_LINE.SPEC_ID
       LEFT JOIN UOM
          ON PR_LINE.TRANS_UOM = UOM.ID
	LEFT JOIN matching m on m.f_ref_id = pr_line.id
	left join V_ITEM_VENDOR_DISC v on v.item_id = PR_LINE.ITEM_ID and v.company_id = PR_LINE.company_id
	left join V_BEST_PRICE_VENDOR_6M v1 on v1.item_id = PR_LINE.ITEM_ID and v1.trans_uom = PR_LINE.trans_uom and v1.company_id = PR_LINE.company_id
	left join item i on i.object_ref_id = PR_LINE.ITEM_ID
left join valueset_value vv on vv.id=O_ITEM.manage_group" update="PR_LINE" updatewhere=1 updatekeyinplace=yes )
