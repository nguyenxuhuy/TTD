 retrieve="SELECT DOCUMENT.ID,
       DOCUMENT.BRANCH_ID,
       DOCUMENT.CODE,
       DOCUMENT.DOCUMENT_DATE,
       ORGANIZATION.TAX_NUMBER,
       OBJECT.CODE BILL_OBJECT_CODE,
       OBJECT.NAME BILL_OBJECT_NAME,
       LEGAL_ENTITY.CODE,
       LEGAL_ENTITY.NAME COMPANY_NAME, 
       BUSINESS_ADDRESS.ADDRESS COMPANY_ADDRESS,
       BUSINESS_ADDRESS.TEL COMPANY_TEL,
       BUSINESS_ADDRESS.FAX COMPANY_FAX,
       V_STAFF.STAFF_NAME,   
       V_STAFF.DEPT_NAME, 
       V1.NAME DELIVERY_MODE_NAME, 
       V2.NAME PAYMENT_METHOD_NAME,
       ORDERS.REQUEST_RECEIVE_DATE, 
       vba.address BILL_TO_ADDTEXT,  
       PEOPLE.PHONE VENDOR_PHONE,
       PEOPLE.LAST_NAME || ' ' || PEOPLE.FIRST_NAME VEN_CONTACT_NAME,
       PEOPLE.EMAIL VENDOR_MAIL,
       PO_LINE.LINE_TEXT,
       PO_LINE.QTY, 
       PO_LINE.AMOUNT,
       PO_LINE.PRICE,
       PO_LINE.AMOUNT_EX_TAX,
       PO_LINE.NOTE,
       UOM.CODE,
       UOM.NAME, 
       VTX.TAX_CORRECTION, 
       V_STAFF.PHONE STAFF_PHONE,
       V3.NAME PAYMENT_TERM_NAME,
       V3.NDAY,
       OITEM.CODE,
       OITEM.NAME,
       VAM.STAFF_NAME,
        Case when DOCUMENT.BRANCH_ID = 24088840
                then 'Pics\header_vi_kni.png'
                else 'Pics\ASV-header-vi.png'
            end hdr
  FROM DOCUMENT
       JOIN ORDERS
          ON ORDERS.ID = DOCUMENT.VERSION_ID
       JOIN OBJECT
          ON OBJECT.ID = ORDERS.BILL_TO_OBJECT
             AND OBJECT.OBJECT_REF_TABLE IN ('VENDOR', 'CUSTOMER')
        left join vendor vdr on vdr.object_ref_id = ORDERS.BILL_TO_OBJECT
        left join BUSINESS_ADDRESS vba on vba.object_ref_id = vdr.id and regexp_like(vba.purpose,'all|invoice')
       LEFT JOIN LEGAL_ENTITY
          ON LEGAL_ENTITY.ID = DOCUMENT.BRANCH_ID
       LEFT JOIN BUSINESS_ADDRESS
          ON BUSINESS_ADDRESS.OBJECT_REF_ID = LEGAL_ENTITY.ID
       JOIN ROLE_USER ROLDOC
          ON ROLDOC.ID = DOCUMENT.HANDLED_BY
       JOIN OBJECT OBJ_RODOC
          ON OBJ_RODOC.ID = ROLDOC.OBJECT_STAFF
             AND OBJ_RODOC.OBJECT_REF_TABLE = 'STAFF'
       LEFT JOIN V_STAFF
          ON V_STAFF.OBJECT_REF_ID = OBJ_RODOC.ID
       LEFT JOIN VALUESET_VALUE V1
          ON V1.ID = ORDERS.DELIVERY_MODE
       LEFT JOIN VALUESET_VALUE V2
          ON V2.ID = ORDERS.PAYMENT_METHOD
       LEFT JOIN PEOPLE
          ON PEOPLE.ID = ORDERS.CONTACT_ID
       LEFT JOIN ORGANIZATION
          ON ORGANIZATION.OBJECT_REF_ID = LEGAL_ENTITY.ID
       LEFT JOIN PO_LINE
          ON PO_LINE.OBJECT_REF_ID = ORDERS.ID
       LEFT JOIN OBJECT OITEM
          ON OITEM.ID = PO_LINE.ITEM_ID 
            AND OITEM.OBJECT_REF_TABLE = 'ITEM'
       LEFT JOIN UOM
          ON UOM.ID = PO_LINE.TRANS_UOM
       LEFT JOIN V_SUM_TAX_LINE VTX
          ON VTX.LINE_ID = PO_LINE.ID
       LEFT JOIN PAYMENT_TERM V3
          ON V3.ID = ORDERS.PAYMENT_TERM
       LEFT JOIN V_APPR_PROFILE_MAX VAM
          ON VAM.OBJECT_REF_ID = DOCUMENT.ID" )
