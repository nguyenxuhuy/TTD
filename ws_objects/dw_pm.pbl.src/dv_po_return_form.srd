 retrieve="select  DOCUMENT.CODE as PO_Number,
         ORDERS.REQUEST_RECEIVE_DATE as DELIVERY_DATE,
         DOCUMENT.DOCUMENT_DATE ,
         ORDERS.INVOICE_REQUEST,
         VAL1.CODE as METHOD_CODE,
         VAL1.NAME as METHOD_DESC,
         PAYMENT_TERM.NAME as payment_term,
         CUR.CODE as CUR_CODE,
         ORDERS.EXCHANGE_RATE,
         OBJVD.CODE as VEND_CODE, 
         OBJVD.NAME as VEND_NAME, 
          PEOCT.LAST_NAME||' '||PEOCT.FIRST_NAME as people_contact, 
         PEOCT.PHONE, 
         ORDERS.SHIP_TO_ADDTEXT, 
         OBJIT.NAME as ITEM_DESC, 
         UNIT.NAME as DESCRIPTION,  
         PO_LINE.QTY * -1 as order_qty,
         PO_LINE.PRICE,
         PO_LINE.AMOUNT * -1 as amount, 
         decode(nvl(ORDERS.INCLUDE_TAX_YN,'N'),'Y', PO_LINE.AMOUNT , round(PO_LINE.AMOUNT * (100 + nvl(TAX_LINE.TAX_PCT,0) ) /100)) * -1 as total_amount ,
         PO_LINE.NOTE,
         PO_LINE.object_ref_id, 
         OBJST.NAME as STAFF_NAME,
         OBJDP.NAME AS DEPT_NAME,         
         TAX_LINE.TAX_PCT,          
         OBJAPPR.NAME as approved_name,
         DOCUMENT.id
from DOCUMENT left join ORDERS on ORDERS.ID = DOCUMENT.VERSION_ID
                      left  join VALUESET_VALUE VAL1 on VAL1.ID = ORDERS.PAYMENT_METHOD
                      left  join PAYMENT_TERM on PAYMENT_TERM.ID = ORDERS.PAYMENT_TERM
                      left  join UOM CUR on CUR.ID = ORDERS.CURRENCY_ID 
                      left  join OBJECT OBJVD on OBJVD.ID = ORDERS.OBJECT_ID
                      left join VENDOR on VENDOR.OBJECT_REF_ID = OBJVD.ID
                      left join PO_LINE on PO_LINE.OBJECT_REF_ID = ORDERS.ID
                      left join OBJECT OBJIT on OBJIT.ID = PO_LINE.ITEM_ID
                       left join UOM UNIT on UNIT.ID = PO_LINE.TRANS_UOM
                       left join role_user ru1 on ru1.id = DOCUMENT.HANDLED_BY
                       left join OBJECT OBJST on OBJST.id = ru1.OBJECT_staff
                       left join STAFF on STAFF.OBJECT_REF_ID = OBJST.id
                       left join OBJECT OBJDP on OBJDP.ID = STAFF.DEPT_ID
                       left join PEOPLE PEOCT on PEOCT.ID = ORDERS.CONTACT_ID
                       left join TAX_LINE on TAX_LINE.OBJECT_REF_ID = PO_LINE.ID                                                   
                       left join V_APPR_PROFILE vapr on DOCUMENT.ID = vapr.OBJECT_REF_ID   
                       left join ROLE_USER on vapr.APPR_BY = ROLE_USER.ID         
                       left join OBJECT OBJAPPR on OBJAPPR.id = ROLE_USER.OBJECT_STAFF  
where document.version_id = orders.id" )
