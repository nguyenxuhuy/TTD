 retrieve="SELECT            
PO_LINE.ID,
PO_LINE.BRANCH_ID,
PO_LINE.COMPANY_ID,
PO_LINE.CREATED_BY,
PO_LINE.CREATED_DATE,
PO_LINE.DISC_AMT,
PO_LINE.DISC_PCT,
PO_LINE.FOC_YN,
PO_LINE.ITEM_ID, 
PO_LINE.LAST_MDF_BY,
PO_LINE.LAST_MDF_DATE, 
PO_LINE.LINE_TEXT,
PO_LINE.NOTE, 
PO_LINE.OBJECT_REF_ID,  
PO_LINE.OBJECT_REF_TABLE, 
PO_LINE.PRICE,   
PO_LINE.QTY,
PO_LINE.AMOUNT,
PO_LINE.BASE_AMOUNT,
PO_LINE.AMOUNT_EX_TAX, 
PO_LINE.PRICE_EX_TAX,
PO_LINE.BASE_AMOUNT_EX_TAX,
PO_LINE.SPEC_ID,
PO_LINE.TRANS_UOM,
PO_LINE.LINE_NO,
PO_LINE.QTY_SKU, 
PO_LINE.REQUEST_RECEIVE_DATE,
PO_LINE.CONFIRM_RECEIVE_DATE,
PO_LINE.WAREHOUSE_ID, 
PO_LINE.CLOSE_YN ,
i_obj.code,
i_obj.name,
item_spec.code,
item_spec.name,
item_spec.length,
item_spec.width,
item_spec.depth,
dtv.code,
i_obj.object_ref_table,
w_obj.code,
w_obj.name,
V_ON_HAND.close_qty,
item.MANUFACTURERS,
PO_LINE.base_price
from PO_LINE  
left join object i_obj on i_obj.id= PO_LINE.ITEM_ID
     AND i_obj.OBJECT_REF_TABLE = 'ITEM'
left join item_spec on item_spec.id = PO_LINE.SPEC_ID
left join uom dtv on dtv.id=PO_LINE.TRANS_UOM
left join object w_obj on w_obj.id = PO_LINE.WAREHOUSE_ID
LEFT JOIN item ON item.object_ref_id = PO_LINE.ITEM_ID
LEFT JOIN V_ON_HAND ON V_ON_HAND.ID = PO_LINE.ITEM_ID 
        AND w_obj.CODE = NVL(V_ON_HAND.WAREHOUSE_CODE,'@')" update="PO_LINE" updatewhere=1 updatekeyinplace=yes )
