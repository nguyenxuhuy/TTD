 retrieve="SELECT
DOCUMENT.ID,
DOCUMENT.CODE,
DOCUMENT.REF_CODE,
DOCUMENT.HANDLED_BY, 
DOCUMENT.STATUS, 
DOCUMENT.REQ_APPR_NOTE, 
DOCUMENT.APPR_NOTE, 
DOCUMENT.DOCUMENT_DATE, 
DOCUMENT.DOC_TYPE,  
DOCUMENT.VERSION_ID,  
DOCUMENT.TRANS_HDR_ID, 
DOCUMENT.MANAGE_GROUP, 
DOCUMENT.COMPANY_ID,
DOCUMENT.BRANCH_ID,
DOCUMENT.OBJECT_REF_ID,
DOCUMENT.OBJECT_REF_TABLE,  
DOCUMENT.CREATED_BY, 
DOCUMENT.CREATED_DATE,
DOCUMENT.LAST_MDF_BY ,
DOCUMENT.LAST_MDF_DATE,
DOCUMENT.ROSTER_YN,
DOCUMENT.SOB,
DOCUMENT.FSOB_ID,
DOCUMENT.SOB_YN         ,
DOCUMENT.SOB_POPUP        ,
BO.TRANS_DATE,
BO.ID ,
BO.OBJECT_REF_ID ,
vv.code delivery_mode_code,
VVLL.name status_name,
OBJ.short_name vend_shortname,
bo.delivery_mode,
BO.INVOICE_OBJECT,
le.name
FROM DOCUMENT 
JOIN BOOKED_SLIP BO ON BO.OBJECT_REF_ID=DOCUMENT.ID
LEFT JOIN OBJECT OBJ ON OBJ.ID=BO.INVOICE_OBJECT
left join valueset_value vv on vv.id = bo.delivery_mode
left join valueset_value VVLL on VVLL.code = DOCUMENT.status
left join legal_entity le on le.id= DOCUMENT.branch_id" update="DOCUMENT" updatewhere=1 updatekeyinplace=yes )
