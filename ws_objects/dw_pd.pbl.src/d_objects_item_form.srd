 retrieve="SELECT '' as gutter,
object.id,
object.code, 
object.name,   
object.short_name,     
object.print_name,    
object.manage_group,
object.posting_group,
object.handled_by,
object.active_yn,  
object.location,
object.company_id, 
object.branch_id,
object.object_ref_id, 
object.object_ref_table,
object.object_ref_name,
object.created_by,
object.created_date, 
object.last_mdf_by,
object.last_mdf_date,
object.note,
VA1X.CODE,  
VA1X.NAME, 
VA2X.CODE,
VA2X.NAME,
ATTACHMENT.LINE_NO,
i.stock_uom,
u.code,
i.round_id,
r.code,
i.value_yn,
i.quantity_yn,
i.lot_yn,
i.manufacturers,
i.min_stock,
i.max_stock,
i.ingredient,
i.spec_desc, 
i.origin,
i.dosage_form, 
ibc.barcode,
l.code,
i.abc_rank,
i.id,
i.bom_id,
i.size_id,
ibc.id,
fd.code,
o_bom.code,
dwh_search.obj_search
FROM OBJECT 
Left join item i on i.object_ref_id = object.id
left join item_barcode ibc on ibc.OBJECT_REF_ID = i.id and ibc.BARCODE_TYPE='vendor'
left join rounding r on r.id= i.round_id
left join flexible_data fd on fd.id = i.size_id and fd.object_ref_table = 'MENU'
left join uom u on u.id= i.stock_uom
left join location l on l.id= object.location
left join object o_bom on o_bom.id = i.bom_id and o_bom.object_ref_table = 'BOM'
LEFT JOIN VALUESET_VALUE VA1X ON OBJECT.MANAGE_GROUP=VA1X.ID 
LEFT JOIN VALUESET_VALUE VA2X ON OBJECT.POSTING_GROUP=VA2X.ID
LEFT JOIN ATTACHMENT ON ATTACHMENT.DWOBJECT='d_objects_item_grid'
                                     AND ATTACHMENT.OBJECT_REF_ID=OBJECT.ID
                                     AND ATTACHMENT.BRANCH_ID=OBJECT.BRANCH_ID
                                     AND ATTACHMENT.LINE_NO=1
left join dwh_search  on dwh_search.object_id = object.id" update="OBJECT" updatewhere=1 updatekeyinplace=yes )
